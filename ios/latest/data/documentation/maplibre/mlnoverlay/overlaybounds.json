ds"]}],"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","sections":[],"metadata":{"fragments":[{"text":"overlayBounds","kind":"identifier"}],"role":"symbol","title":"overlayBounds","roleHeading":"Instance Property","modules":[{"name":"MapLibre"}],"symbolKind":"property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","navigatorTitle":[{"kind":"identifier","text":"overlayBounds"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"role":"symbol","title":"MLNCoordinateBounds","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","kind":"symbol","url":"\/documentation\/maplibre\/mlncoordinatebounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","fragments":[{"kind":"identifier","text":"overlayBounds"}],"role":"symbol","kind":"symbol","title":"overlayBounds","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"title":"MLNOverlay","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","role":"symbol","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnoverlay","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","title":"MapLibre","url":"\/documentation\/maplibre","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"}}}