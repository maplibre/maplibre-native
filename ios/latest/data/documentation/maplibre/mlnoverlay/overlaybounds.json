igatorTitle":[{"kind":"identifier","text":"overlayBounds"}],"fragments":[{"text":"overlayBounds","kind":"identifier"}],"role":"symbol","roleHeading":"Instance Property","title":"overlayBounds","symbolKind":"property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds"},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnoverlay\/overlaybounds"]}],"sections":[],"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"url":"\/documentation\/maplibre\/mlncoordinatebounds","title":"MLNCoordinateBounds","type":"topic","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"kind":"symbol","role":"symbol","fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"role":"symbol","title":"overlayBounds","abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","fragments":[{"text":"overlayBounds","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","type":"topic","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"type":"text","te