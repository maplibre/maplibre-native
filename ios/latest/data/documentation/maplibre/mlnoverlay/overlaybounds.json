verlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"kind":"symbol","type":"topic","title":"MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"kind":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","type":"topic","title":"overlayBounds","role":"symbol"}}}