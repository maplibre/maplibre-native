ject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"},"schemaVersion":{"minor":3,"patch":0,"major":0},"sections":[],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnoverlay\/overlaybounds"],"traits":[{"interfaceLanguage":"occ"}]}],"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"type":"topic","fragments":[{"text":"overlayBounds","kind":"identifier"}],"title":"overlayBounds","role":"symbol","abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlncoordinatebounds","type":"topic","role":"symbol","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"title":"MLNCoordinateBounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"url":"\/documentation\/maplibre\/mlnoverlay","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"text":" ","type":"text"},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","role":"symbol","title":"MLNOverlay","type":"topic","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"kind":"symbol"}}}