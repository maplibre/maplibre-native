e":"paragraph"}]}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnoverlay\/overlaybounds"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","metadata":{"fragments":[{"kind":"identifier","text":"overlayBounds"}],"navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"title":"overlayBounds","modules":[{"name":"MapLibre"}],"symbolKind":"property","roleHeading":"Instance Property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","role":"symbol"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"type":"topic","title":"overlayBounds","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","fragments":[{"text":"overlayBounds","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"url":"\/documentation\/maplibre\/mlncoordinatebounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"type":"topic","title":"MLNCoordinateBounds","role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","title":"MLNOverlay","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that re