":"symbol","sections":[],"metadata":{"fragments":[{"kind":"identifier","text":"overlayBounds"}],"roleHeading":"Instance Property","role":"symbol","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","symbolKind":"property","navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"overlayBounds"},"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"type":"topic","title":"MLNCoordinateBounds","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","url":"\/documentation\/maplibre\/mlncoordinatebounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","type":"topic","title":"MLNOverlay","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"tit