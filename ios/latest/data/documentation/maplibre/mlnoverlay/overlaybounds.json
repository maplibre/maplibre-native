lays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"title":"overlayBounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","type":"topic","role":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","kind":"symbol"}}}