":"symbol","schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","interfaceLanguage":"occ"},"metadata":{"modules":[{"name":"MapLibre"}],"title":"overlayBounds","symbolKind":"property","role":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"roleHeading":"Instance Property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"kind":"symbol","type":"topic","role":"symbol","fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","url":"\/documentation\/maplibre\/mlncoordinatebounds","title":"MLNCoordinateBounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","type":"topic","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnoverlay","title":"MLNOverlay","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"type":"text","text":" "},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"kind":"symbol","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","title":"overlayBounds","role":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds"}}}