ibre\/MLNOverlay"]]},"metadata":{"navigatorTitle":[{"kind":"identifier","text":"overlayBounds"}],"externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","roleHeading":"Instance Property","symbolKind":"property","fragments":[{"kind":"identifier","text":"overlayBounds"}],"modules":[{"name":"MapLibre"}],"role":"symbol","title":"overlayBounds"},"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"type":"topic","role":"symbol","title":"overlayBounds","fragments":[{"text":"overlayBounds","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","url":"\/documentation\/maplibre\/mlncoordinatebounds","type":"topic","kind":"symbol","fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"title":"MLNCoordinateBounds","role":"symbol","abstract":[{"text":"A rectangular area as measured on a two-dimensional map projection.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","type":"topic","title":"MapLibre","role":"collection","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol","title":"MLNOverlay