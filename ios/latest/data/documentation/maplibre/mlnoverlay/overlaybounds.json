","roleHeading":"Instance Property","fragments":[{"text":"overlayBounds","kind":"identifier"}],"externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","symbolKind":"property","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}]},"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnoverlay\/overlaybounds"]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlncoordinatebounds","abstract":[{"text":"A rectangular area as measured on a two-dimensional map projection.","type":"text"}],"fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"title":"MLNCoordinateBounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","kind":"symbol","role":"symbol","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"title":"overlayBounds","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","fragments":[{"text":"overlayBounds","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","type":"topic","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","kind":"symbol","role":"symbol","title":"MLNOverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference","isActive":true},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"text":" ","type":"text"},{"text":"take the form of a polyline or polygon.","type":"text"}],"url":"\/documentation\/maplibre\/mlnoverlay"}}}