ions":[],"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"navigatorTitle":[{"kind":"identifier","text":"overlayBounds"}],"roleHeading":"Instance Property","symbolKind":"property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","fragments":[{"kind":"identifier","text":"overlayBounds"}],"role":"symbol","title":"overlayBounds","modules":[{"name":"MapLibre"}]},"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"kind":"symbol","title":"overlayBounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","type":"topic","fragments":[{"kind":"identifier","text":"overlayBounds"}],"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","title":"MLNCoordinateBounds","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlncoordinatebounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","kind":"symbol","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","title":"MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","role":"symbol","kind":"symbol","type":"topic","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"text":" ","type":"text"},{"text":"take the form of a polyline or polygon.","type":"text"}]}}}