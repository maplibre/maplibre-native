content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","interfaceLanguage":"occ"},"sections":[],"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","title":"overlayBounds","role":"symbol","fragments":[{"kind":"identifier","text":"overlayBounds"}],"symbolKind":"property","roleHeading":"Instance Property","navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"modules":[{"name":"MapLibre"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","title":"MLNCoordinateBounds","role":"symbol","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"type":"topic","url":"\/documentation\/maplibre\/mlncoordinatebounds","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","kind":"symbol","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","role":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"title":"overlayBounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"kind":"symbol","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","title":"MLNOverlay"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","role":"collection","type":"topic","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"}}}