ole":"symbol","title":"MLNOverlay","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"text":"take the form of a polyline or polygon.","type":"text"}],"url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol","fragments":[{"text":"MLNOverlay","kind":"identifier"}]}}}