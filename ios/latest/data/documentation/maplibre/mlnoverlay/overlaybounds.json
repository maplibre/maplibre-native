g the"},{"text":" ","type":"text"},{"text":"Pacific Ocean from Tokyo to San Francisco might have a bounds extending","type":"text"},{"type":"text","text":" "},{"text":"from (35.68476, −220.24257) to (37.78428, −122.41310).","type":"text"}]}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"]]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","role":"collection","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"type":"topic","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"title":"overlayBounds","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","fragments":[{"kind":"identifier","text":"overlayBounds"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"type":"topic","title":"MLNOverlay","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"text":" ","type":"text"},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}],"url":"\/documentation\/maplibre\/mlnoverlay","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","role":"symbol","title":"MLNCoordinateBounds","abstract":[{"text":"A rectangular area as measured on a two-dimensional map projection.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlncoordinatebounds","fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"type":"topic"}}}