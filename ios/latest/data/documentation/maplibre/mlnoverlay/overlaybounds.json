eContent":[{"type":"text","text":"If this overlay spans the antimeridian, its bounds may extend west of −180 degrees"},{"type":"text","text":" "},{"type":"text","text":"longitude or east of 180 degrees longitude. For example, an overlay covering the"},{"type":"text","text":" "},{"type":"text","text":"Pacific Ocean from Tokyo to San Francisco might have a bounds extending"},{"text":" ","type":"text"},{"text":"from (35.68476, −220.24257) to (37.78428, −122.41310).","type":"text"}]}],"kind":"content"}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"title":"MLNCoordinateBounds","fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"url":"\/documentation\/maplibre\/mlncoordinatebounds","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"kind":"symbol","title":"overlayBounds","fragments":[{"text":"overlayBounds","kind":"identifier"}],"type":"topic","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","title":"MLNOverlay","type":"topic","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol"}}}