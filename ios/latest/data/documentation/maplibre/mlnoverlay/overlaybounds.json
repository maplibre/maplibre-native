umentation\/MapLibre\/MLNOverlay"]]},"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"metadata":{"title":"overlayBounds","modules":[{"name":"MapLibre"}],"externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","symbolKind":"property","roleHeading":"Instance Property","role":"symbol","navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"fragments":[{"kind":"identifier","text":"overlayBounds"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","type":"topic","title":"overlayBounds","fragments":[{"kind":"identifier","text":"overlayBounds"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","url":"\/documentation\/maplibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"type":"topic","url":"\/documentation\/maplibre\/mlnoverlay","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","kind":"symbol","title":"MLNOverlay","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","ty