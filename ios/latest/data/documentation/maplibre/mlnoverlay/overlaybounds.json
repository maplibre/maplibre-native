faceLanguage":"occ"},"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"metadata":{"roleHeading":"Instance Property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","title":"overlayBounds","role":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"symbolKind":"property","navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"modules":[{"name":"MapLibre"}]},"schemaVersion":{"major":0,"patch":0,"minor":3},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"type":"topic","role":"symbol","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"title":"MLNOverlay","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","role":"symbol","url":"\/documentation\/maplibre\/mlncoordinatebounds","type":"topic","fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}],"title":"MLNCoordinateBounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"url