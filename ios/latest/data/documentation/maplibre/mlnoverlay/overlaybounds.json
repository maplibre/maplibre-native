gments":[{"kind":"identifier","text":"MLNOverlay"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}],"title":"MLNOverlay"}}}