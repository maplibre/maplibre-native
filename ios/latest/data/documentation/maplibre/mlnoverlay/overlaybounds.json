ation\/MapLibre\/MLNOverlay\/overlayBounds","abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","title":"overlayBounds","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"title":"MLNCoordinateBounds","url":"\/documentation\/maplibre\/mlncoordinatebounds","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"role":"symbol","type":"topic","kind":"symbol","fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds"}}}