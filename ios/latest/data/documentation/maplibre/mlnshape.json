identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"fragments":[{"kind":"identifier","text":"MLNShape"}],"url":"\/documentation\/maplibre\/mlnshape","type":"topic","title":"MLNShape"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"reference","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"text":" ","type":"text"},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference","isActive":true},{"text":" object, while the surrounding","type":"text"},{"text":" ","type":"text"},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmultipolygon","kind":"symbol","role":"symbol","type":"topic","title":"MLNMultiPolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","url":"\/documentation\/maplibre\/mlnpointannotation","type":"topic","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"kind":"symbol","title":"MLNPointAnnotation","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"role":"symbol","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" or "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with an "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"type":"codeVoice","code":"shape"},{"type":"text","text":" or "},{"type":"codeVoice","code":"URL"},{"type":"text","text":" property."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"topic","kind":"symbol","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"title":"MLNShapeSource","url":"\/documentation\/maplibre\/mlnshapesource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"title":"MLNPointCollection","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnpointcollection","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}]}}}