agments":[{"kind":"identifier","text":"MLNShapeCollection"}],"abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"type":"text","text":" "},{"type":"text","text":"distinct but related shapes that are instances of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":". The constituent"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","url":"\/documentation\/maplibre\/mlnshapecollection","title":"MLNShapeCollection","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"fragments":[{"text":"MLNPolylineFeature","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnpolylinefeature","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","title":"MLNPolylineFeature","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"title":"MLNMultiPoint","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmultipoint"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"url":"\/documentation\/maplibre\/mlnannotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"kind":"symbol","type":"topic","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","isActive":true},{"text":" object associates a point collection with an","type":"text"},{"text":