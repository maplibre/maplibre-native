title":{"abstract":[{"type":"text","text":"The subtitle of the shape annotation. The default value of this property is"},{"text":" ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":".","type":"text"}],"kind":"symbol","fragments":[{"kind":"identifier","text":"subtitle"}],"role":"symbol","title":"subtitle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle","url":"\/documentation\/maplibre\/mlnshape\/subtitle","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"type":"text","text":" "},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"type":"text","text":" object."}],"title":"MLNMultiPolyline","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmultipolyline","kind":"symbol"},"doc://org.swift.MyProject/objc(cs)NSObject":{"identifier":"doc:\/\/org.swift.MyProject\/objc(cs)NSObject","title":"NSObject","type":"unresolvable"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/title":{"title":"title","url":"\/documentation\/maplibre\/mlnshape\/title","kind":"symbol","role":"symbol","fragments":[{"text":"title","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title","abstract":[{"type":"text","text":"The title of the shape annotation."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","title":"MLNAnnotation","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"title":"MLNShape","kind":"symbol","