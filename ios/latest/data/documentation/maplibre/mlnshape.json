vide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"type":"topic","title":"MLNPointCollectionFeature","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","url":"\/documentation\/maplibre\/mlnpointcollectionfeature","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}],"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","isActive":true,"type":"reference"},{"text":" object associates a point collection with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"url":"\/documentation\/maplibre\/mlnmultipoint","kind":"symbol","title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"title":"MLNEmptyFeature","kind":"symbol","role":"symbol","abstract":[{"text":"An "