},{"type":"text","text":" and set the"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer\/predicate","type":"reference","isActive":true},{"type":"text","text":" property to that predicate, or set a layout or"},{"type":"text","text":" "},{"type":"text","text":"paint attribute to a similarly formatted "},{"type":"codeVoice","code":"NSExpression"},{"type":"text","text":"."}],"type":"paragraph"}],"kind":"content"}],"metadata":{"symbolKind":"class","roleHeading":"Class","navigatorTitle":[{"text":"MLNShape","kind":"identifier"}],"externalID":"c:objc(cs)MLNShape","title":"MLNShape","modules":[{"name":"MapLibre"}],"role":"symbol","fragments":[{"text":"MLNShape","kind":"identifier"}]},"sections":[],"topicSections":[{"title":"Instance Properties","generated":true,"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title"],"anchor":"Instance-Properties"},{"title":"Instance Methods","generated":true,"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/geoJSONDataUsingEncoding:"],"anchor":"Instance-Methods"},{"title":"Type Methods","generated":true,"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/shapeWithData:encoding:error:"],"anchor":"Type-Methods"}],"relationshipsSections":[{"title":"Inherits From","kind":"relationships","type":"inheritsFrom","identifiers":["doc:\/\/org.swift.MyProject\/objc(cs)NSObject"]},{"title":"Inherited By","kind":"relationships","type":"inheritedBy","identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","doc:\/\