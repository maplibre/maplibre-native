cumentation/MapLibre/MLNMultiPoint":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","title":"MLNMultiPoint","type":"topic","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"title":"MLNPointCollectionFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"type":"topic","url":"\/documentation\/maplibre\/mlnpointcollectionfeature","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"title":"MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","title":"MLNShapeCollection","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollection"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of zero or more"},{"type":"text","text":" "},{"type":"text","text":"distinct but related shapes that are instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":". The constituent"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}],"url":"\/documentation\/maplibre\/mlnshapecollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/geoJSONDataUsingEncoding:":{"type":"topic","abstract":[{"type":"text","text":"Returns the GeoJSON string representation of the shape encapsulated in a data"},{"type":"text","text":" "},{"type":"text","text":"object."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/geoJSONDataUsingEncoding:","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"geoJSONDataUsingEncoding:"}],"role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"geoJSONDataUsingEncoding:"}],"title":"geoJSONDataUsingEncoding:","url":"\/documentation\/maplibre\/mlnshape\/geojsondatausingencoding:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/title":{"fragments":[{"text":"title","kind":"identifier"}],"role":"symbol","type":"topic","abstract":[{"text":"The title of the shape annotation.","type":"text"}],"identif