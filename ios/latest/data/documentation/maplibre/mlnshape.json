t.MyProject/documentation/MapLibre/MLNShape/title":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnshape\/title","fragments":[{"text":"title","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title","kind":"symbol","abstract":[{"text":"The title of the shape annotation.","type":"text"}],"title":"title"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"url":"\/documentation\/maplibre\/mlnpolylinefeature","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"kind":"symbol","type":"topic","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","isActive":true},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","title":"MLNPolylineFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"kind":"symbol","role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNMultiPolygonFeature"}],"url":"\/documentation\/maplibre\/mlnmultipolygonfeature","title":"MLNMultiPolygonFeature","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature"},{"type":"text","text":" object associates a multipolygon shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"kind":"symbol","type":"topic","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"text":" object represents a closed shape consisting of four or more","type":"text"},{"text":" ","type":"text"},{"text":"vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the edges that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use a polygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"building, a lake, or an area you want to highlight.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","url":"\/documentation\/maplibre\/mlnpolygon","fragments":[{"text":"MLNPolygon","kind":"identifier"}],"role":"symbol","title":"MLNPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]}}}