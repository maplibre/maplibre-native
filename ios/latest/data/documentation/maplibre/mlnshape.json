n/MapLibre/MLNEmptyFeature":{"role":"symbol","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","type":"reference","isActive":true},{"type":"text","text":" object associates an empty shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"fragments":[{"kind":"identifier","text":"MLNEmptyFeature"}],"title":"MLNEmptyFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","url":"\/documentation\/maplibre\/mlnemptyfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" or "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"text":"GeoJSON","type":"text"},{"text":" code. A shape source is added to an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"text":" object along with an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"text":" object. The vector style","type":"text"},{"text":" ","type":"text"},{"text":"layer defines the appearance of any content supplied by the shape source. You","type":"text"},{"text":" ","type":"text"},{"text":"can update a shape source by setting its ","type":"text"},{"code":"shape","type":"codeVoice"},{"text":" or ","type":"text"},{"code":"URL","type":"codeVoice"},{"text":" property.","type":"text"}],"url":"\/documentation\/maplibre\/mlnshapesource","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","kind":"symbol","title":"MLNShapeSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"topic","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"title":"MLNPointAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"role":"symbol","fragments":[{"text":"MLNStyle","kind":"identifier"}],"title":"MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"topic","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"reference","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"text":" ","type":"text"},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference","isActive":true},{"text":" object, while the surrounding","type":"text"},{"text":" ","type":"text"},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmultipolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"kind":"symbol","title":"MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"title":"MLNShape","type":"topic","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"role":"symbol","fragments":[{"text":"MLNShape","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshape","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"}}}