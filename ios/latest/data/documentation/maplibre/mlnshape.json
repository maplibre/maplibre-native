role":"symbol","fragments":[{"kind":"identifier","text":"predicate"}],"type":"topic","kind":"symbol","abstract":[{"text":"The style layer’s predicate.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer\/predicate"},"doc://org.swift.MyProject/objc(cs)NSObject":{"type":"unresolvable","title":"NSObject","identifier":"doc:\/\/org.swift.MyProject\/objc(cs)NSObject"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"title":"MLNMultiPolyline","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"text":" object.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolyline","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"title":"MLNVectorTileSource","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"topic","url":"\/documentation\/maplibre\/mlnvectortilesource","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"reference","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","type":"reference","isActive":true},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"title":"MLNMultiPolylineFeature","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","isActive":true,"type":"reference"},{"text":" object associates a multipolyline shape with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","role":"symbol","fragments":[{"text":"MLNMultiPolylineFeature","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolylinefeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"abstract":[{"type":"text","text":"The proxy object for the current map style."}],"url":"\/documentation\/maplibre\/mlnstyle","fragments":[{"text":"MLNStyle","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","role":"symbol","type":"topic","title":"MLNStyle","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/title":{"title":"title","fragments":[{"kind":"identifier","text":"title"}],"abstract":[{"type":"text","text":"The title of the shape annotation."}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title","url":"\/documentation\/maplibre\/mlnshape\/title","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","title":"MLNAnnotation","role":"symbol"}}}