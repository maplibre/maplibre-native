NS SELF","type":"codeVoice"},{"type":"text","text":" and set the"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer\/predicate"},{"type":"text","text":" property to that predicate, or set a layout or"},{"type":"text","text":" "},{"type":"text","text":"paint attribute to a similarly formatted "},{"code":"NSExpression","type":"codeVoice"},{"type":"text","text":"."}]}],"kind":"content"}],"relationshipsSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/objc(cs)NSObject"],"kind":"relationships","title":"Inherits From","type":"inheritsFrom"},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection"],"kind":"relationships","title":"Inherited By","type":"inheritedBy"},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","doc:\/\/org.swift.MyProject\/objc(pl)NSSecureCoding"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"title":"MLNPolygon","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"type":"text","text":" object represents a closed shape consisting of four or more"},{"type":"text","text":" "},{"type":"text","text":"vertices,