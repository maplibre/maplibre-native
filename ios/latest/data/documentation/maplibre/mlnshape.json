rg.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"kind":"symbol","title":"MLNPointCollection","type":"topic","url":"\/documentation\/maplibre\/mlnpointcollection","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","fragments":[{"kind":"identifier","text":"MLNShape"}],"type":"topic","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnshape","title":"MLNShape"},"doc://org.swift.MyProject/objc(cs)NSObject":{"type":"unresolvable","title":"NSObject","identifier":"doc:\/\/org.swift.MyProject\/objc(cs)NSObject"},"doc://org.swift.MyProject/objc(pl)NSSecureCoding":{"identifier":"doc:\/\/org.swift.MyProject\/objc(pl)NSSecureCoding","title":"NSSecureCoding","type":"unresolvable"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/subtitle":{"fragments":[{"text":"subtitle","kind":"identifier"}],"title":"subtitle","abstract":[{"text":"The subtitle of the shape annotation. The default value of this property is","type":"text"},{"text":" ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":".","type":"text"}],"url":"\/documentation\/maplibre\/mlnshape\/subtitle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle","type":"topic","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/title":{"fragments":[{"text":"title","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title","kind":"symbol","title":"title","role":"symbol","url":"\/documentation\/maplibre\/mlnshape\/title","abstract":[{"text":"The title of the shape annotation.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","title":"MLNMultiPoint","type":"topic","url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"url":"\/documentation\/maplibre\/mlnvectortilesource","title":"MLNVectorTileSource","role":"symbol","type":"topic","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"title":"MLNEmptyFeature","fragments":[{"text":"MLNEmptyFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","type":"topic","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature"},{"text":" object associates an empty shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnemptyfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/geoJSONDataUsingEncoding:":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/geoJSONDataUsingEncoding:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"geoJSONDataUsingEncoding:"}],"navigatorTitle":[{"kind":"identifier","text":"geoJSONDataUsingEncoding:"}],"abstract":[{"text":"Returns the GeoJSON string representation of the shape encapsulated in a data","type":"text"},{"text":" ","type":"text"},{"text":"object.","type":"text"}],"title":"geoJSONDataUsingEncoding:","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnshape\/geojsondatausingencoding:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"type":"topic","url":"\/documentation\/maplibre\/mlnpointannotation","kind":"symbol","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}],"title":"MLNPointAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"role":"symbol","fragments":[{"text":"MLNMultiPolylineFeature","kind":"identifier"}],"title":"MLNMultiPolylineFeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolylinefeature","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","isActive":true,"type":"reference"},{"text":" object associates a multipolyline shape with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}]}}}