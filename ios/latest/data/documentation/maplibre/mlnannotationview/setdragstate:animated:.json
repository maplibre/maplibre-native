es"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"title":"MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setDragState:animated:":{"role":"symbol","navigatorTitle":[{"text":"setDragState:animated:","kind":"identifier"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"setDragState:animated:"}],"url":"\/documentation\/maplibre\/mlnannotationview\/setdragstate:animated:","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setDragState:animated:","abstract":[{"type":"text","text":"Sets the current drag state for the annotation view."}],"kind":"symbol","title":"setDragState:animated:"}}}