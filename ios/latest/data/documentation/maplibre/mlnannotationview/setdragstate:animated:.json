/documentation/MapLibre/MLNAnnotationViewDragState":{"kind":"symbol","abstract":[{"text":"These constants indicate the current drag state of an annotation view.","type":"text"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState","url":"\/documentation\/maplibre\/mlnannotationviewdragstate","fragments":[{"kind":"identifier","text":"MLNAnnotationViewDragState"}],"type":"topic","title":"MLNAnnotationViewDragState"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","kind":"symbol","title":"MapLibre","type":"topic","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]}}}