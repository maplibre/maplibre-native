ype":"text"},{"type":"text","text":"objects to handle the display."}],"role":"symbol","title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationViewDragState":{"url":"\/documentation\/maplibre\/mlnannotationviewdragstate","role":"symbol","type":"topic","abstract":[{"text":"These constants indicate the current drag state of an annotation view.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNAnnotationViewDragState"}],"kind":"symbol","title":"MLNAnnotationViewDragState","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState"}}}