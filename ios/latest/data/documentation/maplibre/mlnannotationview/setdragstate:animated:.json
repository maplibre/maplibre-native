d":"declarations"},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"inlineContent":[{"text":"You can override this method to animate a custom annotation view as the user","type":"text"},{"text":" ","type":"text"},{"text":"drags it. As the system detects user actions that would indicate a drag, it","type":"text"},{"text":" ","type":"text"},{"text":"calls this method to update the drag state.","type":"text"}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationViewDragState":{"title":"MLNAnnotationViewDragState","abstract":[{"text":"These constants indicate the current drag state of an annotation view.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNAnnotationViewDragState","kind":"identifier"}],"type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationviewdragstate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setDragState:animated:":{"fragments":[{"text":"- ","kind":"text"},{"text":"setDragState:animated:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotationview\/setdragstate:animated:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setDragState:animated:","abstract":[{"text":"Sets the current drag state for the annotation view.","type":"text"}],"role":"symbol","kind":"symbol","title":"setDragState:animated:","type":"topic","navigatorTitle":[{"text":"setDragState:animated:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","type":"topic","title":"MapLibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","role":"symbol","type":"topic","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView"}}}