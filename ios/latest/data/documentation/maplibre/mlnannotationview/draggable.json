{"metadata":{"externalID":"c:objc(cs)MLNAnnotationView(py)draggable","role":"symbol","navigatorTitle":[{"text":"draggable","kind":"identifier"}],"remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L261"},"symbolKind":"property","fragments":[{"text":"draggable","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"draggable","roleHeading":"Instance Property"},"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is draggable."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"kind":"keyword","text":"nonatomic"},{"text":", ","kind":"text"},{"text":"assign","kind":"keyword"},{"text":", ","kind":"text"},{"kind":"keyword","text":"getter"},{"text":"=","kind":"text"},{"kind":"identifier","text":"isDraggable"},{"text":") ","kind":"text"},{"kind":"typeIdentifier","text":"BOOL","preciseIdentifier":"c:@T@BOOL"},{"text":" ","kind":"text"},{"text":"draggable","kind":"identifier"},{"kind":"text","text":";"}]}]},{"kind":"content","content":[{"type":"heading","anchor":"discussion","level":2,"text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If this property is set to "},{"type":"codeVoice","code":"YES"},{"type":"text","text":", the user can drag the annotation after"},{"text":" ","type":"text"},{"text":"pressing and holding the view, and the associated annotation object must also","type":"text"},{"type":"text","text":" "},{"text":"implement the ","type":"text"},{"code":"setCoordinate:","type":"codeVoice"},{"text":" method. The default value of this property is","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"NO"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"text":"Setting this property to ","type":"text"},{"type":"codeVoice","code":"YES"},{"text":" lets the map view know that the annotation is","type":"text"},{"text":" ","type":"text"},{"text":"always draggable. In other words, you cannot conditionalize drag operations by","type":"text"},{"type":"text","text":" "},{"text":"attempting to stop an operation that has already been initiated; doing so can","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"lead to undefined behavior. Once begun, the drag operation should always"},{"text":" ","type":"text"},{"type":"text","text":"continue to completion."}]},{"type":"heading","anchor":"Related-examples","level":4,"text":"Related examples"},{"inlineContent":[{"text":"TODO: Enable users to drag ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" objects on your map."}],"type":"paragraph"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","interfaceLanguage":"occ"},"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/draggable"]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","kind":"symbol","title":"MLNAnnotationView","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"text":" class is responsible for marking a point annotation","type":"text"},{"type":"text","text":" "},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic","url":"\/documentation\/maplibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/draggable":{"url":"\/documentation\/maplibre\/mlnannotationview\/draggable","role":"symbol","abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is draggable."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"draggable"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","title":"draggable"}}}