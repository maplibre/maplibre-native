ragments":[{"kind":"identifier","text":"reuseIdentifier"}],"symbolKind":"property","navigatorTitle":[{"kind":"identifier","text":"reuseIdentifier"}],"modules":[{"name":"MapLibre"}]},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"abstract":[{"type":"text","text":"The string that identifies that this annotation view is reusable."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/reuseIdentifier":{"abstract":[{"text":"The string that identifies that this annotation view is reusable.","type":"text"}],"fragments":[{"text":"reuseIdentifier","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview\/reuseidentifier","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/reuseIdentifier","type":"topic","kind":"symbol","title":"reuseIdentifier"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-sourc