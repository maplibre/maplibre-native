ections":[],"abstract":[{"text":"The annotation object currently associated with the view.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/annotation","abstract":[{"text":"The annotation object currently associated with the view.","type":"text"}],"role":"symbol","fragments":[{"text":"annotation","kind":"identifier"}],"title":"annotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],