age":"occ"},"abstract":[{"type":"text","text":"The annotation object currently associated with the view."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation","type":"topic","fragments":[{"text":"annotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotationview\/annotation","kind":"symbol","title":"annotation","role":"symbol","abstract":[{"text":"The annotation object currently associated with the view.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","kind":"symbol","url":"\/documentation\/maplibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotationview","kind":"symbol","title":"MLNAnnotationView","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"}}}