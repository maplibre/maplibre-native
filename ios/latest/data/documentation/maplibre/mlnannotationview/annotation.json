ype":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"abstract":[{"type":"text","text":"The annotation object currently associated with the view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview\/annotation","kind":"symbol","type":"topic","title":"annotation","fragments":[{"text":"annotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"type":"topic","title":"MapLibre","url":"\/documentation\/maplibre","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"}}}