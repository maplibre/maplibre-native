nalID":"c:objc(cs)MLNAnnotationView(py)annotation","modules":[{"name":"MapLibre"}],"symbolKind":"property"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"type":"text","text":" "},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"offscreen."}],"title":"MLNAnnotationView","type":"topic","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"role":"symbol","abstract":[{"text":"The annotation object currently associated with the view.","type":"text"}],"title":"annotation","kind":"symbol","fragments":[{"text":"annotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/annotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}]}}}