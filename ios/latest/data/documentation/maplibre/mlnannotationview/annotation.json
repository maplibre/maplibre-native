text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"role":"symbol","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The annotation object currently associated with the view."}],"title":"annotation","fragments":[{"text":"annotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation","url":"\/documentation\/maplibre\/mlnannotationview\/annotation"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre","kind":"symbol"}}}