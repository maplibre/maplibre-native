raits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/annotation"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"title":"MLNAnnotationView","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"text":" ","type":"text"},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"text":" ","type":"text"},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"text":" ","type":"text"},{"type":"text","text":"offscreen."}],"type":"topic","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","type":"topic","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"identifier":"do