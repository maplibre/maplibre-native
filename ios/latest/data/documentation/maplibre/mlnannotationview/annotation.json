ject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"type":"topic","role":"symbol","abstract":[{"text":"The annotation object currently associated with the view.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation","url":"\/documentation\/maplibre\/mlnannotationview\/annotation","kind":"symbol","title":"annotation","fragments":[{"kind":"identifier","text":"annotation"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"text":" ","type":"text"},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"role":"symbol","title":"MLNAnnotationView","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","title":"MLNAnnotation","kind":"symbol"}}}