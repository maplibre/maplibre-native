nd":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"url":"\/documentation\/maplibre\/mlnannotationview\/setselected:animated:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","title":"setSelected:animated:","kind":"symbol","abstract":[{"text":"Sets the selection state of the annotation view with an optional animation.","type":"text"}],"role":"symbol","type":"topic","fragments":[{"kind":"text","text":"- "},{"text":"setSelected:animated:","kind":"identifier"}],"navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","identifier":"doc:\/\/org.swift.MyProj