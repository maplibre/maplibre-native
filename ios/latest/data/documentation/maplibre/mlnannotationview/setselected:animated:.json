ed-examples"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample","type":"reference"}],"type":"paragraph"}]}]}],"kind":"content"}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/setselected:animated:"]}],"metadata":{"symbolKind":"method","roleHeading":"Instance Method","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L230"},"title":"setSelected:animated:","navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}],"externalID":"c:objc(cs)MLNAnnotationView(im)setSelected:animated:","modules":[{"name":"MapLibre"}],"role":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"setSelected:animated:","kind":"identifier"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","title":"setSelected:animated:","url":"\/documentation\/maplibre\/mlnannotationview\/setselected:animated:","kind":"symbol","abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"role":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"setSelected:animated:","kind":"identifier"}],"navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnnotationViewExample":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample","kind":"article","abstract":[{"text":"Add a custom annotation view","type":"text"}],"url":"\/documentation\/maplibre-native-for-ios\/annotationviewexample","role":"article","type":"topic","title":"Custom Annotation View"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","title":"MapLibre","url":"\/documentation\/maplibre","role":"collection","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotationview","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"title":"MLNAnnotationView","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","kind":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"type":"topic"}}}