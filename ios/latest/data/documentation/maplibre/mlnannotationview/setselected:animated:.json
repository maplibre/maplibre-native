{"abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setSelected:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@T@BOOL","text":"BOOL","kind":"typeIdentifier"},{"kind":"text","text":") "},{"text":"selected","kind":"internalParam"},{"kind":"text","text":" "},{"text":"animated:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL","text":"BOOL"},{"kind":"text","text":") "},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"parameters":[{"name":"selected","content":[{"type":"paragraph","inlineContent":[{"code":"YES","type":"codeVoice"},{"type":"text","text":" if the view should display itself as selected; "},{"code":"NO","type":"codeVoice"},{"type":"text","text":" "},{"text":"if it should display itself as unselected.","type":"text"}]}]},{"name":"animated","content":[{"type":"paragraph","inlineContent":[{"code":"YES","type":"codeVoice"},{"text":" if the change in selection state is animated; ","type":"text"},{"code":"NO","type":"codeVoice"},{"type":"text","text":" if the"},{"text":" ","type":"text"},{"text":"change is immediate.","type":"text"}]}]}],"kind":"parameters"},{"content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"You should not call this method directly. A map view calls this method in","type":"text"},{"text":" ","type":"text"},{"text":"response to user interactions with the annotation. Subclasses may override this","type":"text"},{"text":" ","type":"text"},{"text":"method in order to customize the appearance of the view depending on its","type":"text"},{"text":" ","type":"text"}