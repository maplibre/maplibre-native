lineContent":[{"code":"YES","type":"codeVoice"},{"type":"text","text":" if the change in selection state is animated; "},{"type":"codeVoice","code":"NO"},{"type":"text","text":" if the"},{"text":" ","type":"text"},{"text":"change is immediate.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"You should not call this method directly. A map view calls this method in"},{"type":"text","text":" "},{"text":"response to user interactions with the annotation. Subclasses may override this","type":"text"},{"text":" ","type":"text"},{"text":"method in order to customize the appearance of the view depending on its","type":"text"},{"type":"text","text":" "},{"text":"selection state.","type":"text"}]},{"level":4,"type":"heading","anchor":"Related-examples","text":"Related examples"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample","type":"reference"}]}]}]}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","title":"MapLibre","role":"collection","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected: