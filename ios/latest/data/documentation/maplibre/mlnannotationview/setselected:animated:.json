nlineContent":[{"text":"You should not call this method directly. A map view calls this method in","type":"text"},{"text":" ","type":"text"},{"text":"response to user interactions with the annotation. Subclasses may override this","type":"text"},{"text":" ","type":"text"},{"text":"method in order to customize the appearance of the view depending on its","type":"text"},{"text":" ","type":"text"},{"text":"selection state.","type":"text"}]},{"level":4,"type":"heading","anchor":"Related-examples","text":"Related examples"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample","isActive":true}]}]}],"type":"unorderedList"}],"kind":"content"}],"metadata":{"externalID":"c:objc(cs)MLNAnnotationView(im)setSelected:animated:","roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}],"role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"setSelected:animated:","kind":"identifier"}],"symbolKind":"method","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L230"},"title":"setSelected:animated:"},"abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"title":"setSelected:animated:","url":"\/documentation\/maplibre\/mlnannotationview\/setselected:animated:","abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"setSelected:animated:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setSelected:animated:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","type":"topic","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","title":"MLNAnnotationView","kind":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotationview","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","url":"\/documentation\/maplibre","type":"topic","kind":"symbol","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnnotationViewExample":{"type":"topic","role":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample","url":"\/documentation\/maplibre-native-for-ios\/annotationviewexample","title":"Custom Annotation View","kind":"article","abstract":[{"type":"text","text":"Add a custom annotation view"}]}}}