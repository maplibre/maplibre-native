tation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNAnnotationView"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"kind":"symbol","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"tex