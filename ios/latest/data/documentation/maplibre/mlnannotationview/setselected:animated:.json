{"schemaVersion":{"patch":0,"minor":3,"major":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"setSelected:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier","text":"BOOL"},{"kind":"text","text":") "},{"kind":"internalParam","text":"selected"},{"kind":"text","text":" "},{"kind":"identifier","text":"animated:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier","text":"BOOL"},{"kind":"text","text":") "},{"kind":"internalParam","text":"animated"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"YES"},{"type":"text","text":" if the view should display itself as selected; "},{"type":"codeVoice","code":"NO"},{"type":"text","text":" "},{"type":"text","text":"if it should display itself as unselected."}]}],"name":"selected"},{"content":[{"inlineContent":[{"type":"codeVoice","code":"YES"},{"type":"text","text":" if the change in selection state is animated; "},{"type":"codeVoice","code":"NO"},{"type":"text","text":" if the"},{"type":"text","text":" "},{"type":"text","text":"change is immediate."}],"type":"paragraph"}],"name":"animated"}]},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"You should not call this method directly. A map view calls this method in","type":"text"},{"text":" ","type":"text"},{"text":"response to user interactions with the annotation. Subclasses may override this","type":"text"},{"text":" ","type":"text"},{"text":"method in order to customize the appearance of the view depending on its","type":"text"},{"text":" ","type":"text"},{"text":"selection state.","type":"text"}],"type":"paragraph"},{"le