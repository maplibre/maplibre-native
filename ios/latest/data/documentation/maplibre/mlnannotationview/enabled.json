ew/enabled":{"type":"topic","kind":"symbol","fragments":[{"kind":"identifier","text":"enabled"}],"title":"enabled","abstract":[],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/enabled","url":"\/documentation\/maplibre\/mlnannotationview\/enabled","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol. When an annotation’s","type":"text"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}]}}}