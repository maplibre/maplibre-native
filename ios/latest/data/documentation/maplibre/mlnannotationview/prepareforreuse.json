{"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/prepareforreuse"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/prepareForReuse"},"kind":"symbol","sections":[],"metadata":{"title":"prepareForReuse","externalID":"c:objc(cs)MLNAnnotationView(im)prepareForReuse","role":"symbol","symbolKind":"method","fragments":[{"text":"- ","kind":"text"},{"text":"prepareForReuse","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"prepareForReuse"}],"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L117","fileName":"MLNAnnotationView.h"}},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"abstract":[{"type":"text","text":"Called when the view is removed from the reuse queue."}],"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"text":"prepareForReuse","kind":"identifier"},{"text":";","kind":"text"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"content","content":[{"level":2,"type":"heading","anchor":"discussion","text":"Discussion"},{"inlineContent":[{"text":"The default implementation of this method does nothing. You can override it in","type":"text"},{"type":"text","text":" "},{"text":"your custom annotation view implementation to put the view in a known state","type":"text"},{"type":"text","text":" "},{"type":"text","text":"before it is returned to your map view delegate."}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/prepareForRe