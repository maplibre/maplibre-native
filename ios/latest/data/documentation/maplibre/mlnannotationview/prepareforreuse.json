{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/prepareforreuse"]}],"kind":"symbol","schemaVersion":{"minor":3,"patch":0,"major":0},"abstract":[{"text":"Called when the view is removed from the reuse queue.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"text":"prepareForReuse","kind":"identifier"},{"kind":"text","text":";"}]}]},{"kind":"content","content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"The default implementation of this method does nothing. You can override it in"},{"type":"text","text":" "},{"text":"your custom annotation view implementation to put the view in a known state","type":"text"},{"type":"text","text":" "},{"text":"before it is returned to your map view delegate.","type":"text"}],"type":"paragraph"}]}],"metadata":{"role":"symbol","roleHeading":"Instance Method","symbolKind":"method","externalID":"c:objc(cs)MLNAnnotationView(im)prepareForReuse","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L117"},"fragments":[{"kind":"text","text":"- "},{"text":"prepareForReuse","kind":"identifier"}],"navigatorTitle":[{"text":"prepareForReuse","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"prepareForReuse"},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/prepareForReuse","interfaceLanguage":"occ"},"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/prepareForRe