{"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/initwithannotation:reuseidentifier:"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithAnnotation:reuseIdentifier:","interfaceLanguage":"occ"},"kind":"symbol","sections":[],"abstract":[{"text":"Initializes and returns a new annotation view object.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:*Qo","kind":"typeIdentifier","text":"id"},{"kind":"text","text":") "},{"kind":"identifier","text":"initWithAnnotation:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","kind":"typeIdentifier","text":"id<MLNAnnotation>"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"kind":"identifier","text":"reuseIdentifier:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)NSString","kind":"typeIdentifier","text":"NSString"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"reuseIdentifier"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"parameters":[{"name":"annotation","content":[{"inlineContent":[{"text":"The annotation object to associate with the new view.","type":"text"}],"type":"paragraph"}]},{"name":"reuseIdentifier","content":[{"type":"paragraph","inlineContent":[{"text":"A unique string identifier for this view that allows you","type":"text"},{"text":" ","type":"text"},{"text":"to reuse this view with multiple similar annotations. You can set this","type":"text"},{"text":" ","type":"text"},{"text":"parameter to ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" if you don’t intend to reuse the view, but it is a good","type":"text"},{"text":" ","type":"text"},{"text":"idea in general to specify a reuse identifier to avoid creating redundant","type":"text"},{"text":" ","type":"text"},{"text":"views.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"anchor":"return-value","type":"heading","level":2,"text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The initialized annotation view object."}]}]},{"kind":"content","content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"Providing an annotation allows you to explicitly associate the annotation instance"},{"type":"text","text":" "},{"type":"text","text":"with the new view and, in custom subclasses of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":", customize the view"},{"type":"text","text":" "},{"type":"text","text":"based on properties of the annotation instance in an overridden initializer. However,"},{"type":"text","text":" "},{"type":"text","text":"annotation views that are reused will not necessarily be associated with the"},{"type":"text","text":" "},{"type":"text","text":"same annotation they were initialized with. Also, annotation views that are in"},{"type":"text","text":" "},{"type":"text","text":"the reuse queue will have a nil value for the annotation property. Passing an annotation"},{"type":"text","text":" "},{"type":"text","text":"instance to the view is optional and the map view will automatically associate annotations"},{"type":"text","text":" "},{"type":"text","text":"with views when views are provided to the map via the"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:"},{"type":"text","text":" method."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The reuse identifier provides a way for you to improve performance by recycling"},{"type":"text","text":" "},{"type":"text","text":"annotation views as they enter and leave the map’s viewport. As an annotation"},{"type":"text","text":" "},{"type":"text","text":"leaves the viewport, the map view moves its associated view to a reuse queue."},{"type":"text","text":" "},{"type":"text","text":"When a new annotation becomes visible, you can request a view for that"},{"type":"text","text":" "},{"type":"text","text":"annotation by passing the appropriate reuse identifier string to the"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:"},{"text":" method.","type":"text"}],"type":"paragraph"}]}],"metadata":{"role":"symbol","symbolKind":"method","navigatorTitle":[{"text":"initWithAnnotation:reuseIdentifier:","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","externalID":"c:objc(cs)MLNAnnotationView(im)initWithAnnotation:reuseIdentifier:","fragments":[{"text":"- ","kind":"text"},{"text":"initWithAnnotation:reuseIdentifier:","kind":"identifier"}],"title":"initWithAnnotation:reuseIdentifier:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L107","fileName":"MLNAnnotationView.h"}},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"type":"topic","title":"mapView:viewForAnnotation:","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"title":"MLNAnnotationView","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotationview","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/dequeueReusableAnnotationViewWithIdentifier:":{"kind":"symbol","navigatorTitle":[{"text":"dequeueReusableAnnotationViewWithIdentifier:","kind":"identifier"}],"fragments":[{"text":"- ","kind":"text"},{"text":"dequeueReusableAnnotationViewWithIdentifier:","kind":"identifier"}],"type":"topic","title":"dequeueReusableAnnotationViewWithIdentifier:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","role":"symbol","abstract":[{"text":"Returns a reusable annotation view object associated with its identifier.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapview\/dequeuereusableannotationviewwithidentifier:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/initWithAnnotation:reuseIdentifier:":{"type":"topic","fragments":[{"text":"- ","kind":"text"},{"text":"initWithAnnotation:reuseIdentifier:","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithAnnotation:reuseIdentifier:","url":"\/documentation\/maplibre\/mlnannotationview\/initwithannotation:reuseidentifier:","kind":"symbol","navigatorTitle":[{"text":"initWithAnnotation:reuseIdentifier:","kind":"identifier"}],"abstract":[{"type":"text","text":"Initializes and returns a new annotation view object."}],"title":"initWithAnnotation:reuseIdentifier:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol"}}}