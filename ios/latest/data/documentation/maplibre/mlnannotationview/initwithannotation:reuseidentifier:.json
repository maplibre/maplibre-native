ViewDelegate\/mapView:viewForAnnotation:","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"title":"mapView:viewForAnnotation:","type":"topic","abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","role":"collection","kind":"symbol"}}}