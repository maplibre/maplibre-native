le":"MLNAnnotationView","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"topic","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"title":"MLNAnnotationImage","url":"\/documentation\/maplibre\/mlnannotationimage","role":"symbol","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"fragments":[{"kind":"identifier","text":"MLNAnnotationImage"}],"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","url":"\/documentation\/maplibre","title":"MapLibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","role":"symbol","fragments":[{"text":"rotatesToMatchCamera","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"text","text":"A Boolean value that determines whether the annotation view rotates together"},{"type":"text","text":" "},{"type":"text","text":"with the map."}],"type":"topic","iden