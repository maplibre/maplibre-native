{"schemaVersion":{"major":0,"patch":0,"minor":3},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"kind":"keyword","text":"nonatomic"},{"kind":"text","text":", "},{"text":"assign","kind":"keyword"},{"text":") ","kind":"text"},{"preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier","text":"BOOL"},{"kind":"text","text":" "},{"text":"rotatesToMatchCamera","kind":"identifier"},{"kind":"text","text":";"}]}]},{"kind":"content","content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"When the value of this property is ","type":"text"},{"code":"YES","type":"codeVoice"},{"type":"text","text":" and the map is rotated, the annotation"},{"text":" ","type":"text"},{"type":"text","text":"view rotates. This is also the behavior of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true,"type":"reference"},{"type":"text","text":" objects. When the"},{"text":" ","type":"text"},{"type":"text","text":"value of this property is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":" the annotation has its rotation angle fixed."}]},{"inlineContent":[{"type":"text","text":"The default value of this property is "},{"code":"NO","type":"codeVoice"},{"type":"text","text":". Set this property to "},{"code":"YES","type":"codeVoice"},{"type":"text","text":" if the"},{"type":"text","text":" "},{"type":"text","text":"view’s rotation is important."}],"type":"paragraph"}]}],"kind":"symbol","abstract":[{"text":"A Boolean value that determines whether the annotation view rotates together","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with the map."}],"sections":[],"metadata":{"externalID":"c:objc(cs)MLNAnnotationView(py)rotatesToMatchCamera","role":"symbol","navigatorTitle":[{"text":"rotatesToMatchCamera","kind":"identifier"