ifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","kind":"symbol","type":"topic","url":"\/documentation\/maplibre","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"type":"topic","title":"rotatesToMatchCamera","kind":"symbol","fragments":[{"text":"rotatesToMatchCamera","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/rotatesToMatchCamera","abstract":[{"text":"A Boolean value that determines whether the annotation view rotates together","type":"text"},{"text":" ","type":"text"},{"text":"with the map.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","role":"symbol"}}}