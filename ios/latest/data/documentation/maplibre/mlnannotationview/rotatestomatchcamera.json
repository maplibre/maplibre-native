ject\/documentation\/MapLibre\/MLNAnnotationImage"},{"text":" objects. When the","type":"text"},{"text":" ","type":"text"},{"text":"value of this property is ","type":"text"},{"type":"codeVoice","code":"NO"},{"text":" the annotation has its rotation angle fixed.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The default value of this property is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":". Set this property to "},{"type":"codeVoice","code":"YES"},{"type":"text","text":" if the"},{"type":"text","text":" "},{"type":"text","text":"view’s rotation is important."}],"type":"paragraph"}]}],"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","url":"\/documentation\/maplibre","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true,"type":"reference"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"title":"MLNAnnotationImage","fragments":[{"kind":"identifier","text":"MLNAnnotationImage"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnota