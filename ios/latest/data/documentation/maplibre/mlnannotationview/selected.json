nView"},{"type":"text","text":" or "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"type":"text","text":" object, deselects any currently"},{"type":"text","text":" "},{"type":"text","text":"selected view."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Setting this property changes the view’s appearance to reflect the new value"},{"type":"text","text":" "},{"type":"text","text":"immediately. If you want the change to be animated, use the"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","isActive":true,"type":"reference"},{"type":"text","text":" method instead."}]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/selected"]}],"metadata":{"fragments":[{"kind":"identifier","text":"selected"}],"navigatorTitle":[{"text":"selected","kind":"identifier"}],"role":"symbol","roleHeading":"Instance Property","externalID":"c:objc(cs)MLNAnnotationView(py)selected","title":"selected","symbolKind":"property","modules":[{"name":"MapLibre"}],"remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L212"}},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/selected":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/selected","title":"selected","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/selected","abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is currently selected."}],"role":"symbol","fragments":[{"kind":"identifier","text":"selected"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","role":"symbol","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"title":"MapLibre","role":"collection","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","type":"topic","kind":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"url":"\/documentation\/maplibre\/mlnannotationview\/setselected:animated:","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","kind":"symbol","navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}],"type":"topic","abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"title":"setSelected:animated:","fragments":[{"text":"- ","kind":"text"},{"text":"setSelected:animated:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"type":"topic","kind":"symbol","title":"MLNAnnotationImage","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"}}}