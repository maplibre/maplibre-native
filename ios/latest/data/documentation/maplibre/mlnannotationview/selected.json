ference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true},{"text":" object, deselects any currently","type":"text"},{"type":"text","text":" "},{"type":"text","text":"selected view."}]},{"inlineContent":[{"text":"Setting this property changes the view’s appearance to reflect the new value","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"immediately. If you want the change to be animated, use the"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:"},{"type":"text","text":" method instead."}],"type":"paragraph"}],"kind":"content"}],"kind":"symbol","metadata":{"roleHeading":"Instance Property","modules":[{"name":"MapLibre"}],"fragments":[{"text":"selected","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"selected"}],"remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L212"},"symbolKind":"property","externalID":"c:objc(cs)MLNAnnotationView(py)selected","title":"selected","role":"symbol"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","kind":"symbol","abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"type":"topic","fragments":[{"kind":"text","text":"- "},{"text":"setSelected:animated:","kind":"identifier"}],"navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}],"title":"setSelected:animated:","url":"\/documentation\/maplibre\/mlnannotationview\/s