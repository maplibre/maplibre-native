hat"},{"type":"text","text":" "},{"text":"annotation by passing the appropriate reuse identifier string to the","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","type":"reference"},{"text":" method.","type":"text"}]}]}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/initwithreuseidentifier:"]}],"kind":"symbol","schemaVersion":{"major":0,"minor":3,"patch":0},"metadata":{"externalID":"c:objc(cs)MLNAnnotationView(im)initWithReuseIdentifier:","modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"initWithReuseIdentifier:"}],"title":"initWithReuseIdentifier:","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"initWithReuseIdentifier:"}],"symbolKind":"method","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L77","fileName":"MLNAnnotationView.h"}},"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"abstract":[{"text":"Initializes and returns a new annotation view object.","type":"text"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithReuseIdentifier:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/dequeueReusableAnnotationViewWithIdentifier:":{"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"dequeueReusableAnnotationViewWithIdentifier:"}],"type":"topic","title":"dequeueReusableAnnotationViewWithIdentifier:","url":"\/documentation\/maplibre\/mlnmapview\/dequeuereusableannotationviewwithidentifier:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapVi