ue.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"When a new annotation becomes visible, you can request a view for that"},{"type":"text","text":" "},{"type":"text","text":"annotation by passing the appropriate reuse identifier string to the"},{"type":"text","text":" "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","type":"reference"},{"type":"text","text":" method."}]}]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithReuseIdentifier:"},"kind":"symbol","sections":[],"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"initWithReuseIdentifier:"}],"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","role":"symbol","navigatorTitle":[{"text":"initWithReuseIdentifier:","kind":"identifier"}],"symbolKind":"method","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L77","fileName":"MLNAnnotationView.h"},"externalID":"c:objc(cs)MLNAnnotationView(im)initWithReuseIdentifier:","title":"initWithReuseIdentifier:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"abstract":[{"text":"Initializes and returns a new annotation view object.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/dequeueReusableAnnotationViewWithIdentifier:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","type":"topic","navigatorTitle":[{"text":"dequeueReusableAnnotationViewWithIdentifier:","kind":"identifier"}],"abstract":[{"text":"Returns a reusable annotation view object associated with its identifier.","type":"text"}],"role":"