hat"},{"type":"text","text":" "},{"text":"annotation by passing the appropriate reuse identifier string to the","type":"text"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:"},{"type":"text","text":" method."}]}]}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"abstract":[{"text":"Initializes and returns a new annotation view object.","type":"text"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"metadata":{"fragments":[{"text":"- ","kind":"text"},{"text":"initWithReuseIdentifier:","kind":"identifier"}],"externalID":"c:objc(cs)MLNAnnotationView(im)initWithReuseIdentifier:","symbolKind":"method","modules":[{"name":"MapLibre"}],"title":"initWithReuseIdentifier:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L77","fileName":"MLNAnnotationView.h"},"roleHeading":"Instance Method","role":"symbol","navigatorTitle":[{"text":"initWithReuseIdentifier:","kind":"identifier"}]},"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithReuseIdentifier:"},"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/initwithreuseidentifier:"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/initWithReuseIdentifier:":{"navigatorTitle":[{"kind":"identifier","text":"initWithReuseIdentifier:"}],"url":"\/documentation\/maplibre\/mlnannotationview\/initwithreuseidentifier:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithReuseIdentifier:","abstract":[{"text":"Initializes and returns a new annotation view object.","type":"text"}],"kind":"sy