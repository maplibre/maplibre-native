sible, you can request a view for that"},{"type":"text","text":" "},{"type":"text","text":"annotation by passing the appropriate reuse identifier string to the"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","isActive":true},{"type":"text","text":" method."}]}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithReuseIdentifier:","interfaceLanguage":"occ"},"metadata":{"symbolKind":"method","title":"initWithReuseIdentifier:","role":"symbol","roleHeading":"Instance Method","externalID":"c:objc(cs)MLNAnnotationView(im)initWithReuseIdentifier:","navigatorTitle":[{"text":"initWithReuseIdentifier:","kind":"identifier"}],"remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L77"},"fragments":[{"text":"- ","kind":"text"},{"text":"initWithReuseIdentifier:","kind":"identifier"}],"modules":[{"name":"MapLibre"}]},"schemaVersion":{"major":0,"patch":0,"minor":3},"abstract":[{"type":"text","text":"Initializes and returns a new annotation view object."}],"sections":[],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/initwithreuseidentifier:"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/dequeueReusableAnnotationViewWithIdentifier:":{"type":"topic","abstract":[{"text":"Returns a reusable annotation view object associated with its identifier.","type":"text"}],"title":"dequeueReusableAnnotationViewWithIdentifier:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","fragments":[