"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/draggable":{"abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is draggable."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","fragments":[{"kind":"identifier","text":"draggable"}],"type":"topic","title":"draggable","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview\/draggable"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"title":"MLNAnnotationView","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationViewDragState":{"abstract":[{"text":"These constants indicate the current drag state of an annotation view.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState","url":"\/documentation\/maplibre\/mlnannotationviewdragstate","title":"MLNAnnotationViewDragState","role":"symbol","fragments":[{"text":"MLNAnnotationViewDragState","kind":"identifier"}],"type":"topic"}}}