fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"kind":"symbol","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","kind":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/draggable":{"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","url":"\/documentation\/maplibre\/mlnannotationview\/draggable","role":"symbol","title":"draggable","fragments":[{"text":"draggable","kind":"identifier"}],"abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is draggable."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationViewDragState":{"type":"topic","kind":"symbol","title":"MLNAnnotationViewDragState","fragments":[{"kind":"identifier","text":"MLNAnnotationViewDragState"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotationviewdragstate","abstract":[{"text":"These constants indicate the current drag state of an annotation view.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState"}}}