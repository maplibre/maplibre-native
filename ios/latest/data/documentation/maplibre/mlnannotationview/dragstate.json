cumentation/MapLibre/MLNAnnotationView":{"kind":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"role":"symbol","title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/dragState":{"role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview\/dragstate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/dragState","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The current drag state of the annotation view."}],"title":"dragState","fragments":[{"kind":"identifier","text":"dragState"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/draggable":{"abstract":[{"text":"A Boolean value indicating whether the annotation view is draggable.","type":"text"}],"title":"draggable","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/draggable","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","fragments":[{"text":"draggable","kind":"identifier"}],"kind":"symbol"}}}