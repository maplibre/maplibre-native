tle":"MLNAnnotationViewDragState","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState","type":"topic","abstract":[{"type":"text","text":"These constants indicate the current drag state of an annotation view."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","title":"MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setDragState:animated:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setDragState:animated:","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setDragState:animated:"}],"url":"\/documentation\/maplibre\/mlnannotationview\/setdragstate:animated:","type":"topic","navigatorTitle":[{"kind":"identifier","text":"setDragState:animated:"}],"title":"setDragState:animated:","role":"symbol","abstract":[{"type":"text","text":"Sets the current drag state for the annotation view."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","role":"symbol","kind":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]}}}