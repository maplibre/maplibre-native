xt"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"type":"topic","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/dragState":{"type":"topic","abstract":[{"text":"The current drag state of the annotation view.","type":"text"}],"title":"dragState","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/dragState","url":"\/documentation\/maplibre\/mlnannotationview\/dragstate","fragments":[{"kind":"identifier","text":"dragState"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setDragState:animated:":{"title":"setDragState:animated:","abstract":[{"text":"Sets the current drag state for the annotation view.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview\/setdragstate:animated:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setDragState:animated:"}],"kind":"symbol","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setDragState:animated:","navigatorTitle":[{"kind":"identifier","text":"setDragState:animated:"}]}}}