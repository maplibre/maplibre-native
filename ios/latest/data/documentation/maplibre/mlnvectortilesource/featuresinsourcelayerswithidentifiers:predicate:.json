ype":"text","text":" "},{"text":"load but filters out the features in question, preventing them from being","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"drawn. To obtain only "},{"type":"emphasis","inlineContent":[{"type":"text","text":"visible"}]},{"type":"text","text":" features, use the"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:"},{"type":"text","text":" "},{"text":"or","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:"},{"type":"text","text":" "},{"text":"method.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"]]},"kind":"symbol","metadata":{"externalID":"c:objc(cs)MLNVectorTileSource(im)featuresInSourceLayersWithIdentifiers:predicate:","symbolKind":"method","roleHeading":"Instance Method","navigatorTitle":[{"kind":"identifier","text":"featuresInSourceLayersWithIdentifiers:predicate:"}],"fragments":[{"text":"- ","kind":"text"},{"text":"featuresInSourceLayersWithIdentifiers:predicate:","kind":"identifier"}],"title":"featuresInSourceLayersWithIdentifiers:predicate:","modules":[{"name":"MapLibre"}],"role":"symbol"},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnvectortilesource\/featuresinsourcelayerswithidentifiers:predicate:"]}],"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"type":"text","text":"Returns an array of map features loaded by this source, restricted to the given"},{"type":"text","text":" "},{"type":"text","text":"source layers and filtered by the given predicate."}],"iden