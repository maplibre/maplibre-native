rdinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"kind":"symbol","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"text":" class is responsible for presenting point-based","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"code":"UIImage","type":"codeVoice"},{"text":" ","type":"text"},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage","title":"MLNAnnotationImage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:imageForAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:imageForAnnotation:","abstract":[{"type":"text","text":"Returns an annotation image object to mark the given point annotation object on"},{"text":" ","type":"text"},{"type":"text","text":"the map."}],"kind":"symbol","type":"topic","role":"symbol","navigatorTitle":[{"text":"mapView:imageForAnnotation:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:imageforannotation:","title":"mapView:imageForAnnotation:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:imageForAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic