ts that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","fragments":[{"kind":"identifier","text":"MLNMapView"}],"url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","title":"MLNMapView","role":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:didSelectAnnotationView:":{"role":"symbol","type":"topic","title":"mapView:didSelectAnnotationView:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:didSelectAnnotationView:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:didSelectAnnotationView:"}],"abstract":[{"type":"text","text":"Tells the delegate that one of its annotation views was selected."}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:didselectannotationview:","navigatorTitle":[{"kind":"identifier","text":"mapView:didSelectAnnotationView:"}]},"