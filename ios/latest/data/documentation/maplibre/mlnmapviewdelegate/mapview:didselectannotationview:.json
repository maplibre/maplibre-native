ind":"parameters","parameters":[{"name":"mapView","content":[{"inlineContent":[{"text":"The map view containing the annotation.","type":"text"}],"type":"paragraph"}]},{"name":"annotationView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation view that was selected."}]}]}]},{"content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"You can use this method to track changes in the selection state of annotation"},{"type":"text","text":" "},{"type":"text","text":"views."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This method is only called for annotation views. To track changes in the"},{"type":"text","text":" "},{"type":"text","text":"selection state of all annotations, including those associated with static"},{"type":"text","text":" "},{"type":"text","text":"annotation images, implement "},{"type":"codeVoice","code":"-mapView:didSelectAnnotation:"},{"type":"text","text":", which is called"},{"type":"text","text":" "},{"type":"text","text":"immediately before this method is called."}],"type":"paragraph"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:didselectannotationview:"]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"title":"MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:didSelectAnnotationView:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:didSelectAnnotationView:","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:didselectannotationview:","navigatorTitle":[{"kind":"identifier","text":"mapView:didSelectAnnotationView:"}],"abstract":[{"text":"Tells the delegate that one of its annotation views was selected.","type":"text"}],"title":"mapView:didSelectAnnotationView:","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:didSelectAnnotationView:"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"type":"topic","role":"collection","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"kind":"identifier","text":"MLNMapView"}],"title":"MLNMapView"}}}