content":[{"type":"heading","text":"Discussion","anchor":"discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"type":"text","text":" "},{"text":"positioned on either side of the annotation title text. If an accessory view","type":"text"},{"type":"text","text":" "},{"type":"text","text":"you specify is a descendant of the "},{"type":"codeVoice","code":"UIControl"},{"type":"text","text":" class, the map view calls this"},{"text":" ","type":"text"},{"text":"method as a convenience whenever the user taps your view. You can use this","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"text":" ","type":"text"},{"type":"text","text":"For example, if your control displays additional information about the"},{"type":"text","text":" "},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"text":" ","type":"text"},{"type":"text","text":"information."}]},{"inlineContent":[{"text":"If your custom accessory views are not descendants of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the map view does not call this method. If the annotation has a custom callout"},{"type":"text","text":" "},{"text":"view via the ","type":"text"},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"type":"text","text":" method, you can specify the"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true},{"type":"text","text":" protocol’s"},{"text":" ","type":"text"},{"type":"codeVoice","code":"leftAccessoryView"},{"text":" and ","type":"text"},{"type":"codeVoice","code":"rightAccessoryView"},{"type":"text","text":" properties."}],"type":"paragraph"},{"text":"Related examples","anchor":"Related-examples","type":"heading","level":4},{"inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"type":"text","text":"standard callout bubble’s accessory control is tapped."}],"type":"paragraph"}],"kind":"content"}],"metadata":{"symbolKind":"method","fragments":[{"kind":"text","text":"- "},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"role":"symbol","modules":[{"name":"MapLibre"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L982","fileName":"MLNMapViewDelegate.h"},"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:","navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"roleHeading":"Instance Method","title":"mapView:annotation:calloutAccessoryControlTapped:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"kind":"symbol","title":"MLNMapViewDelegate","url":"\/documentation\/maplibre\/mlnmapviewdelegate","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"text","text":" "},{"text":"require the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"type":"text","text":" "},{"text":"individual annotations.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","type":"topic","url":"\/documentation\/maplibre\/mlncalloutview","fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"abstract":[{"type":"text","text":"A protocol for a "},{"type":"codeVoice","code":"UIView"},{"type":"text","text":" subclass that displays information about a selected"},{"text":" ","type":"text"},{"text":"annotation near that annotation.","type":"text"}],"title":"MLNCalloutView"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","fragments":[{"text":"MLNM