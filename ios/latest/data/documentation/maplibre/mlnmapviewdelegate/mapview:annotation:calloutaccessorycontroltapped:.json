{"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"text":" ","type":"text"},{"type":"text","text":"annotation’s callout view."}],"primaryContentSections":[{"declarations":[{"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"text":"mapView:","kind":"identifier"},{"kind":"text","text":"("},{"text":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"text":"annotation:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","kind":"typeIdentifier","text":"id<MLNAnnotation>"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"text":" ","kind":"text"},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"kind":"text","text":"("},{"text":"UIControl","preciseIdentifier":"c:objc(cs)UIControl","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"control"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}]}],"name":"mapView"},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}]}]},{"name":"control","content":[{"inlineContent":[{"text":"The control that was tapped.","type":"text"}],"type":"paragraph"}]}]},{"kind":"content","content":[{"anchor":"di