{"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L982","fileName":"MLNMapViewDelegate.h"},"modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"roleHeading":"Instance Method","symbolKind":"method","navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"role":"symbol","title":"mapView:annotation:calloutAccessoryControlTapped:"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","text":"MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"kind":"identifier","text":"annotation:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)UIControl","text":"UIControl","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"text":"control","kind":"internalParam"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}]},{"parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}]}]},{"name