ind":"parameters","parameters":[{"name":"mapView","content":[{"inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}]}],"name":"annotation"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The control that was tapped."}]}],"name":"control"}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"inlineContent":[{"text":"In a standard callout view, accessory views contain custom content and are","type":"text"},{"type":"text","text":" "},{"type":"text","text":"positioned on either side of the annotation title text. If an accessory view"},{"type":"text","text":" "},{"type":"text","text":"you specify is a descendant of the "},{"code":"UIControl","type":"codeVoice"},{"type":"text","text":" class, the map view calls this"},{"type":"text","text":" "},{"text":"method as a convenience whenever the user taps your view. You can use this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"type":"text","text":" "},{"text":"For example, if your control displays additional information about the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"text":" ","type":"text"},{"type":"text","text":"information."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If your custom accessory views are not descendants of the "},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"text":"the map view does not call this method. If the annotation has a custom callout","type":"text"},{"type":"text","text":" "},{"type":"text","text":"view via the "},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"type":"text","text":" method, you can specify the"},{"type":"text","text":" "},{"text":"custom accessory views using the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true},{"text":" protocol’s","type":"text"},{"type":"text","text":" "},{"code":"leftAccessoryView","type":"codeVoice"},{"type":"text","text":" and "},{"code":"rightAccessoryView","type":"codeVoice"},{"type":"text","text":" properties."}],"type":"paragraph"},{"level":4,"text":"Related examples","type":"heading","anchor":"Related-examples"},{"inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}],"type":"paragraph"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"text":"annotation’s callout view.","type":"text"}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"url":"\/documentation\/maplibre\/mlncalloutview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","abstract":[{"text":"A protocol for a ","type":"text"},{"type":"codeVoice","code":"UIView"},{"type":"text","text":" subclass that displays information about a selected"},{"type":"text","text":" "},{"type":"text","text":"annotation near that annotation."}],"role":"symbol","type":"topic","title":"MLNCalloutView","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNCalloutVie