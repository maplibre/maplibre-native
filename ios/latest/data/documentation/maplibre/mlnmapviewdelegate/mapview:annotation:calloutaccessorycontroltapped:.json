{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView","text":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"kind":"identifier","text":"annotation:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"kind":"identifier","text":"calloutAccessoryControlTapped:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIControl","text":"UIControl"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}]}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}]}]},{"name":"control","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The control that was tapped."}]}]}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"In a standard callout view, accessory views contain custom content and are","type":"text"},{"text":" ","type":"text"},{"text":"positioned on either side of the annotation title text. If an accessory view","type":"text"},{"text":" ","type":"text"},{"text":"you specify is a descendant of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class, the map view calls this","type":"text"},{"text":" ","type":"text"},{"text":"method as a convenience whenever the user taps your view. You can use this","type":"text"},{"text":" ","type":"text"},{"text":"method to respond to taps and perform any actions associated with that control.","type":"text"},{"text":" ","type":"text"},{"text":"For example, if your control displays additional information about the","type":"text"},{"text":" ","type":"text"},{"text":"annotation, you could use this method to present a modal panel with that","type":"text"},{"text":" ","type":"text"},{"text":"information.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"If your custom accessory views are not descendants of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"text":"the map view does not call this method. If the annotation has a custom callout","type":"text"},{"text":" ","type":"text"},{"text":"view via the ","type":"text"},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"text":" method, you can specify the","type":"text"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView"},{"text":" protocol’s","type":"text"},{"text":" ","type":"text"},{"code":"leftAccessoryView","type":"codeVoice"},{"text":" and ","type":"text"},{"code":"rightAccessoryView","type":"codeVoice"},{"text":" properties.","type":"text"}]},{"type":"heading","text":"Related examples","level":4,"anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"text":"TODO: Default callout usage, learn how to trigger an action when the","type":"text"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}]}]}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"],"traits":[{"interfaceLanguage":"occ"}]}],"abstract":[{"text":"Tells the delegate that the user tapped one of the accessory controls in the","type":"text"},{"text":" ","type":"text"},{"text":"annotation’s callout view.","type":"text"}],"metadata":{"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L982"},"role":"symbol","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:","title":"mapView:annotation:calloutAccessoryControlTapped:","symbolKind":"method"},"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","role":"collection","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"abstra