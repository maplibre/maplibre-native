{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"text":" ","type":"text"},{"text":"annotation’s callout view.","type":"text"}],"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"text":") ","kind":"text"},{"kind":"identifier","text":"mapView:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","text":"MLNMapView","preciseIdentifier":"c:objc(cs)MLNMapView"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"annotation:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"kind":"text","text":" "},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)UIControl","text":"UIControl","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}],"languages":["occ"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}]}]},{"name":"annotation","content":[{"inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"The control that was tapped."}],"type":"paragraph"}],"name":"control"}]},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"dis