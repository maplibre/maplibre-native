nnotation:"},{"type":"text","text":" method, you can specify the"},{"type":"text","text":" "},{"type":"text","text":"custom accessory views using the "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","type":"reference"},{"text":" protocol’s","type":"text"},{"text":" ","type":"text"},{"code":"leftAccessoryView","type":"codeVoice"},{"text":" and ","type":"text"},{"code":"rightAccessoryView","type":"codeVoice"},{"text":" properties.","type":"text"}]},{"text":"Related examples","anchor":"Related-examples","type":"heading","level":4},{"type":"paragraph","inlineContent":[{"text":"TODO: Default callout usage, learn how to trigger an action when the","type":"text"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}]}],"kind":"content"}],"kind":"symbol","abstract":[{"text":"Tells the delegate that the user tapped one of the accessory controls in the","type":"text"},{"text":" ","type":"text"},{"text":"annotation’s callout view.","type":"text"}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotation:calloutAccessoryControlTapped:":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","navigatorTitle":[{"kind":"identifier","text":"mapView:annotation:calloutAccessoryControlTapped:"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:annotatio