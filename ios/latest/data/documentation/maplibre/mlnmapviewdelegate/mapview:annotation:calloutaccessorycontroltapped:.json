{"kind":"symbol","abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"],"traits":[{"interfaceLanguage":"occ"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"schemaVersion":{"major":0,"minor":3,"patch":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"typeIdentifier","text":"MLNMapView","preciseIdentifier":"c:objc(cs)MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"kind":"identifier","text":"annotation:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"kind":"identifier","text":"calloutAccessoryControlTapped:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIControl","text":"UIControl"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}],"platforms":["iOS"]}]},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}]}],"name":"mapView"},{"content":[{"inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}],"type":"paragr