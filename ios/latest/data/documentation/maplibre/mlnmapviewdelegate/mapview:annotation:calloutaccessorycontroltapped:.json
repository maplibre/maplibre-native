t":"("},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"UIControl","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIControl"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}],"type":"paragraph"}],"name":"mapView"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}]}],"name":"annotation"},{"name":"control","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The control that was tapped."}]}]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"type":"text","text":" "},{"type":"text","text":"positioned on either side of the annotation title text. If an accessory view"},{"type":"text","text":" "},{"type":"text","text":"you specify is a descendant of the "},{"code":"UIControl","type":"codeVoice"},{"type":"text","text":" class, the map view calls this"},{"text":" ","type":"text"},{"text":"method as a convenience whenever the user taps your view. You can use this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"type":"text","text":" "},{"type":"text","text":"For example, if your control displays additional information about the"},{"text":" ","type":"text"},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"type":"text","text":" "},{"text":"information.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If your custom accessory views are not descendants of the "},{"type":"codeVoice","code":"UIControl"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the map view does not call this method. If the annotation has a custom callout"},{"type":"text","text":" "},{"type":"text","text":"view via the "},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"type":"text","text":" method, you can specify the"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true,"type":"reference"},{"type":"text","text":" protocol’s"},{"text":" ","type":"text"},{"type":"codeVoice","code":"leftAccessoryView"},{"text":" and ","type":"text"},{"type":"codeVoice","code":"rightAccessoryView"},{"type":"text","text":" properties."}]},{"level":4,"type":"heading","text":"Related examples","anchor":"Related-examples"},{"inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","kind":"symbol","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"kind":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","url":"\/documentation\/maplibre\/mlnmapviewdelegate","abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"type":"text","text":" "},{"text":"individual annotations.","type":"text"}],"type":"topic","role":"symbol","title":"MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"fragments":[{"kind":"identifier","text":"MLNCalloutView"}],"type":"topic","kind":"symbol","abstract":[{"type":"text","text":"A protocol for a "},{"type":"codeVoice","code":"UIView"},{"type":"text","text":" subclass that displays information about a selected"},{"type":"text","text":" "},{"text":"annotation near that annotation.","type":"text"}],"title":"MLNCalloutView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","url":"\/documentation\/maplibre\/mlncalloutview","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"text":"MLN