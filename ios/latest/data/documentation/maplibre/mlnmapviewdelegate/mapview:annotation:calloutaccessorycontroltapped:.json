{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"sections":[],"metadata":{"modules":[{"name":"MapLibre"}],"symbolKind":"method","role":"symbol","title":"mapView:annotation:calloutAccessoryControlTapped:","navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L982","fileName":"MLNMapViewDelegate.h"},"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:"},"primaryContentSections":[{"declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView","text":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"kind":"identifier","text":"annotation:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"id<MLNAnnotation>","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"kind":"identifier","text":"calloutAccessoryControlTapped:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"UIControl","preciseIdentifier":"c:objc(cs)UIControl"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}]}],"kind":"declarations"},{"parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}]}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}]}]},{"name":"control","content":[{"inlineContent":[{"type":"text","text":"The control that was tapped."}],"type":"paragraph"}]}],"kind":"parameters"},{"content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"type":"text","text":" "},{"type":"text","text":"positioned on either side of the annotation title text. If an accessory view"},{"type":"text","text":" "},{"type":"text","text":"you specify is a descendant of the "},{"type":"codeVoice","code":"UIControl"},{"type":"text","text":" class, the map view calls this"},{"type":"text","text":" "},{"type":"text","text":"method as a convenience whenever the user taps your view. You can use this"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"type":"text","text":" "},{"type":"text","text":"For example, if your control displays additional information about the"},{"type":"text","text":" "},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"type":"text","text":" "},{"type":"text","text":"information."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"If your custom accessory views are not descendants of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"text":"the map view does not call this method. If the annotation has a custom callout","type":"text"},{"text":" ","type":"text"},{"text":"view via the ","type":"text"},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"text":" method, you can specify the","type":"text"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView"},{"type":"text","text":" protocol’s"},{"type":"text","text":" "},{"type":"codeVoice","code":"leftAccessoryView"},{"type":"text","text":" and "},{"type":"codeVoice","code":"rightAccessoryView"},{"type":"text","text":" properties."}]},{"level":4,"text":"Related examples","type":"heading","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"type":"text","text":"standard callout bubble’s accessory control is tapped."}]}],"kind":"content"}],"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","kind":"symbol","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"title":"MLNMapViewDelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","role":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotation:calloutAccessoryControlTapped:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:","kind":"symbol","navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"title":"mapView:annotation:calloutAccessoryControlTapped:","abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"type":"topic","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"abstract":[{"text":"A protocol for a ","type":"text"},{"code":"UIView","type":"codeVoice"},{"text":" subclass that displays information about a selected","type":"text"},{"text":" ","type":"text"},{"text":"annotation near that annotation.","type":"text"}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlncalloutview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","title":"MLNCalloutView","kind":"symbol","fragments":[{"text":"MLNCalloutView","kind":"identifier"}]}}}