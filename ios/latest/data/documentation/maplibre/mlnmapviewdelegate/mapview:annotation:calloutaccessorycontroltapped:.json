{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","abstract":[{"text":"Tells the delegate that the user tapped one of the accessory controls in the","type":"text"},{"text":" ","type":"text"},{"text":"annotation’s callout view.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"]}],"metadata":{"role":"symbol","roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"symbolKind":"method","title":"mapView:annotation:calloutAccessoryControlTapped:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L982","fileName":"MLNMapViewDelegate.h"},"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:","modules":[{"name":"MapLibre"}]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"primaryContentSections":[{"declarations":[{"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"typeIdentifier","text":"MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"kind":"identifier","text":"annotation:"},{"kind":"text","text":"("},{"prec