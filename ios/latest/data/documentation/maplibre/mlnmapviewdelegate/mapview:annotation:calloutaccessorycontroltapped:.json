t":"("},{"text":"id<MLNAnnotation>","kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"UIControl","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIControl"},{"text":" *) ","kind":"text"},{"text":"control","kind":"internalParam"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}]}]},{"name":"annotation","content":[{"inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}],"type":"paragraph"}]},{"name":"control","content":[{"inlineContent":[{"type":"text","text":"The control that was tapped."}],"type":"paragraph"}]}],"kind":"parameters"},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"type":"text","text":" "},{"type":"text","text":"positioned on either side of the annotation title text. If an accessory view"},{"type":"text","text":" "},{"type":"text","text":"you specify is a descendant of the "},{"type":"codeVoice","code":"UIControl"},{"type":"text","text":" class, the map view calls this"},{"type":"text","text":" "},{"type":"text","text":"method as a convenience whenever the user taps your view. You can use this"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"text":" ","type":"text"},{"text":"For example, if your control displays additional information about the","type":"text"},{"text":" ","type":"text"},{"text":"annotation, you could use this method to present a modal panel with that","type":"text"},{"text":" ","type":"text"},{"text":"information.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"If your custom accessory views are not descendants of the ","type":"text"},{"type":"codeVoice","code":"UIControl"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"text":"the map view does not call this method. If the annotation has a custom callout","type":"text"},{"text":" ","type":"text"},{"text":"view via the ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"text":" method, you can specify the","type":"text"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView"},{"type":"text","text":" protocol’s"},{"type":"text","text":" "},{"type":"codeVoice","code":"leftAccessoryView"},{"type":"text","text":" and "},{"type":"codeVoice","code":"rightAccessoryView"},{"type":"text","text":" properties."}],"type":"paragraph"},{"text":"Related examples","anchor":"Related-examples","type":"heading","level":4},{"inlineContent":[{"text":"TODO: Default callout usage, learn how to trigger an action when the","type":"text"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","title":"MapLibre","role":"collection","kind":"symbol","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"title":"MLNMapView","kind":"symbol","type":"topic","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotation:calloutAccessoryControlTapped:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:","navigatorTitle":[{"kind":"identifier","text":"mapView:annotation:calloutAccessoryControlTapped:"}],"title":"mapView:annotation:calloutAccessoryControlTapped:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:annotation:calloutAccessoryControlTapped:"}],"kind":"symbol","abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"reference","isActive":true},{"text":" protocol defines a set of optional methods that you","type":"text"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"title":"MLNMapViewDelegate","url":"\/documentation\/maplibre\/mlnmapviewdelegate","role":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"title":"MLNCalloutView","abstract":[{"type":"text","text":"A protocol for a "},{"code":"UIView","type":"codeVoice"},{"type":"text","text":" subclass that displays information about a selected"},{"type":"text","text":" "},{"type":"text","text":"annotation near that annotation."}],"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","url":"\/documentation\/maplibre\/mlncalloutview","fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"role":"symbol"}}}