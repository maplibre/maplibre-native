{"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"text":") ","kind":"text"},{"text":"mapView:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"typeIdentifier","text":"MLNMapView"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"annotation:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"id<MLNAnnotation>","kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"kind":"text","text":" "},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"UIControl","preciseIdentifier":"c:objc(cs)UIControl"},{"kind":"text","text":" *) "},{"text":"control","kind":"internalParam"},{"text":";","kind":"text"}]}]},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}]}],"name":"mapView"},{"name":"annotation","content":[{"inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"The control that was tapped.","type":"text"}],"type":"paragraph"}],"name":"control"}]},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"text":"In a standard callout view, accessory views contain custom content and are","type":"text"},{"text":" ","type":"text"},{"type":"text","text":