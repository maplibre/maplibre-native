utViewForAnnotation:"},{"text":" method, you can specify the","type":"text"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView"},{"type":"text","text":" protocol’s"},{"type":"text","text":" "},{"type":"codeVoice","code":"leftAccessoryView"},{"type":"text","text":" and "},{"code":"rightAccessoryView","type":"codeVoice"},{"text":" properties.","type":"text"}]},{"type":"heading","level":4,"text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}]}]}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"]}],"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"sections":[],"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"abstract":[{"text":"A protocol for a ","type":"text"},{"type":"codeVoice","code":"UIView"},{"text":" subclass that displays information about a selected","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotation near that annotation."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","title":"MLNCalloutView","type":"topic","url":"\/documentation\/maplibre\/mlncalloutview","role":"symbol","fragments":[{"kind":"identifier","text":"MLNCalloutView"}],"kind":"sym