me":"annotation","content":[{"type":"paragraph","inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}]}]},{"name":"control","content":[{"type":"paragraph","inlineContent":[{"text":"The control that was tapped.","type":"text"}]}]}]},{"content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"In a standard callout view, accessory views contain custom content and are","type":"text"},{"text":" ","type":"text"},{"text":"positioned on either side of the annotation title text. If an accessory view","type":"text"},{"text":" ","type":"text"},{"text":"you specify is a descendant of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class, the map view calls this","type":"text"},{"text":" ","type":"text"},{"text":"method as a convenience whenever the user taps your view. You can use this","type":"text"},{"text":" ","type":"text"},{"text":"method to respond to taps and perform any actions associated with that control.","type":"text"},{"text":" ","type":"text"},{"text":"For example, if your control displays additional information about the","type":"text"},{"text":" ","type":"text"},{"text":"annotation, you could use this method to present a modal panel with that","type":"text"},{"text":" ","type":"text"},{"text":"information.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"If your custom accessory views are not descendants of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"text":"the map view does not call this method. If the annotation has a custom callout","type":"text"},{"text":" ","type":"text"},{"text":"view via the ","type":"text"},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"text":" method, you can specify the","type":"text"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView"},{"type":"text","text":" protocol’s"},{"type":"text","text":" "},{"type":"codeVoice","code":"leftAccessoryView"},{"type":"text","text":" and "},{"type":"codeVoice","code":"rightAccessoryView"},{"type":"text","text":" properties."}]},{"anchor":"Related-examples","level":4,"type":"heading","text":"Related examples"},{"type":"paragraph","inlineContent":[{"text":"TODO: Default callout usage, learn how to trigger an action when the","type":"text"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}]}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"abstract":[{"text":"Tells the delegate that the user tapped one of the accessory controls in the","type":"text"},{"text":" ","type":"text"},{"text":"annotation’s callout view.","type":"text"}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"schemaVersion":{"major":0,"patch":0,"minor":3},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"title":"MLNCalloutView","abstract":[{"text":"A protocol for a ","type":"text"},{"code":"UIView","type":"codeVoice"},{"text":" subclass that displays information about a selected","type":"text"},{"text":" ","type":"text"},{"text":"annotation near that annotation.","type":"text"}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlncalloutview","kind":"sym