{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"text":" ","type":"text"},{"type":"text","text":"annotation’s callout view."}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"],"traits":[{"interfaceLanguage":"occ"}]}],"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"text":") ","kind":"text"},{"text":"mapView:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)MLNMapView","text":"MLNMapView","kind":"typeIdentifier","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"annotation:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","kind":"typeIdentifier","text":"id<MLNAnnotation>"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"text":" ","kind":"text"},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"UIControl","preciseIdentifier":"c:objc(cs)UIControl"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"mapView","content":[{"inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}],"type":"paragraph"}]},{"name":"annotation","content":[{"inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}],"type":"paragraph"}]},{"name":"control","content":[{"type":"paragraph","inlineContent":[{"text":"The control that was tapped.","type":"text"}]}]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"inlineContent":[{"text":"In a standard callout view, accessory views contain custom content and are","type":"text"},{"text":" ","type":"text"},{"text":"positioned on either side of the annotation title text. If an accessory view","type":"text"},{"type":"text","text":" "},{"text":"you specify is a descendant of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class, the map view calls this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"method as a convenience whenever the user taps your view. You can use this"},{"text":" ","type":"text"},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"text":" ","type":"text"},{"type":"text","text":"For example, if your control displays additional information about the"},{"type":"text","text":" "},{"text":"annotation, you could use this method to present a modal panel with that","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"information."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If your custom accessory views are not descendants of the "},{"type":"codeVoice","code":"UIControl"},{"text":" class,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the map view does not call this method. If the annotation has a custom callout"},{"type":"text","text":" "},{"text":"view via the ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method, you can specify the"},{"text":" ","type":"text"},{"type":"text","text":"custom accessory views using the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true},{"type":"text","text":" protocol’s"},{"text":" ","type":"text"},{"type":"codeVoice","code":"leftAccessoryView"},{"type":"text","text":" and "},{"type":"codeVoice","code":"rightAccessoryView"},{"text":" properties.","type":"text"}],"type":"paragraph"},{"anchor":"Related-examples","level":4,"text":"Related examples","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"text":" ","type":"text"},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}]}]}],"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"metadata":{"modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"mapView:annotation:calloutAccessoryControlTapped:"}],"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:","title":"mapView:annotation:calloutAccessoryControlTapped:","role":"symbol","roleHeading":"Instance Method","symbolKind":"method","navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L964","fileName":"MLNMapViewDelegate.h"}},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotation:calloutAccessoryControlTapped:":{"title":"mapView:annotation:calloutAccessoryControlTapped:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:annotation:calloutAccessoryControlTapped:"}],"navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","abstract":[{"text":"Tells the delegate that the user tapped one of the accessory controls in the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotation’s callout view."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"reference","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true,"type":"reference"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"role":"symbol","title":"MLNMapViewDelegate","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"text":"MLNMapView","kind":"identifier"}],"type":"topic","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"abstract":[{"type":"text","text":"A protocol for a "},{"code":"UIView","type":"codeVoice"},{"type":"text","text":" subclass that displays information about a selected"},{"text":" ","type":"text"},{"type":"text","text":"annotation near that annotation."}],"role":"symbol","url":"\/documentation\/maplibre\/mlncalloutview","fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"title":"MLNCalloutView","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","kind":"symbol"}}}