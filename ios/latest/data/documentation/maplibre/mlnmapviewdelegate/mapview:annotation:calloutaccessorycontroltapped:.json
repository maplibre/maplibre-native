{"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"abstract":[{"text":"Tells the delegate that the user tapped one of the accessory controls in the","type":"text"},{"text":" ","type":"text"},{"text":"annotation’s callout view.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"text":") ","kind":"text"},{"kind":"identifier","text":"mapView:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","preciseIdentifier":"c:objc(cs)MLNMapView"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"text":"annotation:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"id<MLNAnnotation>","kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)UIControl","kind":"typeIdentifier","text":"UIControl"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"control"},{"text":";","kind":"text"}]}]},{"parameters":[{"name":"mapView","content":[{"inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}]}],"name":"annotation"},{"content":[{"type":"paragraph","inlineContent":[{"text":"The control that was tapped.","type":"text"}]}],"name":"control"}],"kind":"parameters"},{"content":[{"text":"Discussion","type":"h