pe":"text"},{"text":"view via the ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"text":" method, you can specify the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"custom accessory views using the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true,"type":"reference"},{"type":"text","text":" protocol’s"},{"type":"text","text":" "},{"code":"leftAccessoryView","type":"codeVoice"},{"text":" and ","type":"text"},{"type":"codeVoice","code":"rightAccessoryView"},{"type":"text","text":" properties."}],"type":"paragraph"},{"type":"heading","text":"Related examples","level":4,"anchor":"Related-examples"},{"inlineContent":[{"text":"TODO: Default callout usage, learn how to trigger an action when the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"standard callout bubble’s accessory control is tapped."}],"type":"paragraph"}],"kind":"content"}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"reference"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"t