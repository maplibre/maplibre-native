{"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"text":") ","kind":"text"},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"MLNMapView","preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"mapView"},{"text":" ","kind":"text"},{"kind":"identifier","text":"annotation:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"text":" ","kind":"text"},{"text":"calloutAccessoryControlTapped:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"UIControl","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIControl"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"control"},{"kind":"text","text":";"}],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}]}],"name":"mapView"},{"content":[{"inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}],"type":"paragraph"}],"name":"annotation"},{"name":"control","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The control that was tapped."}]}]}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","anchor":"discussion","text":"Discussion","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"type":"text