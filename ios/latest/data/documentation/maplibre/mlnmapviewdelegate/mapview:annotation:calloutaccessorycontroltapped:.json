pe":"text"},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"type":"text","text":" method, you can specify the"},{"type":"text","text":" "},{"text":"custom accessory views using the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true,"type":"reference"},{"type":"text","text":" protocol’s"},{"type":"text","text":" "},{"type":"codeVoice","code":"leftAccessoryView"},{"text":" and ","type":"text"},{"code":"rightAccessoryView","type":"codeVoice"},{"text":" properties.","type":"text"}]},{"level":4,"type":"heading","text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"text":"standard callout bubble’s accessory control is tapped.","type":"text"}]}]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"],"traits":[{"interfaceLanguage":"occ"}]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotation:calloutAccessoryControlTapped:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:","navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"type":"topic","abstract":[{"text":"Tells the delegate that the user tapped one of the accessory contro