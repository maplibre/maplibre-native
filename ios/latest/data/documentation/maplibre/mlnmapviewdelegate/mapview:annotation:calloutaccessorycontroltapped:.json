nd":"parameters","parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}]}]},{"name":"annotation","content":[{"inlineContent":[{"text":"The annotation whose accessory view was tapped.","type":"text"}],"type":"paragraph"}]},{"name":"control","content":[{"inlineContent":[{"text":"The control that was tapped.","type":"text"}],"type":"paragraph"}]}]},{"content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"type":"text","text":" "},{"type":"text","text":"positioned on either side of the annotation title text. If an accessory view"},{"type":"text","text":" "},{"type":"text","text":"you specify is a descendant of the "},{"type":"codeVoice","code":"UIControl"},{"type":"text","text":" class, the map view calls this"},{"type":"text","text":" "},{"type":"text","text":"method as a convenience whenever the user taps your view. You can use this"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"type":"text","text":" "},{"type":"text","text":"For example, if your control displays additional information about the"},{"type":"text","text":" "},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"type":"text","text":" "},{"type":"text","text":"information."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If your custom accessory views are not descendants of the "},{"type":"codeVoice","code":"UIControl"},{"type":"text","text":" class,"},{"type":"text","text":" "},{"type":"text","text":"the map view does not call this method. If the annotation has a custom callout"},{"type":"text","text":" "},{"type":"text","text":"view via the "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method, you can specify the"},{"type":"text","text":" "},{"type":"text","text":"custom accessory views using the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true,"type":"reference"},{"text":" protocol’s","type":"text"},{"text":" ","type":"text"},{"code":"leftAccessoryView","type":"codeVoice"},{"text":" and ","type":"text"},{"code":"rightAccessoryView","type":"codeVoice"},{"text":" properties.","type":"text"}],"type":"paragraph"},{"type":"heading","level":4,"anchor":"Related-examples","text":"Related examples"},{"inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"type":"text","text":"standard callout bubble’s accessory control is tapped."}],"type":"paragraph"}],"kind":"content"}],"schemaVersion":{"patch":0,"major":0,"minor":3},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","role":"symbol","type":"topic","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNMapView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotation:calloutAccessoryControlTapped:":{"type":"topic","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"kind":"symbol","title":"mapView:annotation:calloutAccessoryControlTapped:","abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"title":"MLNMapViewDelegate","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","abstract":[{"text":"A protocol for a ","type":"text"},{"code":"UIView","type":"codeVoice"},{"text":" subclass that displays information about a selected","type":"text"},{"text":" ","type":"text"},{"text":"annotation near that annotation.","type":"text"}],"kind":"symbol","title":"MLNCalloutView","url":"\/documentation\/maplibre\/mlncalloutview","fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"type":"topic","role":"symbol"}}}