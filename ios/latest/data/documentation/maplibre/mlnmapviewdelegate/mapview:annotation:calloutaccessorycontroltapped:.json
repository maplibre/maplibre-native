":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"title":"MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"title":"MLNCalloutView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","kind":"symbol","fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"abstract":[{"type":"text","text":"A protocol for a "},{"type":"codeVoice","code":"UIView"},{"type":"text","text":" subclass that displays information about a selected"},{"type":"text","text":" "},{"type":"text","text":"annotation near that annotation."}],"url":"\/documentation\/maplibre\/mlncalloutview","type":"topic","role":"symbol"}}}