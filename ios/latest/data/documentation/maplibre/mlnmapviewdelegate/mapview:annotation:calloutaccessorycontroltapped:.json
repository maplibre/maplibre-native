{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotation:calloutaccessorycontroltapped:"]}],"sections":[],"metadata":{"roleHeading":"Instance Method","role":"symbol","symbolKind":"method","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotation:calloutAccessoryControlTapped:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L964","fileName":"MLNMapViewDelegate.h"},"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"mapView:annotation:calloutAccessoryControlTapped:"}],"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"mapView:annotation:calloutAccessoryControlTapped:","kind":"identifier"}],"title":"mapView:annotation:calloutAccessoryControlTapped:"},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"kind":"text","text":") "},{"text":"mapView:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"MLNMapView","preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"annotation:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>","kind":"typeIdentifier"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":" "},{"kind":"identifier","text":"calloutAccessoryControlTapped:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"UIControl","preciseIdentifier":"c:objc(cs)UIControl"},{"text":" *) ","kind":"text"},{"text":"control","kind":"internalParam"},{"kind":"text","text":";"}]}]},{"parameters":[{"name":"mapView","content":[{"inlineContent":[{"type":"text","text":"The map view containing the specified annotation."}],"type":"paragraph"}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}]}]},{"name":"control","content":[{"inlineContent":[{"type":"text","text":"The control that was tapped."}],"type":"paragraph"}]}],"kind":"parameters"},{"content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"In a standard callout view, accessory views contain custom content and are"},{"text":" ","type":"text"},{"text":"positioned on either side of the annotation title text. If an accessory view","type":"text"},{"type":"text","text":" "},{"text":"you specify is a descendant of the ","type":"text"},{"type":"codeVoice","code":"UIControl"},{"type":"text","text":" class, the map view calls this"},{"text":" ","type":"text"},{"type":"text","text":"method as a convenience whenever the user taps your view. You can use this"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"type":"text","text":" "},{"type":"text","text":"For example, if your control displays additional information about the"},{"type":"text","text":" "},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"type":"text","text":" "},{"type":"text","text":"information."}]},{"type":"paragraph","inlineContent":[{"text":"If your custom accessory views are not descendants of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the map view does not call this method. If the annotation has a custom callout"},{"type":"text","text":" "},{"type":"text","text":"view via the "},{"code":"-mapView:calloutViewForAnnotation:","type":"codeVoice"},{"type":"text","text":" method, you can specify the"},{"text":" ","type":"text"},{"text":"custom accessory views using the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","isActive":true,"type":"reference"},{"type":"text","text":" protocol’s"},{"text":" ","type":"text"},{"type":"codeVoice","code":"leftAccessoryView"},{"type":"text","text":" and "},{"code":"rightAccessoryView","type":"codeVoice"},{"type":"text","text":" properties."}]},{"level":4,"type":"heading","text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"type":"text","text":"standard callout bubble’s accessory control is tapped."}]}],"kind":"content"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotation:calloutAccessoryControlTapped:","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Tells the delegate that the user tapped one of the accessory controls in the"},{"type":"text","text":" "},{"type":"text","text":"annotation’s callout view."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"url":"\/documentation\/maplibre\/mlncalloutview","kind":"symbol","title":"MLNCalloutView","role":"symbol","fragments":[{"kind":"identifier","text":"MLNCalloutView"}],"abstract":[{"type":"text","text":"A protocol for a "},{"type":"codeVoice","code":"UIView"},{"text":" subclass that displays information about a selected","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotation near that annotation."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","type":"to