{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:"]}],"metadata":{"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:viewForAnnotation:","title":"mapView:viewForAnnotation:","symbolKind":"method","navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"roleHeading":"Instance Method","remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L742"},"modules":[{"name":"MapLibre"}]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","text":"MLNAnnotationView","preciseIdentifier":"c:objc(cs)MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" *) ","kind":"text"},{"text":"mapView:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","text":"MLNMapView","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"viewForAnnotation:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"id<MLNAnnotation>","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view that requested the annotation view."}]}]},{"name":"annotation","content":[{"inlineContent":[{"text":"The object representing the annotation that is about to be","type":"text"},{"type":"text","text":" "},{"type":"text","text":"displayed."}],"type":"paragraph"}]}]},{"kind":"content","content":[{"anchor":"return-value","text":"Return Value","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"The view object to display for the given annotation or ","type":"text"},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if you"},{"type":"text","text":" "},{"text":"want to display an annotation image instead.","type":"text"}]}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","level":2,"type":"heading"},{"inlineContent":[{"type":"text","text":"Implement this method to mark a point annotation with a view object. If you"},{"text":" ","type":"text"},{"text":"want to mark a particular point annotation with a static image instead, omit","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"this method or have it return "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" for that annotation, then implement"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:imageForAnnotation:"},{"type":"text","text":" instead."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa"},{"type":"text","text":" "},{"type":"text","text":"Touch frameworks. On the other hand, static annotation images use less memory"},{"text":" ","type":"text"},{"type":"text","text":"and draw more quickly than annotation views."}],"type":"paragraph"},{"inlineContent":[{"text":"The user location annotation view can also be customized via this method. When","type":"text"},{"type":"text","text":" "},{"code":"annotation","type":"codeVoice"},{"text":" is an instance of ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference"},{"text":" (or equal to the map view’s","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"userLocation"},