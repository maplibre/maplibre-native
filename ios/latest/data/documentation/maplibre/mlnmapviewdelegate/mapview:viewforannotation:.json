kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"name":"mapView","content":[{"inlineContent":[{"type":"text","text":"The map view that requested the annotation view."}],"type":"paragraph"}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The object representing the annotation that is about to be"},{"type":"text","text":" "},{"type":"text","text":"displayed."}]}]}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The view object to display for the given annotation or "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" if you"},{"type":"text","text":" "},{"type":"text","text":"want to display an annotation image instead."}]}],"kind":"content"},{"content":[{"text":"Discussion","anchor":"discussion","type":"heading","level":2},{"inlineContent":[{"type":"text","text":"Implement this method to mark a point annotation with a view object. If you"},{"type":"text","text":" "},{"type":"text","text":"want to mark a particular point annotation with a static image instead, omit"},{"type":"text","text":" "},{"type":"text","text":"this method or have it return "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" for that annotation, then implement"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:imageForAnnotation:"},{"type":"text","text":" instead."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa"},{"type":"text","text":" "},{"type":"text","text":"Touch frameworks. On the other hand, static annotation images use less memory"},{"type":"text","text":" "},{"type":"text","text":"and draw more quickly than annotation views."}],"type":"paragraph"},{"inlineContent":[{"text":"The user location annotation view can also be customized via this method. When","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"annotation"},{"text":" is an instance of ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"text":" (or equal to the map view’s","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"userLocation"},{"text":" property), return an instance of ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView"},{"text":" ","type":"text"},{"text":"(or a subclass thereof).","type":"text"}],"type":"paragraph"},{"text":"Related examples","anchor":"Related-examples","type":"heading","level":4},{"inlineContent":[{"text":"TODO: Add annotation views and images, learn how to specify what","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" to use for a given ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" object on your","type":"text"},{"text":" ","type":"text"},{"text":"map.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","type":"topic","title":"mapView:viewForAnnotation:","abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"fragments":[{"kind":"identifier","text":"MLNUserLocation"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnuserlocation","title":"MLNUserLocation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"topic","abstract":[{"text":"The MLNUserLocation class defines a specific type of annotation that identifies","type":"text"},{"text":" ","type":"text"},{"text":"the user’s current location. You do not create instances of this class","type":"text"},{"text":" ","type":"text"},{"text":"directly. Instead, you retrieve an existing ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference"},{"text":" object from the","type":"text"},{"text":" ","type":"text"},{"code":"userLocation","type":"codeVoice"},{"text":" property of the map view displayed in your application.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"title":"MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","title":"MLNMapViewDelegate","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","role":"collection","title":"MapLibre","type":"topic","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"type":"topic","url":"\/documentation\/maplibre\/mlnuserlocationannotationview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","title":"MLNUserLocationAnnotationView","role":"symbol","abstract":[{"type":"text","text":"View representing an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"type":"text","text":" on screen."}],"fragments":[{"kind":"identifier","text":"MLNUserLocationAnnotationView"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","role":"symbol","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView"}}}