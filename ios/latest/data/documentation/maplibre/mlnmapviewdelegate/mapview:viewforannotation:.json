{"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:"]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:"},"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}],"metadata":{"title":"mapView:viewForAnnotation:","role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L742"},"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"symbolKind":"method","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:viewForAnnotation:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"text":"MLNAnnotationView","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" *) ","kind":"text"},{"text":"mapView:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"MLNMapView","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"viewForAnnotation:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseId