urn Value"},{"type":"paragraph","inlineContent":[{"text":"The view object to display for the given annotation or ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if you","type":"text"},{"text":" ","type":"text"},{"text":"want to display an annotation image instead.","type":"text"}]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"inlineContent":[{"type":"text","text":"Implement this method to mark a point annotation with a view object. If you"},{"type":"text","text":" "},{"type":"text","text":"want to mark a particular point annotation with a static image instead, omit"},{"type":"text","text":" "},{"type":"text","text":"this method or have it return "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" for that annotation, then implement"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:imageForAnnotation:"},{"type":"text","text":" instead."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa"},{"type":"text","text":" "},{"type":"text","text":"Touch frameworks. On the other hand, static annotation images use less memory"},{"type":"text","text":" "},{"type":"text","text":"and draw more quickly than annotation views."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The user location annotation view can also be customized via this method. When"},{"type":"text","text":" "},{"type":"codeVoice","code":"annotation"},{"type":"text","text":" is an instance of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"type":"text","text":" (or equal to the map view’s"},{"type":"text","text":" "},{"type":"codeVoice","code":"userLocation"},{"type":"text","text":" property), return an instance of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","isActive":true},{"type":"text","text":" "},{"type":"text","text":"(or a subclass thereof)."}],"type":"paragraph"},{"anchor":"Related-examples","level":4,"type":"heading","text":"Related examples"},{"inlineContent":[{"type":"text","text":"TODO: Add annotation views and images, learn how to specify what"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" to use for a given "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" object on your"},{"type":"text","text":" "},{"type":"text","text":"map."}],"type":"paragraph"}]}],"abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}],"metadata":{"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:viewForAnnotation:","role":"symbol","symbolKind":"method","roleHeading":"Instance Method","remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L742"},"navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"title":"mapView:viewForAnnotation:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"title":"mapView:viewForAnnotation:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","role":"symbol","type":"topi