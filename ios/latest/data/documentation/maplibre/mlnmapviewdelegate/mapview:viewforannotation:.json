onView":{"type":"topic","role":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol","title":"MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","role":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","role":"collection","kind":"symbol","title":"MapLibre","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"title":"mapView:viewForAnnotation:","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"kind":"symbol","title":"MLNMapViewDelegate","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"text","text":" "},{"text":"require the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"type":"text","text":" "},{"text":"individual annotations.","type":"text"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"fragments":[{"kind":"identifier","text":"MLNUserLocation"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","title":"MLNUserLocation","url":"\/documentation\/maplibre\/mlnuserlocation","abstract":[{"text":"The MLNUserLocation class defines a specific type of annotation that identifies","type":"text"},{"type":"text","text":" "},{"text":"the user’s current location. You do not create instances of this class","type":"text"},{"type":"text","text":" "},{"text":"directly. Instead, you retrieve an existing ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference","isActive":true},{"type":"text","text":" object from the"},{"type":"text","text":" "},{"code":"userLocation","type":"codeVoice"},{"type":"text","text":" property of the map view displayed in your application."}],"kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"type":"topic","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","fragments":[{"text":"MLNUserLocationAnnotationView","kind":"identifier"}],"abstract":[{"type":"text","text":"View representing an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true,"type":"reference"},{"type":"text","text":" on screen."}],"title":"MLNUserLocationAnnotationView","url":"\/documentation\/maplibre\/mlnuserlocationannotationview"}}}