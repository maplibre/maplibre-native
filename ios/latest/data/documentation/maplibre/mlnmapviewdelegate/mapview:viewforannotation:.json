ters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view that requested the annotation view."}]}],"name":"mapView"},{"content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation that is about to be"},{"type":"text","text":" "},{"type":"text","text":"displayed."}],"type":"paragraph"}],"name":"annotation"}],"kind":"parameters"},{"content":[{"type":"heading","level":2,"text":"Return Value","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"text":"The view object to display for the given annotation or ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" if you","type":"text"},{"text":" ","type":"text"},{"text":"want to display an annotation image instead.","type":"text"}]}],"kind":"content"},{"content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Implement this method to mark a point annotation with a view object. If you","type":"text"},{"text":" ","type":"text"},{"text":"want to mark a particular point annotation with a static image instead, omit","type":"text"},{"text":" ","type":"text"},{"text":"this method or have it return ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" for that annotation, then implement","type":"text"},{"text":" ","type":"text"},{"code":"-mapView:imageForAnnotation:","type":"codeVoice"},{"text":" instead.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa","type":"text"},{"text":" ","type":"text"},{"text":"Touch frameworks. On the other hand, static annotation images use less memory","type":"text"},{"text":" ","type":"text"},{"text":"and draw more quickly than annotation views.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"The user location annotation view can also be customized via this method. When","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"annotation"},{"text":" is an instance of ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"text":" (or equal to the map view’s","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"userLocation"},{"text":" property), return an instance of ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","isActive":true},{"text":" ","type":"text"},{"text":"(or a subclass thereof).","type":"text"}]},{"level":4,"type":"heading","text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"text":"TODO: Add annotation views and images, learn how to specify what","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" to use for a given ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" object on your","type":"text"},{"text":" ","type":"text"},{"text":"map.","type":"text"}]}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","interfaceLanguage":"occ"},"schemaVersion":{"minor":3,"patch":0,"major":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","title":"MLNAnnotationView","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","type":"topic","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"reference","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"type":"topic","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"kind":"symbol","title":"MLNMapViewDelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","url":"\/documentation\/maplibre\/mlnmapviewdelegate","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview","fragments":[{"kind":"identifier","text":"MLNMapView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"role":"symbol","title":"MLNMapView","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"title":"MLNAnnotation","type":"topic","url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","url":"\/documentation\/maplibre\/mlnuserlocationannotationview","abstract":[{"text":"View representing an ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"text":" on screen.","type":"text"}],"title":"MLNUserLocationAnnotationView","role":"symbol","kind":"symbol","fragments":[{"text":"MLNUserLocationAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"title":"mapView:viewForAnnotation:","kind":"symbol","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"abstract":[{"type":"text","text":"The MLNUserLocation class defines a specific type of annotation that identifies"},{"type":"text","text":" "},{"text":"the user’s current location. You do not create instances of this class","type":"text"},{"text":" ","type":"text"},{"text":"directly. Instead, you retrieve an existing ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference","isActive":true},{"text":" object from the","type":"text"},{"text":" ","type":"text"},{"code":"userLocation","type":"codeVoice"},{"text":" property of the map view displayed in your application.","type":"text"}],"url":"\/documentation\/maplibre\/mlnuserlocation","type":"topic","fragments":[{"text":"MLNUserLocation","kind":"identifier"}],"kind":"symbol","title":"MLNUserLocation","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"}}}