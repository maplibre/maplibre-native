ame":"mapView","content":[{"type":"paragraph","inlineContent":[{"text":"The map view that requested the annotation view.","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation that is about to be"},{"type":"text","text":" "},{"type":"text","text":"displayed."}],"type":"paragraph"}],"name":"annotation"}]},{"content":[{"text":"Return Value","level":2,"anchor":"return-value","type":"heading"},{"inlineContent":[{"text":"The view object to display for the given annotation or ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" if you","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"want to display an annotation image instead."}],"type":"paragraph"}],"kind":"content"},{"content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Implement this method to mark a point annotation with a view object. If you"},{"text":" ","type":"text"},{"text":"want to mark a particular point annotation with a static image instead, omit","type":"text"},{"text":" ","type":"text"},{"text":"this method or have it return ","type":"text"},{"code":"nil","type":"codeVoice"},{"text":" for that annotation, then implement","type":"text"},{"type":"text","text":" "},{"code":"-mapView:imageForAnnotation:","type":"codeVoice"},{"type":"text","text":" instead."}]},{"inlineContent":[{"type":"text","text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa"},{"type":"text","text":" "},{"text":"Touch frameworks. On the other hand, static annotation images use less memory","type":"text"},{"type":"text","text":" "},{"type":"text","text":"and draw more quickly than annotation views."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"The user location annotation view can also be customized via this method. When","type":"text"},{"text":" ","type":"text"},{"code":"annotation","type":"codeVoice"},{"type":"text","text":" is an instance of "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference"},{"text":" (or equal to the map view’s","type":"text"},{"text":" ","type":"text"},{"code":"userLocation","type":"codeVoice"},{"type":"text","text":" property), return an instance of "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","type":"reference"},{"type":"text","text":" "},{"type":"text","text":"(or a subclass thereof)."}]},{"level":4,"text":"Related examples","type":"heading","anchor":"Related-examples"},{"inlineContent":[{"text":"TODO: Add annotation views and images, learn how to specify what","type":"text"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" to use for a given "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" object on your"},{"type":"text","text":" "},{"text":"map.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","interfaceLanguage":"occ"},"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"title":"MLNMapViewDelegate","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate","role":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"type":"text","text":" "},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"individual annotations."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"role":"symbol","kind":"symbol","fragments":[{"text":"MLNUserLocationAnnotationView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","type":"topic","url":"\/documentation\/maplibre\/mlnuserlocationannotationview","abstract":[{"text":"View representing an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference","isActive":true},{"type":"text","text":" on screen."}],"title":"MLNUserLocationAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"title":"MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"role":"symbol","abstract":[{"type":"text","text":"The MLNUserLocation class defines a specific type of annotation that identifies"},{"text":" ","type":"text"},{"text":"the user’s current location. You do not create instances of this class","type":"text"},{"type":"text","text":" "},{"type":"text","text":"directly. Instead, you retrieve an existing "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"type":"text","text":" object from the"},{"type":"text","text":" "},{"type":"codeVoice","code":"userLocation"},{"type":"text","text":" property of the map view displayed in your application."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","title":"MLNUserLocation","fragments":[{"text":"MLNUserLocation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnuserlocation","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"kind":"symbol","abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"type":"topic","role":"symbol","title":"mapView:viewForAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","role":"collection","url":"\/documentation\/maplibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView","kind":"symbol","type":"topic","fragments":[{"text":"MLNMapView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol"}}}