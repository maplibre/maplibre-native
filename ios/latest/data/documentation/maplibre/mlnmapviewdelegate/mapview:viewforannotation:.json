tent":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"text":"Implement this method to mark a point annotation with a view object. If you","type":"text"},{"text":" ","type":"text"},{"text":"want to mark a particular point annotation with a static image instead, omit","type":"text"},{"text":" ","type":"text"},{"text":"this method or have it return ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":" for that annotation, then implement","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:imageForAnnotation:"},{"text":" instead.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"Annotation views are compatible with UIKit, Core Animation, and other Cocoa","type":"text"},{"text":" ","type":"text"},{"text":"Touch frameworks. On the other hand, static annotation images use less memory","type":"text"},{"text":" ","type":"text"},{"text":"and draw more quickly than annotation views.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The user location annotation view can also be customized via this method. When"},{"type":"text","text":" "},{"code":"annotation","type":"codeVoice"},{"type":"text","text":" is an instance of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"type":"text","text":" (or equal to the map view’s"},{"type":"text","text":" "},{"code":"userLocation","type":"codeVoice"},{"type":"text","text":" property), return an instance of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","isActive":true},{"type":"text","text":" "},{"type":"text","text":"(or a subclass thereof)."}],"type":"paragraph"},{"text":"Related examples","level":4,"anchor":"Related-examples","type":"heading"},{"inlineContent":[{"type":"text","text":"TODO: Add annotation views and images, learn how to specify what"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" to use for a given "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" object on your"},{"type":"text","text":" "},{"type":"text","text":"map."}],"type":"paragraph"}]}],"sections":[],"metadata":{"roleHeading":"Instance Method","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L742","fileName":"MLNMapViewDelegate.h"},"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:viewForAnnotation:","symbolKind":"method","title":"mapView:viewForAnnotation:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:viewForAnnotation:","kind":"identifier"}]},"abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","interfaceLanguage":"occ"},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"abstract":[{"type":"text","text":"Returns a view object to mark the given point annotation object on the map."}],"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","role":"symbol","kind":"symbol","title":"mapView:viewForAnnotation:","navigatorTitle":[{"text":"mapView:viewForAnnotation:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"abstract":[{"type":"text","text":"View representing an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"reference","isActive":true},{"type":"text","text":" on screen."}],"title":"MLNUserLocationAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","kind":"symbol","role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNUserLocationAnnotationView"}],"url":"\/documentation\/maplibre\/mlnuserlocationannotationview"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","role":"collection","title":"MapLibre","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"fragments":[{"text":"MLNUserLocation","kind":"identifier"}],"title":"MLNUserLocation","url":"\/documentation\/maplibre\/mlnuserlocation","role":"symbol","abstract":[{"type":"text","text":"The MLNUserLocation class defines a specific type of annotation that identifies"},{"type":"text","text":" "},{"type":"text","text":"the user’s current location. You do not create instances of this class"},{"type":"text","text":" "},{"type":"text","text":"directly. Instead, you retrieve an existing "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"type":"text","text":" object from the"},{"type":"text","text":" "},{"code":"userLocation","type":"codeVoice"},{"type":"text","text":" property of the map view displayed in your application."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","title":"MLNMapViewDelegate","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","type":"topic","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"title":"MLNAnnotationView","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}]}}}