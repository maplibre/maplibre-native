require the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"text":" ","type":"text"},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"type":"text","text":" "},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"individual annotations."}],"fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate","type":"topic","kind":"symbol","title":"MLNMapViewDelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"type":"topic","title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","abstract":[{"type":"text","text":"Returns a Boolean value indicating whether the annotation is able to display"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."