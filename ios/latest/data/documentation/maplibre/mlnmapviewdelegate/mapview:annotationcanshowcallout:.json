arameters","parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"text":"The map view that has selected the annotation.","type":"text"}]}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"text":"The object representing the annotation.","type":"text"}]}]}]},{"kind":"content","content":[{"text":"Return Value","level":2,"type":"heading","anchor":"return-value"},{"inlineContent":[{"text":"A Boolean value indicating whether the annotation should show a","type":"text"},{"text":" ","type":"text"},{"text":"callout.","type":"text"}],"type":"paragraph"}]},{"kind":"content","content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This method is called after an annotation is selected, before any callout is"},{"type":"text","text":" "},{"type":"text","text":"displayed for the annotation."}]},{"type":"paragraph","inlineContent":[{"text":"If the return value is ","type":"text"},{"type":"codeVoice","code":"YES"},{"text":", a callout view is shown when the user taps on an","type":"text"},{"text":" ","type":"text"},{"text":"annotation, selecting it. The default callout displays the annotation’s title","type":"text"},{"text":" ","type":"text"},{"text":"and subtitle. You can add accessory views to either end of the callout by","type":"text"},{"text":" ","type":"text"},{"text":"implementing the ","type":"text"},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"text":" and","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:rightCalloutAccessoryViewForAnnotation:"},{"text":" methods. You can further","type":"text"},{"text":" ","type":"text"},{"text":"customize the callout’s contents by implementing the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"text":" method.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"If the return value is ","type":"text"},{"code":"NO","type":"codeVoice"},{"text":", or if this method is absent from the delegate, or","type":"text"},{"text":" ","type":"text"},{"text":"if the annotation lacks a title, the annotation will not show a callout even","type":"text"},{"text":" ","type":"text"},{"text":"when selected.","type":"text"}]},{"type":"heading","level":4,"text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Add annotation views and images"},{"type":"text","text":" "},{"type":"text","text":"TODO: Display custom views as callouts"},{"type":"text","text":" "},{"type":"text","text":"TODO: Default callout usage, learn how to show callouts for"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" objects."}]}]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","interfaceLanguage":"occ"},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","kind":"symbol","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","role":"symbol","navigatorTitle":[{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"text":" ","type":"text"},{"text":"extra information in a callout bubble.","type":"text"}],"type":"topic","kind":"symbol","title":"mapView:annotationCanShowCallout:","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"kind":"symbol","type":"topic","role":"symbol","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview","fragments":[{"text":"MLNMapView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"title":"MLNMapViewDelegate","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true,"type":"reference"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true,"type":"reference"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","kind":"symbol","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"}}}