{"schemaVersion":{"minor":3,"patch":0,"major":0},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:"]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"text":" ","type":"text"},{"text":"extra information in a callout bubble.","type":"text"}],"primaryContentSections":[{"declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL","text":"BOOL"},{"kind":"text","text":") "},{"kind":"identifier","text":"mapView:"},{"kind":"text","text":"("},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView","text":"MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"kind":"text","text":" "},{"kind":"identifier","text":"annotationCanShowCallout:"},{"kind":"text","text":"("},{"text":"id<MLNAnnotation>","kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"text":";","kind":"text"}]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The map view that has selected the annotation.","type":"text"}]}],"name":"mapView"},{"content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation."}],"type":"paragraph"}],"name":"annotation"}],"kind":"parameters"},{"content":[{"level":2,"type":"heading","text":"Return Value","anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A Boolean value indicating whether the annotation should show a"},{"type":"text","text":" "},{"type":"text","text":"callout."}]}],"kind":"content"},{"content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"This method is called after an annotation is selected, before any callout is"},{"type":"text","text":" "},{"type":"text","text":"displayed for the annotation."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"YES"},{"type":"text","text":", a callout view is shown when the user taps on an"},{"type":"text","text":" "},{"type":"text","text":"annotation, selecting it. The default callout displays the annotation’s title"},{"type":"text","text":" "},{"type":"text","text":"and subtitle. You can add accessory views to either end of the callout by"},{"type":"text","text":" "},{"type":"text","text":"implementing the "},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" and"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:rightCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" methods. You can further"},{"type":"text","text":" "},{"type":"text","text":"customize the callout’s contents by implementing the"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"type":"text","text":" "},{"type":"text","text":"if the annotation lacks a title, the annotation will not show a callout even"},{"type":"text","text":" "},{"type":"text","text":"when selected."}],"type":"paragraph"},{"level":4,"text":"Related examples","anchor":"Related-examples","type":"heading"},{"inlineContent":[{"text":"TODO: Add annotation views and images","type":"text"},{"text":" ","type":"text"},{"text":"TODO: Display custom views as callouts","type":"text"},{"text":" ","type":"text"},{"text":"TODO: Default callout usage, learn how to show callouts for","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" objects.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"metadata":{"symbolKind":"method","roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"title":"mapView:annotationCanShowCallout:","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotationCanShowCallout:","modules":[{"name":"MapLibre"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L869","fileName":"MLNMapViewDelegate.h"}},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","kind":"symbol","abstract":[{"type":"text","text":"Returns a Boolean value indicating whether the annotation is able to display"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","title":"mapView:annotationCanShowCallout:","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"navigatorTitle":[{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"title":"MLNMapViewDelegate","kind":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true,"type":"reference"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true,"type":"reference"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","url":"\/documentation\/maplibre","title":"MapLibre"}}}