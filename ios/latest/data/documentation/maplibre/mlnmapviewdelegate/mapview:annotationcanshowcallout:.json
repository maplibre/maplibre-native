"paragraph","inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"YES"},{"text":", a callout view is shown when the user taps on an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotation, selecting it. The default callout displays the annotation’s title"},{"text":" ","type":"text"},{"text":"and subtitle. You can add accessory views to either end of the callout by","type":"text"},{"type":"text","text":" "},{"type":"text","text":"implementing the "},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" and"},{"text":" ","type":"text"},{"code":"-mapView:rightCalloutAccessoryViewForAnnotation:","type":"codeVoice"},{"text":" methods. You can further","type":"text"},{"type":"text","text":" "},{"type":"text","text":"customize the callout’s contents by implementing the"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}]},{"inlineContent":[{"text":"If the return value is ","type":"text"},{"type":"codeVoice","code":"NO"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"type":"text","text":" "},{"text":"if the annotation lacks a title, the annotation will not show a callout even","type":"text"},{"text":" ","type":"text"},{"text":"when selected.","type":"text"}],"type":"paragraph"},{"level":4,"anchor":"Related-examples","type":"heading","text":"Related examples"},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Add annotation views and images"},{"text":" ","type":"text"},{"type":"text","text":"TODO: Display custom views as callouts"},{"type":"text","text":" "},{"type":"text","text":"TODO: Default callout usage, learn how to show callouts for"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" objects.","type":"text"}]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:"]}],"abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."}],"kind":"symbol","metadata":{"remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L851"},"roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"title":"mapView:annotationCanShowCallout:","role":"symbol","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotationCanShowCallout:","symbolKind":"method","fragments":[{"kind":"text","text":"- "},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:"},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","url":"\/documentation\/maplibre","title":"MapLibre","type":"topic","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"type":"topic","abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","title":"mapView:annotationCanShowCallout:","role":"symbol","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"topic","role":"symbol","kind":"symbol","title":"MLNMapViewDelegate","url":"\/documentation\/maplibre\/mlnmapviewdelegate","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"reference","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"text":" ","type":"text"},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"text":" ","type":"text"},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"text":" ","type":"text"},{"type":"text","text":"individual annotations."}]}}}