iants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:"]}],"abstract":[{"type":"text","text":"Returns a Boolean value indicating whether the annotation is able to display"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","interfaceLanguage":"occ"},"sections":[],"metadata":{"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotationCanShowCallout:","role":"symbol","title":"mapView:annotationCanShowCallout:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L869","fileName":"MLNMapViewDelegate.h"},"symbolKind":"method","roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}]},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"kind":"identifier","text":"MLNMapView"}],"title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"type":"topic","title":"mapView:annotationCanShowCallout:","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcal