ation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:"},"metadata":{"symbolKind":"method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L869","fileName":"MLNMapViewDelegate.h"},"roleHeading":"Instance Method","title":"mapView:annotationCanShowCallout:","navigatorTitle":[{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"role":"symbol","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotationCanShowCallout:"},"sections":[],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"type":"text","text":" "},{"text":"extra information in a callout bubble.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate","type":"topic","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\