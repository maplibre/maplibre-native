neContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"YES"},{"type":"text","text":", a callout view is shown when the user taps on an"},{"type":"text","text":" "},{"type":"text","text":"annotation, selecting it. The default callout displays the annotation’s title"},{"type":"text","text":" "},{"type":"text","text":"and subtitle. You can add accessory views to either end of the callout by"},{"type":"text","text":" "},{"type":"text","text":"implementing the "},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" and"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:rightCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" methods. You can further"},{"type":"text","text":" "},{"type":"text","text":"customize the callout’s contents by implementing the"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"type":"text","text":" "},{"type":"text","text":"if the annotation lacks a title, the annotation will not show a callout even"},{"type":"text","text":" "},{"type":"text","text":"when selected."}],"type":"paragraph"},{"type":"heading","level":4,"anchor":"Related-examples","text":"Related examples"},{"inlineContent":[{"type":"text","text":"TODO: Add annotation views and images"},{"type":"text","text":" "},{"type":"text","text":"TODO: Display custom views as callouts"},{"type":"text","text":" "},{"type":"text","text":"TODO: Default callout usage, learn how to show callouts for"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" objects."}],"type":"paragraph"}],"kind":"content"}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"kind":"symbol","schemaVersion":{"patch":0,"minor":3,"major":0},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:"]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:"},"metadata":{"symbolKind":"method","title":"mapView:annotationCanShowCallout:","role":"symbol","roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:annotationCanShowCallout:","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L869","fileName":"MLNMapViewDelegate.h"}},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","role":"collection","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"text":" ","type":"text"},{"text":"require the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"text":" ","type":"text"},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"type":"topic","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"title":"MLNMapViewDelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"type":"topic","kind":"symbol","abstract":[{"type":"text","text":"Returns a Boolean value indicating whether the annotation is able to display"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"role":"symbol","title":"mapView:annotationCanShowCallout:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","navigatorTitle":[{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","type":"topic","title":"MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"kind":"symbol","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","role":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation"}}}