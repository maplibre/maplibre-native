ers":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view that has selected the annotation."}]}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"text":"The object representing the annotation.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"anchor":"return-value","level":2,"type":"heading","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A Boolean value indicating whether the annotation should show a"},{"type":"text","text":" "},{"type":"text","text":"callout."}]}]},{"kind":"content","content":[{"anchor":"discussion","text":"Discussion","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"text":"This method is called after an annotation is selected, before any callout is","type":"text"},{"text":" ","type":"text"},{"text":"displayed for the annotation.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"YES"},{"type":"text","text":", a callout view is shown when the user taps on an"},{"type":"text","text":" "},{"type":"text","text":"annotation, selecting it. The default callout displays the annotation’s title"},{"type":"text","text":" "},{"type":"text","text":"and subtitle. You can add accessory views to either end of the callout by"},{"type":"text","text":" "},{"type":"text","text":"implementing the "},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" and"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:rightCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" methods. You can further"},{"type":"text","text":" "},{"type":"text","text":"customize the callout’s contents by implementing the"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"type":"text","text":" "},{"type":"text","text":"if the annotation lacks a title, the annotation will not show a callout even"},{"type":"text","text":" "},{"type":"text","text":"when selected."}]},{"type":"heading","level":4,"text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"text":"TODO: Add annotation views and images","type":"text"},{"text":" ","type":"text"},{"text":"TODO: Display custom views as callouts","type":"text"},{"text":" ","type":"text"},{"text":"TODO: Default callout usage, learn how to show callouts for","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" objects.","type":"text"}]}]}],"sections":[],"abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"text":" ","type":"text"},{"text":"extra information in a callout bubble.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","role":"symbol","navigatorTitle":[{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","type":"topic","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"kind":"symbol","title":"mapView:annotationCanShowCallout:","abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"text":" ","type":"text"},{"text":"extra information in a callout bubble.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","kind":"symbol","role":"symbol","title":"MLNMapViewDelegate","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}]}}}