ext":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The map view that has selected the annotation.","type":"text"}]}],"name":"mapView"},{"content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation."}],"type":"paragraph"}],"name":"annotation"}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","anchor":"return-value","level":2,"text":"Return Value"},{"type":"paragraph","inlineContent":[{"text":"A Boolean value indicating whether the annotation should show a","type":"text"},{"text":" ","type":"text"},{"text":"callout.","type":"text"}]}]},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This method is called after an annotation is selected, before any callout is"},{"type":"text","text":" "},{"type":"text","text":"displayed for the annotation."}]},{"type":"paragraph","inlineContent":[{"text":"If the return value is ","type":"text"},{"code":"YES","type":"codeVoice"},{"text":", a callout view is shown when the user taps on an","type":"text"},{"text":" ","type":"text"},{"text":"annotation, selecting it. The default callout displays the annotation’s title","type":"text"},{"text":" ","type":"text"},{"text":"and subtitle. You can add accessory views to either end of the callout by","type":"text"},{"text":" ","type":"text"},{"text":"implementing the ","type":"text"},{"code":"-mapView:leftCalloutAccessoryViewForAnnotation:","type":"codeVoice"},{"text":" and","type":"text"},{"text":" ","type":"text"},{"code":"-mapView:rightCalloutAccessoryViewForAnnotation:","type":"codeVoice"},{"text":" methods. You can further","type":"text"},{"text":" ","type":"text"},{"text":"customize the callout’s contents by implementing the","type":"text"},{"text":" ","type":"text