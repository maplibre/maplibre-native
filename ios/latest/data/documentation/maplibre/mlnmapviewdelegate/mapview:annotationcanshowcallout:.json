aragraph"},{"type":"paragraph","inlineContent":[{"text":"If the return value is ","type":"text"},{"type":"codeVoice","code":"YES"},{"type":"text","text":", a callout view is shown when the user taps on an"},{"type":"text","text":" "},{"text":"annotation, selecting it. The default callout displays the annotation’s title","type":"text"},{"text":" ","type":"text"},{"text":"and subtitle. You can add accessory views to either end of the callout by","type":"text"},{"type":"text","text":" "},{"type":"text","text":"implementing the "},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"text":" and","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:rightCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" methods. You can further"},{"type":"text","text":" "},{"text":"customize the callout’s contents by implementing the","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}]},{"inlineContent":[{"text":"If the return value is ","type":"text"},{"code":"NO","type":"codeVoice"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"type":"text","text":" "},{"text":"if the annotation lacks a title, the annotation will not show a callout even","type":"text"},{"type":"text","text":" "},{"type":"text","text":"when selected."}],"type":"paragraph"},{"level":4,"type":"heading","text":"Related examples","anchor":"Related-examples"},{"type":"paragraph","inlineContent":[{"text":"TODO: Add annotation views and images","type":"text"},{"text":" ","type":"text"},{"text":"TODO: Display custom views as callouts","type":"text"},{"type":"text","text":" "},{"text":"TODO: Default callout usage, learn how to show callouts for","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" objects."}]}]}],"v