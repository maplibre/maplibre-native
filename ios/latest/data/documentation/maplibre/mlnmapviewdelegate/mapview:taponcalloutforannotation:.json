alID":"c:objc(pl)MLNMapViewDelegate(im)mapView:tapOnCalloutForAnnotation:","title":"mapView:tapOnCalloutForAnnotation:"},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:taponcalloutforannotation:"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","title":"MLNMapViewDelegate","type":"topic","kind":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"fragments":[{"text":"MLNMapView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","role":"symbol","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","title":"MapLibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutViewDelegate/calloutViewTapped:":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlncalloutviewdelegate\/calloutviewtapped:","title":"calloutViewTapped:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"calloutViewTapped:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutViewDelegate\/calloutViewTapped:","abstract":[{"type":"text","text":"Tells the delegate that the callout view has been tapped."}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"calloutViewTapped:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:tapOnCalloutForAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:tapOnCalloutForAnnotation:","abstract":[{"type":"text","text":"Tells the delegate that the user tapped on an annotation’s callout view."}],"type":"topic","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:tapOnCalloutForAnnotation:","kind":"identifier"}],"kind":"symbol","role":"symbol","title":"mapView:tapOnCalloutForAnnotation:","navigatorTitle":[{"text":"mapView:tapOnCalloutForAnnotation:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:taponcalloutforannotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","kind":"symbol","title":"MLNAnnotation","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}]}}}