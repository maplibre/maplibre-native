gments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","title":"MLNMapView","type":"topic","fragments":[{"text":"MLNMapView","kind":"identifier"}],"role":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:tapOnCalloutForAnnotation:":{"role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:tapOnCalloutForAnnotation:"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:tapOnCalloutForAnnotation:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:taponcalloutforannotation:","abstract":[{"text":"Tells the delegate that the user tapped on an annotation’s callout view.","type":"text"}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"mapView:tapOnCalloutForAnnotation:"}],"title":"mapView:tapOnCalloutForAnnotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutViewDelegate/calloutViewTapped:":{"kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"calloutViewTapped:"}],"title":"calloutViewTapped:","url":"\/documentation\/maplibre\/mlncalloutviewdelegate\/calloutviewtapped:","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutViewDelegate\/calloutViewTapped:","role":"symbol","abstract":[{"text":"Tells the delegate that the callout view has been tapped.","type":"text"}],"navigatorTitle":[{"kind":"identifier","text":"calloutViewTapped:"}]}}}