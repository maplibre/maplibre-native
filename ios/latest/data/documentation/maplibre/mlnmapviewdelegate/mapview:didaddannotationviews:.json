bol","schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"symbolKind":"method","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:didAddAnnotationViews:","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:didAddAnnotationViews:"}],"remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L756"},"role":"symbol","roleHeading":"Instance Method","title":"mapView:didAddAnnotationViews:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"mapView:didAddAnnotationViews:"}]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:didaddannotationviews:"]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:didAddAnnotationViews:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","title":"MLNAnnotationView","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"text":" ","type":"text"},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol. When an annotation’s"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in