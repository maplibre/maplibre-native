oject/documentation/MapLibre/MLNMapViewDelegate/mapView:didAddAnnotationViews:":{"navigatorTitle":[{"kind":"identifier","text":"mapView:didAddAnnotationViews:"}],"abstract":[{"type":"text","text":"Tells the delegate that one or more annotation views have been added and"},{"text":" ","type":"text"},{"type":"text","text":"positioned on the map."}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:didaddannotationviews:","title":"mapView:didAddAnnotationViews:","fragments":[{"kind":"text","text":"- "},{"text":"mapView:didAddAnnotationViews:","kind":"identifier"}],"role":"symbol","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:didAddAnnotationViews:"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","type":"topic","title":"MapLibre","role":"collection","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"fragments":[{"text":"MLNMapView","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","role":"symbol","title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation"}}}