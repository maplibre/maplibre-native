stract":[{"type":"text","text":"Tells the delegate that one or more annotation views have been added and"},{"type":"text","text":" "},{"text":"positioned on the map.","type":"text"}],"metadata":{"navigatorTitle":[{"text":"mapView:didAddAnnotationViews:","kind":"identifier"}],"role":"symbol","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:didAddAnnotationViews:","modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:didAddAnnotationViews:","kind":"identifier"}],"title":"mapView:didAddAnnotationViews:","roleHeading":"Instance Method","symbolKind":"method","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L756","fileName":"MLNMapViewDelegate.h"}},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:didAddAnnotationViews:","interfaceLanguage":"occ"},"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map