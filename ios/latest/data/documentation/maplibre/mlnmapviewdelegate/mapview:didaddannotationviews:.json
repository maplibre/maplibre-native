arameters","parameters":[{"name":"mapView","content":[{"type":"paragraph","inlineContent":[{"text":"The map view to which the annotation views were added.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"An array of ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" objects representing the"},{"type":"text","text":" "},{"text":"views that were added.","type":"text"}]}],"name":"annotationViews"}]},{"kind":"content","content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"inlineContent":[{"text":"This method is called just after the views are added to the map. You can","type":"text"},{"type":"text","text":" "},{"type":"text","text":"implement this method to animate the addition of the annotation views."}],"type":"paragraph"}]}],"abstract":[{"type":"text","text":"Tells the delegate that one or more annotation views have been added and"},{"type":"text","text":" "},{"type":"text","text":"positioned on the map."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:didAddAnnotationViews:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:didAddAnnotationViews:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:didaddannotationviews:","type":"topic","title":"mapView:didAddAnnotationViews:","abstract":[{"type":"text","text":"Tells the delegate that one or more annotation views have been added and"},{"type":"text","text":" "},{"type":"text","text":"positioned on the map."}],"navigatorTitle":[{"kind":"identifier","text":"mapView:didAddAnnotationViews:"}],"role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:didAddAnnotationViews:"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","fragments":[{"kind":"identifier","text":"MLNMapView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" class is responsible for marking a point annotation","type":"text"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"text":"object that corresponds to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"text":" ","type":"text"},{"type":"text","text":"offscreen."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"title":"MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"text":" ","type":"text"},{"type":"text","text":"individual annotations."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate","type":"topic","role":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"title":"MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","role":"collection"}}}