ragraph","inlineContent":[{"type":"text","text":""},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":", "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":", and "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" objects"},{"type":"text","text":" "},{"type":"text","text":"cannot be added to the snapshotter at this time. Nor can "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" or "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"type":"text","text":"are specified are silently ignored."}]}],"name":"Note"}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotations:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"type":"topic","title":"MLNMapSnapshotterDelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapsnapshotterdelegate","role":"symbol","fragments":[{"text":"MLNMapSnapshotterDelegate","kind":"identifier"}],"abstract":[{"type":"text","text":"Optional methods about significant events when creating a snapshot using an"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true},{"type":"text","text":" object."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotations:":{"navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotations:","title":"addAnnotations:","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotations:","type":"topic","abstract":[{"type":"text","text":"Adds an array of annotations to the snapshotter."}],"fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotations:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","type":"topic","fragments":[{"text":"MLNMapSnapshotter","kind":"identifier"}],"role":"symbol","title":"MLNMapSnapshotter","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","type":"reference"},{"type":"text","text":" generates static raster images of the map. Each snapshot"},{"type":"text","text":" "},{"type":"text","text":"image depicts a portion of a map defined by an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotOptions","type":"reference"},{"type":"text","text":" object"},{"type":"text","text":" "},{"type":"text","text":"you provide. The snapshotter generates an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","type":"reference"},{"type":"text","text":" object"},{"type":"text","text":" "},{"type":"text","text":"asynchronously, calling "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","type":"reference"},{"type":"text","text":" methods if defined, then"},{"type":"text","text":" "},{"type":"text","text":"passing it into a completion handler once tiles and other resources needed for"},{"type":"text","text":" "},{"type":"text","text":"the snapshot are finished loading."}],"url":"\/documentation\/maplibre\/mlnmapsnapshotter"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline","fragments":[{"text":"MLNMultiPolyline","kind":"iden