ifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true,"type":"reference"},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"type":"text","text":"are specified are silently ignored."}]}],"type":"aside"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter"]]},"abstract":[{"type":"text","text":"Adds an array of annotations to the snapshotter."}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotations:"},"schemaVersion":{"major":0,"patch":0,"minor":3},"metadata":{"modules":[{"name":"MapLibre"}],"role":"symbol","roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotations:","kind":"identifier"}],"navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"title":"addAnnotations:","externalID":"c:objc(cs)MLNMapSnapshotter(im)addAnnotations:","symbolKind":"method"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"fragments":[{"text":"MLNMapSnapshotterDelegate","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","title":"MLNMapSnapshotterDelegate","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"Optional methods about significant events when creating a snapshot using an"},{"type":"text","text":" "},