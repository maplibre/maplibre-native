ation","role":"symbol","kind":"symbol","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","title":"MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}],"type":"topic","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"url":"\/documentation\/maplibre\/mlnpolyline","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotations:":{"url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotations:","fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotations:","kind":"identifier"}],"navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotations:","abstract":[{"type":"text","text":"Adds an array of annotations to the snapshotter."}],"role":"symbol","kind":"symbol","title":"addAnnotations:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"kind":"symbol","title":"MLNMapSnapshotterDelegate","type":"topic","abstract":[{"text":"Optional methods about significant events when creating a snapshot using an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true,"type":"reference"},{"text":" object.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","url":"\/documentation\/maplibre\/mlnmapsnapshotterdelegate","fragments":[{"text":"MLNMapSnapshotterDelegate","kind":"identifier"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"abstract":[{"type":"text","text":"An image generated by a snapshotter object."}],"fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapsnapshot","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","title":"MLNMapSnapshot","role":"symbol","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/docu