content":[{"inlineContent":[{"type":"text","text":""},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":", "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":", and "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" objects"},{"type":"text","text":" "},{"type":"text","text":"cannot be added to the snapshotter at this time. Nor can "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"type":"text","text":"are specified are silently ignored."}],"type":"paragraph"}],"style":"note","type":"aside"}],"kind":"content"}],"abstract":[{"text":"Adds an array of annotations to the snapshotter.","type":"text"}],"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"kind":"symbol","title":"MLNMapSnapshotterDelegate","role":"symbol","fragments":[{"text":"MLNMapSnapshotterDelegate","kind":"identifier"}],"type":"topic","abstract":[{"text":"Optional methods about significant events when creating a snapshot using an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true,"type":"reference"},{"text":" object.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapsnapshotterdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"url":"\/documentation\/maplibre\/mlnpolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"kind":"symbol","title":"MLNPolyline","fragments":[{"text":"MLNPolyline","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"text":" object.","type":"text"}],"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","title":"MLNMultiPolyline","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","title":"MLNMapSnapshotter","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapsnapshotter","fragments":[{"kind":"identifier","text":"MLNMapSnapshotter"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true},{"type":"text","text":" generates static raster images of the map. Each snapshot"},{"type":"text","text":" "},{"type":"text","text":"image depicts a portion of a map defined by an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotOptions","isActive":true},{"type":"text","text":" object"},{"type":"text","text":" "},{"type":"text","text":"you provide. The snapshotter generates an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","isActive":true},{"type":"text","text":" object"},{"type":"text","text":" "},{"type":"text","text":"asynchronously, calling "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","isActive":true},{"type":"text","text":" methods if defined, then"},{"type":"text","text":" "},{"type":"text","text":"passing it into a completion handler once tiles and other resources needed for"},{"type":"text","text":" "},{"type":"text","text":"the snapshot are finished loading."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmultipoint","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"role":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","kind":"symbol","url":"\/documentation\/maplibre","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotOptions":{"fragments":[{"kind":"identifier","text":"MLNMapSnapshotOptions"}],"url":"\/documentation\/maplibre\/mlnmapsnapshotoptions","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotOptions","abstract":[{"type":"text","text":"The options to use when creating images with the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true},{"type":"text","text":"."}],"kind":"symbol","role":"symbol","type":"topic","title":"MLNMapSnapshotOptions"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"title":"MLNMapSnapshot","url":"\/documentation\/maplibre\/mlnmapsnapshot","kind":"symbol","abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotations:":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotations:","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotations:"}],"title":"addAnnotations:","navigatorTitle":[{"kind":"identifier","text":"addAnnotations:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotations:","role":"symbol","abstract":[{"text":"Adds an array of annotations to the snapshotter.","type":"text"}]}}}