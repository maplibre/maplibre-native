s":[{"kind":"identifier","text":"MLNMapSnapshotter"}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"url":"\/documentation\/maplibre\/mlnmapsnapshot","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","kind":"symbol","fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"title":"MLNMapSnapshot","abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotations:":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotations:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotations:","fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotations:","kind":"identifier"}],"navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"abstract":[{"type":"text","text":"Adds an array of annotations to the snapshotter."}],"title":"addAnnotations:","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object."}],"fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","title":"MLNMultiPolyline","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"url":"\/documentation\/maplibre\/mlnmultipoint","title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"fragments":[{"text":"MLNMapSnapshotterDelegate","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","title":"MLNMapSnapshotterDelegate","role":"symbol","abstract":[{"text":"Optional methods about significant events when creating a snapshot using an","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true},{"text":" object.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapsnapshotterdelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"fragments":[{"text":"MLNPolyline","kind":"identifier"}],"role":"symbol","title":"MLNPolyline","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}]}}}