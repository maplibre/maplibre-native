notation:","role":"symbol","title":"addAnnotation:","url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotation:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"type":"topic","abstract":[{"type":"text","text":"Adds an annotation to the snapshotter."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter":{"title":"MLNMapSnapshotter","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","url":"\/documentation\/maplibre\/mlnmapsnapshotter","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","type":"reference","isActive":true},{"type":"text","text":" generates static raster images of the map. Each snapshot"},{"type":"text","text":" "},{"type":"text","text":"image depicts a portion of a map defined by an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotOptions","type":"reference","isActive":true},{"type":"text","text":" object"},{"type":"text","text":" "},{"type":"text","text":"you provide. The snapshotter generates an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","type":"reference","isActive":true},{"type":"text","text":" object"},{"type":"text","text":" "},{"type":"text","text":"asynchronously, calling "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","type":"reference","isActive":true},{"type":"text","text":" methods if defined, then"},{"type":"text","text":" "},{"type":"text","text":"passing it into a completion handler once tiles and other resources needed for"},{"type":"text","text":" "},{"type":"text","text":"the snapshot are finished loading."}],"fragments":[{"kind":"identifier","text":"MLNMapSnapshotter"}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapsnapshot","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"title":"MLNMapSnapshot","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotOptions":{"role":"symbol","url":"\/documentation\/maplibre\/mlnmapsnapshotoptions","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotOptions","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMapSnapshotOptions"}],"type":"topic","abstract":[{"type":"text","text":"The options to use when creating images with the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","isActive":true},{"type":"text","text":"."}],"title":"MLNMapSnapshotOptions"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","title":"MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"reference","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","title":"MLNMultiPolyline","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"text":" object.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmultipolyline","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}],"url":"\/documentation\/maplibre\/mlnpolyline","title":"MLNPolyline","type":"topic","fragments":[{"text":"MLNPolyline","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","role":"symbol","kind":"symbol"}}}