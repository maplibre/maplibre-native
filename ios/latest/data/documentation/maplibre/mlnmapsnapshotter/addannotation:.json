xt"},{"text":"objects to handle the display.","type":"text"}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","title":"MLNAnnotation","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}],"title":"MLNMapSnapshot","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","type":"topic","url":"\/documentation\/maplibre\/mlnmapsnapshot","fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotation:","kind":"symbol","title":"addAnnotation:","role":"symbol","type":"topic","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"abstract":[{"text":"Adds an annotation to the snapshotter.","type":"text"}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","type":"topic","title":"MLNMapSnapshotterDelegate","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapSnapshotterDelegate"}],"abstract":[{"text":"Optional methods about significant events when creating a snapshot using an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","type":"reference"},{"text":" object.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapsnapshotterdelegate"},"doc://org.