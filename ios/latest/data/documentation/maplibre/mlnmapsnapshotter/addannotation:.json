":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","role":"collection","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"title":"MLNPointCollection","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"type":"topic","role":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"url":"\/documentation\/maplibre\/mlnmapsnapshot","fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","type":"topic","kind":"symbol","title":"MLNMapSnapshot","role":"symbol","abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotterDelegate":{"abstract":[{"text":"Optional methods about significant events when creating a snapshot using an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter","type":"reference"},{"text":" object.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapsnapshotterdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotterDelegate","title":"MLNMapSnapshotterDelegate","fragments":[{"text":"MLNMapSnapshotterDelegate","kind":"identifier"}],"kind":"symbol","role":"symbol"}}}