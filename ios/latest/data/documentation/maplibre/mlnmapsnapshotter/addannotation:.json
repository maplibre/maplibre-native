text"},{"text":"the snapshot are finished loading.","type":"text"}],"type":"topic","role":"symbol","fragments":[{"text":"MLNMapSnapshotter","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}],"title":"MLNMapSnapshot","type":"topic","fragments":[{"text":"MLNMapSnapshot","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapsnapshot","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"url":"\/documentation\/maplibre\/mlnpolyline","title":"MLNPolyline","kind":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotation:":{"abstract":[{"type":"text","text":"Adds an annotation to the snapshotter."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotation:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"title":"addAnnotat