rence"},{"text":" object.","type":"text"}],"title":"MLNMultiPolyline"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","role":"collection","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshot":{"abstract":[{"text":"An image generated by a snapshotter object.","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapSnapshot"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshot","type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapsnapshot","title":"MLNMapSnapshot"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapSnapshotter/addAnnotation:":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapsnapshotter\/addannotation:","role":"symbol","abstract":[{"type":"text","text":"Adds an annotation to the snapshotter."}],"title":"addAnnotation:","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapSnapshotter\/addAnnotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text