ments":[{"text":"MLNPointCollection","kind":"identifier"}],"title":"MLNPointCollection","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"type":"topic","title":"MLNMultiPoint","kind":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","url":"\/documentation\/maplibre\/mlnmultipoint"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","role":"symbol","fragments":[{"text":"MLNEmptyFeature","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnemptyfeature","kind":"symbol","type":"topic","title":"MLNEmptyFeature","abstract":[{"text":"An ","type":"text