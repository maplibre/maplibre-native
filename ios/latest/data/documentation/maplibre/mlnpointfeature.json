rance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"topic","url":"\/documentation\/maplibre\/mlnvectortilesource","title":"MLNVectorTileSource"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/WebAPIDataExample":{"title":"Showing data from an API","role":"article","type":"topic","url":"\/documentation\/maplibre-native-for-ios\/webapidataexample","abstract":[{"text":"Showing data from an API with custom styling and interaction","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/WebAPIDataExample","kind":"article"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeature":{"url":"\/documentation\/maplibre\/mlnpointfeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPointFeature"}],"title":"MLNPointFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature"},{"type":"text","text":" object associates a point shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","title":"MLNShape","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShape"}],"url":"\/documentation\/maplibre\/mlnshape","abstract":[{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference"},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"title":"MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNFeature"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","url":"\/documentation\/maplibre\/mlnfeature","title":"MLNFeature","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature"},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"type":"text","text":" "},{"type":"text","text":"contained in an "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" or "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"type":"text","text":" object. Each concrete"},{"type":"text","text":" "},{"type":"text","text":"subclass of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotationimage","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true,"type":"reference"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","title":"MLNAnnotationImage","role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","kind":"symbol","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"role":"symbol","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference"},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeatureCluster":{"url":"\/documentation\/maplibre\/mlnpointfeaturecluster","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","isActive":true},{"type":"text","text":" object associates a point shape (with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes) and represents a point cluster."}],"role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNPointFeatureCluster"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","kind":"symbol","title":"MLNPointFeatureCluster"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"url":"\/documentation\/maplibre\/mlnannotationview","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"title":"MLNAnnotationView","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"fragments":[{"kind":"identifier","text":"MLNShapeSource"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSou