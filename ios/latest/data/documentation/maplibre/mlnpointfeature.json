rence","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}],"title":"MLNFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","fragments":[{"kind":"identifier","text":"MLNFeature"}],"role":"symbol","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeature":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnpointfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature","title":"MLNPointFeature","role":"symbol","type":"topic","fragments":[{"text":"MLNPointFeature","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature"},{"text":" object associates a point shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeatureCluster":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNPointFeatureCluster"}],"url":"\/documentation\/maplibre\/mlnpointfeaturecluster","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","isActive":true},{"type":"text","text":" object associates a point shape (with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes) and represents a point cluster."}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","title":"MLNPointFeatureCluster"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorT