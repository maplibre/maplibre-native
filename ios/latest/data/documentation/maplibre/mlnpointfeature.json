//org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"type":"topic","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","title":"MLNShapeSource","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnshapesource","role":"symbol","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" or "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with an "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"code":"shape","type":"codeVoice"},{"type":"text","text":" or "},{"code":"URL","type":"codeVoice"},{"type":"text","text":" property."}]},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/WebAPIDataExample":{"title":"Showing data from an API","url":"\/documentation\/maplibre-native-for-ios\/webapidataexample","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/WebAPIDataExample","abstract":[{"text":"Showing data from an API with custom styling and interaction","type":"text"}],"kind":"article","type":"topic","role":"article"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"title":"MLNAnnotationImage","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"role":"symbol","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"text":" class is responsible for presenting point-based","type":"text"},{"text":" ","type":"text"},{"text":"annotations visually on a map view. Annotation image objects wrap ","type":"text"},{"code":"UIImage","type":"codeVoice"},{"text":" ","type":"text"},{"text":"objects and may be recycled later and put into a reuse queue that is maintained","type":"text"},{"text":" ","type":"text"},{"text":"by the map view.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationimage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnshape","title":"MLNShape","role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNShape"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"url":"\/documentation\/maplibre\/mlnannotationview","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","title":"MLNAnnotationView","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"type":"topic","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeatureCluster":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","role":"symbol","fragments":[{"text":"MLNPointFeatureCluster","kind":"identifier"}],"kind":"symbol","type":"topic","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","isActive":true,"type":"reference"},{"type":"text","text":" object associates a point shape (with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes) and represents a point cluster."}],"url":"\/documentation\/maplibre\/mlnpointfeaturecluster","title":"MLNPointFeatureCluster"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"type":"topic","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","url":"\/documentation\/maplibre\/mlnpointannotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeature":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature"},{"type":"text","text":" object associates a point shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"fragments":[{"kind":"identifier","text":"MLNPointFeature"}],"url":"\/documentation\/maplibre\/mlnpointfeature","title":"MLNPointFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","role":"