ments":[{"text":"MLNFeature","kind":"identifier"}],"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","type":"topic","title":"MLNFeature","url":"\/documentation\/maplibre\/mlnfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"role":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true,"type":"reference"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"text":"objects and may be recycled later and put into a reuse queue that is maintained","type":"text"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"url":"\/documentation\/maplibre\/mlnannotationimage","kind":"symbol","title":"MLNAnnotationImage","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/WebAPIDataExample":{"type":"topic","title":"Showing data from an API","url":"\/documentation\/maplibre-native-for-ios\/webapidataexample","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/WebAPIDataExample","role":"article","abstract":[{"text":"Showing data from an API with custom styling and interaction","type":"text"}],"kind":"article"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"title":"MLNShapeSource","role":"symbol","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" is a map content source that supplies vector shape