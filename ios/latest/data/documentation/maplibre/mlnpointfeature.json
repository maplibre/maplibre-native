apLibre/MLNFeature":{"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"type":"text","text":" "},{"type":"text","text":"contained in an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" object. Each concrete"},{"type":"text","text":" "},{"type":"text","text":"subclass of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}],"fragments":[{"kind":"identifier","text":"MLNFeature"}],"role":"symbol","type":"topic","title":"MLNFeature","url":"\/documentation\/maplibre\/mlnfeature","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeatureCluster":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","kind":"symbol","url":"\/documentation\/maplibre\/mlnpointfeaturecluster","role":"symbol","fragments":[{"text":"MLNPointFeatureCluster","kind":"identifier"}],"type":"topic","title":"MLNPointFeatureCluster","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster"},{"type":"text","text":" object associates a point shape (with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes) and represents a point cluster."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"url":"\/documentation\/maplibre\/mlnpointannotation","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference"},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"fragments":[{"kind":"identifier","text":"MLNShape"}],"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","url":"\/documentation\/maplibre\/mlnshape","kind":"symbol","title":"MLNShape"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnvectortilesource","type":"topic","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"title":"MLNVectorTileSource","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"url":"\/documentation\/maplibre\/mlnannotationview","kind":"symbol","type":"topic","role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"title":"MLNAnnotationView","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSource"}],"abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"type":"codeVoice","code":"shape"},{"type":"text","text":" or "},{"type":"codeVoice","code":"URL"},{"type":"text","text":" property."}],"type":"topic","url":"\/documentation\/maplibre\/mlnshapesource","title":"MLNShapeSource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","title":"MLNAnnotation","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/WebAPIDataExample":{"title":"Showing data from an API","role":"article","type":"topic","kind":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/WebAPIDataExample","url":"\/documentation\/maplibre-native-for-ios\/webapidataexample","abstract":[{"text":"Showing data from an API with custom styling and interaction","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeature":{"fragments":[{"kind":"identifier","text":"MLNPointFeature"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature","type":"reference","isActive":true},{"type":"text","text":" object associates a point shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"url":"\/documentation\/maplibre\/mlnpointfeature","title":"MLNPointFeature","type":"topic","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature"}}}