//org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"url":"\/documentation\/maplibre\/mlnpointannotation","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/WebAPIDataExample":{"abstract":[{"type":"text","text":"Showing data from an API with custom styling and interaction"}],"kind":"article","title":"Showing data from an API","role":"article","url":"\/documentation\/maplibre-native-for-ios\/webapidataexample","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/WebAPIDataExample"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","url":"\/documentation\/maplibre\/mlnshape","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"fragments":[{"text":"MLNShape","kind":"identifier"}],"role":"symbol","title":"MLNShape","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeatureCluster":{"type":"topic","fragments":[{"kind":"identifier","text":"MLNPointFeatureCluster"}],"role":"symbol","kind":"symbol","title":"MLNPointFeatureCluster","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","url":"\/documentation\/maplibre\/mlnpointfeaturecluster","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","type":"reference","isActive":true},{"text":" object associates a point shape (with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes) and represents a point cluster.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"type":"topic","kind":"symbol","title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeature":{"url":"\/documentation\/maplibre\/mlnpointfeature","fragments":[{"kind":"identifier","text":"MLNPointFeature"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature","type":"reference","isActive":true},{"type":"text","text":" object associates a point shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeature","role":"symbol","title":"MLNPointFeature","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference","isActive":true},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotationimage","identifier":"doc:\/\/org.swift.MyProje