//org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","fragments":[{"kind":"identifier","text":"MLNShape"}],"type":"topic","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnshape","title":"MLNShape"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/WebAPIDataExample":{"type":"topic","url":"\/documentation\/maplibre-native-for-ios\/webapidataexample","role":"article","abstract":[{"text":"Showing data from an API with custom styling and interaction","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/WebAPIDataExample","kind":"article","title":"Showing data from an API"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"url":"\/documentation\/maplibre\/mlnvectortilesource","title":"MLNVectorTileSource","role":"symbol","type":"topic","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointFeatureCluster":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster","fragments":[{"kind":"identifier","text":"MLNPointFeatureCluster"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnpointfeaturecluster","type":"topic","title":"MLNPointFeatureCluster","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointFeatureCluster"},{"type":"text","text":" object associates a point shape (with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes) and represents a point cluster."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"type":"topic","role":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference","isActive":true},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"code":"UIImage","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage","fragments":[{"kind":"identifier","text":"MLNAnnotationImage"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","title":"MLNAnnotationImage"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"title":"MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview","abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"role":"symbol","url":"\/documentation\/maplibre\/mlnshapesource","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/