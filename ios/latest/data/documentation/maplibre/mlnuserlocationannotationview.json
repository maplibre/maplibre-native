ect/documentation/MapLibre/MLNUserLocationAnnotationView/userLocation":{"role":"symbol","kind":"symbol","type":"topic","title":"userLocation","url":"\/documentation\/maplibre\/mlnuserlocationannotationview\/userlocation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView\/userLocation","fragments":[{"text":"userLocation","kind":"identifier"}],"abstract":[{"text":"Returns the annotation object indicating the user’s current location.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView/hitTestLayer":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView\/hitTestLayer","abstract":[{"type":"text","text":"Returns the layer that should be used for annotation selection hit testing."}],"url":"\/documentation\/maplibre\/mlnuserlocationannotationview\/hittestlayer","kind":"symbol","role":"symbol","fragments":[{"text":"hitTestLayer","kind":"identifier"}],"title":"hitTestLayer"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"role":"symbol","abstract":[{"type":"text","text":"View representing an "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"type":"text","text":" on screen."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","title":"MLNUserLocationAnnotationView","type":"topic","url":"\/documentation\/maplibre\/mlnuserlocationannotationview","fragments":[{"text":"MLNUserLocationAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView/mapView":{"title":"mapView","type":"topic","fragments":[{"text":"mapView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnuserlocationannotationview\/mapview","kind":"symbol","abstract":[{"type":"text","text":"Returns the associated map view."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView\/mapView"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","url":"\/documentation\/maplibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"url":"\/documentation\/maplibre\/mlnannotationview","kind":"symbol","role":"symbol","title":"MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}]}}}