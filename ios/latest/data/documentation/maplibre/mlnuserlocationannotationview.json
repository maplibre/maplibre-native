ments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"title":"MLNUserLocation","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","abstract":[{"text":"The MLNUserLocation class defines a specific type of annotation that identifies","type":"text"},{"text":" ","type":"text"},{"text":"the user’s current location. You do not create instances of this class","type":"text"},{"text":" ","type":"text"},{"text":"directly. Instead, you retrieve an existing ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true,"type":"reference"},{"text":" object from the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"userLocation"},{"text":" property of the map view displayed in your application.","type":"text"}],"url":"\/documentation\/maplibre\/mlnuserlocation","role":"symbol","fragments":[{"kind":"identifier","text":"MLNUserLocation"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"tex