le":"MLNUserLocation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView/mapView":{"url":"\/documentation\/maplibre\/mlnuserlocationannotationview\/mapview","abstract":[{"text":"Returns the associated map view.","type":"text"}],"title":"mapView","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView\/mapView","fragments":[{"text":"mapView","kind":"identifier"}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView/update":{"url":"\/documentation\/maplibre\/mlnuserlocationannotationview\/update","abstract":[{"text":"Updates the user location annotation.","type":"text"}],"title":"update","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView\/update","navigatorTitle":[{"kind":"identifier","text":"update"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"update"}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"url":"\/documentation\/maplibre\/mlnuserlocationannotationview","abstract":[{"type":"text","text":"View representing an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true,"type":"reference"},{"type":"text","text":" on screen."}],"title":"MLNUserLocationAnnotationView","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","fragments":[{"text":"MLNUserLocationAnnotationView","kind":"identifier"}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"type":"topic","abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"text":" class is responsible for marking a point annotation","type":"text"},{"type":"text","text":" "},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol. When an annotation’s"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol","title":"MLNAnnotation","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView/userLocation":{"url":"\/documentation\/maplibre\/mlnuserlocationannotationview\/userlocation","abstract":[{"text":"Returns the annotation object indicating the user’s current location.","type":"text"}],"title":"userLocation","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView\/userLocation","fragments":[{"text":"userLocation","kind":"identifier"}],"type":"topic","kind":"symbol"}}}