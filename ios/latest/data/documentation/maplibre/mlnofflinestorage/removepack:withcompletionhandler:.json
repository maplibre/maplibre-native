}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnofflinepack","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","role":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"title":"MLNOfflinePack","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true,"type":"reference"},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"type":"text","text":" "},{"text":"a region offline to a local database.","type":"text"}],"type":"topic"}}}