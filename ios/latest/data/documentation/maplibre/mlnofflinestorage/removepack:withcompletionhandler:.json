topic","navigatorTitle":[{"kind":"identifier","text":"reloadPacks"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackRemovalCompletionHandler":{"abstract":[{"type":"text","text":"A block to be called once an offline pack has been completely invalidated and"},{"type":"text","text":" "},{"type":"text","text":"removed."}],"type":"topic","url":"\/documentation\/maplibre\/mlnofflinepackremovalcompletionhandler","kind":"symbol","role":"symbol","title":"MLNOfflinePackRemovalCompletionHandler","fragments":[{"text":"MLNOfflinePackRemovalCompletionHandler","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackRemovalCompletionHandler"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"url":"\/documentation\/maplibre\/mlnofflinepack","title":"MLNOfflinePack","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","abstract":[{"text":"An ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"text":" ","type":"text"},{"text":"a region offline to a local database.","type":"text"}],"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"url":"\/documentation\/maplibre\/mlnofflinestorage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"type":"text","text":" "},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"type":"text","text":" "},{"type":"text","text":"object maintains a canonical collection of offline packs in its "},{"type":"codeVoice","code":"packs"},{"type":"text","text":" property."}],"fragments":[{"kind":"identifier","text":"MLNOfflineStorage"}],"title":"MLNOfflineStorage","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/removePack:withCompletionHandler:":{"url":"\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:","abstract":[{"text":"Unregisters the given offline pack and allows resources that are no longer","type":"text"},{"text":" ","type":"text"},{"text":"required by any remaining packs to be potentially freed.","type":"text"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removePack:withCompletionHandler:"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"removePack:withCompletionHandler:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:","kind":"symbol","type":"topic","title":"removePack:withCompletionHandler:"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"title":"MapLibre","role":"collection","kind":"symbol","url":"\/documentation\/maplibre","type":"topic"}}}