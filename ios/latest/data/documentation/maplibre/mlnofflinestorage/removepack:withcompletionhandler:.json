l"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"fragments":[{"text":"MLNOfflinePack","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnofflinepack","title":"MLNOfflinePack","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"type":"text","text":" "},{"text":"a region offline to a local database.","type":"text"}],"role":"symbol","kind":"symbol"}}}