.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"title":"MLNOfflinePack","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true,"type":"reference"},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"text":" ","type":"text"},{"text":"a region offline to a local database.","type":"text"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnofflinepack","fragments":[{"text":"MLNOfflinePack","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackRemovalCompletionHandler":{"title":"MLNOfflinePackRemovalCompletionHandler","url":"\/documentation\/maplibre\/mlnofflinepackremovalcompletionhandler","type":"topic","abstract":[{"text":"A block to be called once an offline pack has been completely invalidated and","type":"text"},{"text":" ","type":"text"},{"text":"removed.","type":"text"}],"kind":"symbol","fragments":[{"text":"MLNOfflinePackRemovalCompletionHandler","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackRemovalCompletionHandler","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","title":"MLNOfflineStorage","type":"topic","url":"\/documentation\/maplibre\/mlnofflinestorage","role":"symbol","fragments":[{"text":"MLNOfflineStorage","kind":"identifier"}],"abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"type":"text","text":" "},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"type":"text","text":" "},{"type":"text","text":"object maintains a canonical collection of offline packs in its "},{"type":"codeVoice","code":"packs"},{"type":"text","text":" property."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","fragments":[{"text":"- ","kind":"text"},{"text":"reloadPacks","kind":"identifier"}],"navigatorTitle":[{"text":"reloadPacks","kind":"identifier"}],"kind":"symbol","title":"reloadPacks","abstract":[{"text":"Forcibly, asynchronously reloads the ","type":"text"},{"code":"packs","type":"codeVoice"},{"text":" property. At some point after this","type":"text"},{"text":" ","type":"text"},{"text":"method is called, the pointer values of the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference"},{"text":" objects in the","type":"text"},{"text":" ","type":"text"},{"code":"packs","type":"codeVoice"},{"text":" property change, even if the underlying data for these packs has not","type":"text"},{"text":" ","type":"text"},{"text":"changed. If this method is called while a pack is actively downloading, the","type":"text"},{"text":" ","type":"text"},{"text":"behavior is undefined.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks"}}}