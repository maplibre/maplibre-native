{"metadata":{"modules":[{"name":"MapLibre"}],"role":"symbol","title":"removePack:withCompletionHandler:","symbolKind":"method","externalID":"c:objc(cs)MLNOfflineStorage(im)removePack:withCompletionHandler:","roleHeading":"Instance Method","fragments":[{"kind":"text","text":"- "},{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"removePack:withCompletionHandler:"}]},"abstract":[{"text":"Unregisters the given offline pack and allows resources that are no longer","type":"text"},{"type":"text","text":" "},{"text":"required by any remaining packs to be potentially freed.","type":"text"}],"kind":"symbol","sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:"],"traits":[{"interfaceLanguage":"occ"}]}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"text":"removePack:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNOfflinePack","text":"MLNOfflinePack","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"pack"},{"kind":"text","text":" "},{"text":"withCompletionHandler:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:BFv(#*$objc(cs)NSError)","text":"void (^)(NSError *)","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"completion"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"name":"pack","content":[{"inlineContent":[{"text":"The offline pack to remove.","type":"text"}],"type":"p