ePack:withCompletionHandler:":{"url":"\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:","type":"topic","kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"removePack:withCompletionHandler:"}],"title":"removePack:withCompletionHandler:","abstract":[{"type":"text","text":"Unregisters the given offline pack and allows resources that are no longer"},{"type":"text","text":" "},{"type":"text","text":"required by any remaining packs to be potentially freed."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"url":"\/documentation\/maplibre\/mlnofflinepack","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"topic","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"title":"MLNOfflinePack","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"text":" ","type":"text"},{"text":"a region offline to a local database.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","kind":"symbol","type":"topic","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"reloadPacks"}],"navigatorTitle":[{"text":"reloadPacks","kind":"identifier"}],"title":"reloadPacks","abstract":[{"type":"text","text":"Forcibly, asynchronously reloads the "},{"code":"packs","type":"codeVoice"},{"type":"text","text":" property. At some point after this"},{"type":"text","text":" "},{"text":"method is called, the pointer values of the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" objects in the","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"packs"},{"text":" property change, even if the underlying data for these packs has not","type":"text"},{"text":" ","type":"text"},{"text":"changed. If this method is called while a pack is actively downloading, the","type":"text"},{"type":"text","text":" "},{"text":"behavior is undefined.","type":"text"}],"role":"symbol"}}}