{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:"]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage"]]},"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"role":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removePack:withCompletionHandler:"}],"externalID":"c:objc(cs)MLNOfflineStorage(im)removePack:withCompletionHandler:","title":"removePack:withCompletionHandler:","navigatorTitle":[{"kind":"identifier","text":"removePack:withCompletionHandler:"}],"roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"symbolKind":"method"},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Unregisters the given offline pack and allows resources that are no longer"},{"type":"text","text":" "},{"type":"text","text":"required by any remaining packs to be potentially freed."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"removePack:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)MLNOfflinePack","text":"MLNOfflinePack","kind":"typeIdentifier","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" *) ","kind":"text"},{"text":"pack","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"withCompletionHandler:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:BFv(#*$objc(cs)NSError)","text":"void (^)(NSError *)","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"completion","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"kind":"parameters","parameters":[{"name":"pack","content":[{"type":"paragraph","inlineContent":[{"text":"The offline pack to remove.","type":"text"}]}]},{"name":"completion","content":[{"inlineContent":[{"text":"The completion handler to call once the pack has been","type":"text"},{"text":" ","type":"text"},{"text":"removed. This handler is executed asynchronously on the main queue.","type":"text"}],"type":"paragraph"}]}]},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"As soon as this method is called on a pack, the pack becomes invalid; any","type":"text"},{"text":" ","type":"text"},{"text":"attempt to send it a message will result in an exception being thrown. If an","type":"text"},{"text":" ","type":"text"},{"text":"error occurs and the pack cannot be removed, do not attempt to reuse the pack","type":"text"},{"text":" ","type":"text"},{"text":"object. Instead, if you need continued access to the pack, suspend all packs","type":"text"},{"text":" ","type":"text"},{"text":"and use the ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","type":"reference"},{"text":" method to obtain valid pointers to all the packs.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"To detect when any call to this method results in a pack being removed, observe"},{"type":"text","text":" "},{"type":"text","text":"KVO change notifications on the shared offline storage object’s "},{"type":"codeVoice","code":"packs"},{"type":"text","text":" key"},{"type":"text","text":" "},{"type":"text","text":"path. Removals from that array result in an "},{"type":"codeVoice","code":"NSKeyValueChangeRemoval"},{"type":"text","text":" change."}]},{"type":"paragraph","inlineContent":[{"text":"When you remove an offline pack, any resources that are required by that pack,","type":"text"},{"text":" ","type":"text"},{"text":"but not other packs, become eligible for deletion from offline storage. Because","type":"text"},{"text":" ","type":"text"},{"text":"the backing store used for offline storage is also used as a general purpose","type":"text"},{"text":" ","type":"text"},{"text":"cache for map resources, such resources may not be immediately removed if the","type":"text"},{"text":" ","type":"text"},{"text":"implementation determines that they remain useful for general performance of","type":"text"},{"text":" ","type":"text"},{"text":"the map.","type":"text"}]}]}],"kind":"symbol","sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/removePack:withCompletionHandler:":{"url":"\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:","fragments":[{"text":"- ","kind":"text"},{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"navigatorTitle":[{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"kind":"symbol","title":"removePack:withCompletionHandler:","abstract":[{"type":"text","text":"Unregisters the given offline pack and allows resources that are no longer"},{"type":"text","text":" "},{"type":"text","text":"required by any remaining packs to be potentially freed."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"fragments":[{"text":"MLNOfflineStorage","kind":"identifier"}],"role":"symbol","title":"MLNOfflineStorage","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnofflinestorage","abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"type":"text","text":" "},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"type":"text","text":" "},{"type":"text","text":"object maintains a canonical collection of offline packs in its "},{"type":"codeVoice","code":"packs"},{"type":"text","text":" property."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"role":"symbol","title":"MLNOfflinePack","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"topic","fragments":[{"text":"MLNOfflinePack","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"type":"text","text":" "},{"type":"text","text":"a region offline to a local database."}],"url":"\/documentation\/maplibre\/mlnofflinepack","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"navigatorTitle":[{"kind":"identifier","text":"reloadPacks"}],"abstract":[{"text":"Forcibly, asynchronously reloads the ","type":"text"},{"code":"packs","type":"codeVoice"},{"text":" property. At some point after this","type":"text"},{"text":" ","type":"text"},{"text":"method is called, the pointer values of the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true,"type":"reference"},{"text":" objects in the","type":"text"},{"text":" ","type":"text"},{"code":"packs","type":"codeVoice"},{"text":" property change, even if the underlying data for these packs has not","type":"text"},{"text":" ","type":"text"},{"text":"changed. If this method is called while a pack is actively downloading, the","type":"text"},{"text":" ","type":"text"},{"text":"behavior is undefined.","type":"text"}],"url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","title":"reloadPacks","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"reloadPacks"}],"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","type":"topic"}}}