l"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"url":"\/documentation\/maplibre\/mlnofflinepack","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"type":"text","text":" "},{"type":"text","text":"a region offline to a local database."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"topic","title":"MLNOfflinePack","role":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"kind":"symbol"}}}