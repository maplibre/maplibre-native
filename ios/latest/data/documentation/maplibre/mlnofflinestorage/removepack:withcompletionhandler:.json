}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"title":"MLNOfflinePack","abstract":[{"text":"An ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"a region offline to a local database."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnofflinepack","type":"topic","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}]}}}