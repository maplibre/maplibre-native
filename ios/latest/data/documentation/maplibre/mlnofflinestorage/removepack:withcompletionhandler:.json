.swift.MyProject/documentation/MapLibre/MLNOfflinePackRemovalCompletionHandler":{"title":"MLNOfflinePackRemovalCompletionHandler","url":"\/documentation\/maplibre\/mlnofflinepackremovalcompletionhandler","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"A block to be called once an offline pack has been completely invalidated and"},{"type":"text","text":" "},{"type":"text","text":"removed."}],"fragments":[{"text":"MLNOfflinePackRemovalCompletionHandler","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackRemovalCompletionHandler","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"title":"reloadPacks","navigatorTitle":[{"text":"reloadPacks","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","abstract":[{"text":"Forcibly, asynchronously reloads the ","type":"text"},{"type":"codeVoice","code":"packs"},{"text":" property. At some point after this","type":"text"},{"text":" ","type":"text"},{"text":"method is called, the pointer values of the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"text":" objects in the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"packs"},{"text":" property change, even if the underlying data for these packs has not","type":"text"},{"text":" ","type":"text"},{"text":"changed. If this method is called while a pack is actively downloading, the","type":"text"},{"text":" ","type":"text"},{"text":"behavior is undefined.","type":"text"}],"url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","fragments":[{"text":"- ","kind":"text"},{"text":"reloadPacks","kind":"identifier"}],"kind":"symbol","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"kind":"symbol","title":"MLNOfflinePack","url":"\/documentation\/maplibre\/mlnofflinepack","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"type":"text","text":" "},{"type":"text","text":"a region offline to a local database."}],"fragments":[{"text":"MLNOfflinePack","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"type":"topic","role":"symbol","kind":"symbol","title":"MLNOfflineStorage","url":"\/documentation\/maplibre\/mlnofflinestorage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"type":"text","text":" "},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"type":"text","text":" "},{"type":"text","text":"object maintains a canonical collection of offline packs in its "},{"code":"packs","type":"codeVoice"},{"type":"text","text":" property."}],"fragments":[{"kind":"identifier","text":"MLNOfflineStorage"}]}}}