abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"type":"text","text":" "},{"type":"text","text":"a region offline to a local database."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","fragments":[{"text":"MLNOfflinePack","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"fragments":[{"text":"- ","kind":"text"},{"text":"reloadPacks","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"reloadPacks"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","kind":"symbol","abstract":[{"type":"text","text":"Forcibly, asynchronously reloads the "},{"code":"packs","type":"codeVoice"},{"text":" property. At some point after this","type":"text"},{"type":"text","text":" "},{"text":"method is called, the pointer values of the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true,"type":"reference"},{"text":" objects in the","type":"text"},{"type":"text","text":" "},{"code":"packs","type":"codeVoice"},{"type":"text","text":" property change, even if the underlying data for these packs has not"},{"type":"text","text":" "},{"type":"text","text":"changed. If this method is called while a pack is actively downloading, the"},{"text":" ","type":"text"},{"text":"behavior is undefined.","type":"text"}],"title":"reloadPacks","url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnofflinestorage","fragments":[{"kind":"identifier","text":"MLNOfflineStorage"}],"role":"symbol","title":"MLNOfflineStorage","abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"text":" ","type":"text"},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"type":"text","text":" "},{"text":"object maintains a canonical collection of offline packs in its ","type":"text"},{"type":"codeVoice","code":"packs"},{"type":"text","text":" property."}]}}}