role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"role":"symbol","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference"},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"topic","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"kind":"symbol","title":"MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnpointcollection","title":"MLNPointCollection","type":"topic","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"t