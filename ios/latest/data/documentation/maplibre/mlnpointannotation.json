tCollectionFeature":{"url":"\/documentation\/maplibre\/mlnpointcollectionfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","isActive":true,"type":"reference"},{"text":" object associates a point collection with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"fragments":[{"text":"MLNPointCollectionFeature","kind":"identifier"}],"kind":"symbol","title":"MLNPointCollectionFeature","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"title":"MLNShapeCollection","url":"\/documentation\/maplibre\/mlnshapecollection","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"topic","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of zero or more"},{"type":"text","text":" "},{"text":"distinct but related shapes that are instances of ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"text":". The constituent","type":"text"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollection"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"title":"MLNMultiPolygon","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"type":"text","text":" "},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolygon","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:imageForAnnotation:":{"fragments":[{"kind":"text","text":"- "},{"text":"mapView:imageForAnnotation:","kind":"identifier"}],"role":"symbol","abstract":[{"type":"text","text":"Returns an annotation image object to mark the given point annotation object on"},{"text":" ","type":"text"},{"type":"text","text":"the map."}],"kind":"symbol","type":"topic","title":"mapView:imageForAnnotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:imageForAnnotation:","navigatorTitle":[{"text":"mapView:imageForAnnotation:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:imageforannotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","abstract":[{"text":"Adds an annotation to the map view.","type":"text"}],"fragments":[{"kind":"text","text":"- "},{"text":"addAnnotation:","kind":"identifier"}],"type":"topic","title":"addAnnotation:","kind":"symbol","navigatorTitle":[{"text":"addAnnotation:","kind":"identifier"}],"role":"symbol"}}}