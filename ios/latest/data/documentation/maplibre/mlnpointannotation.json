:[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"abstract":[{"type":"text","text":"Adds an annotation to the map view."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"url":"\/documentation\/maplibre\/mlnpolygon","type":"topic","role":"symbol","title":"MLNPolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","fragments":[{"text":"MLNPolygon","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"text":" object represents a closed shape consisting of four or more","type":"text"},{"text":" ","type":"text"},{"text":"vertices, specified as ","type":"text"},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the edges that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use a polygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"building, a lake, or an area you want to highlight.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolygonfeature","kind":"symbol","title":"MLNMultiPolygonFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","isActive":true},{"type":"text","text":" object associates a multipolygon shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"fragments":[{"kind":"identifier","text":"MLNMultiPolygonFeature"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","url":"\/documentation\/maplibre\/mlnmultipoint","title":"MLNMultiPoint","role":"symbol","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference","isActive":true},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/title":{"kind":"symbol","id