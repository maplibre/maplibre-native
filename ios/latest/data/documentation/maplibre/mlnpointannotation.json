":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","title":"MLNPointCollectionFeature","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","type":"reference","isActive":true},{"type":"text","text":" object associates a point collection with an"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}],"url":"\/documentation\/maplibre\/mlnpointcollectionfeature","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnpointannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"topic","title":"MLNPointAnnotation","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"type":"text","text":" "},{"ty