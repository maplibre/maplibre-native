ft.MyProject/documentation/MapLibre/MLNPolygon":{"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"type":"text","text":" object represents a closed shape consisting of four or more"},{"type":"text","text":" "},{"type":"text","text":"vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the edges that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use a polygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"building, a lake, or an area you want to highlight."}],"type":"topic","role":"symbol","title":"MLNPolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","kind":"symbol","url":"\/documentation\/maplibre\/mlnpolygon","fragments":[{"kind":"identifier","text":"MLNPolygon"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","title":"MLNPointCollection","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"url":"\/documentation\/maplibre\/mlnshapecollectionfeature","fragments":[{"text":"MLNShapeCollectionFeature","kind":"identifier"}],"type":"topic","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature"},{"type":"text","text":" object associates a shape collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"kind":"symbol","role":"symbol","title":"MLNShapeCollectionFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation/coordinate":{"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation\/coordinate","title":"coordinate","type":"topic","url":"\/documentation\/maplibre\/mlnpointannotation\/coordinate","fragments":[{"kind":"identifier","text":"coordinate"}],"abstract":[{"type":"text","text":"The coordinate point of the shape, specified as a latitude and longitude."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnstyle","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"title":"MLNStyle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","kind":"symbol","fragments":[{"text":"MLNStyle","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"url":"\/documentation\/maplibre\/mlnmultipoint","type":"topic","title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"fragments":[{"text":"MLNMultiPoint","kind":"identifier"}]}}}