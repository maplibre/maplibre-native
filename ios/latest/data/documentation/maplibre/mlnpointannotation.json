nFeature":{"url":"\/documentation\/maplibre\/mlnmultipolygonfeature","type":"topic","role":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates a multipolygon shape with an"},{"text":" ","type":"text"},{"type":"text","text":"optional identifier and attributes."}],"title":"MLNMultiPolygonFeature","fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"url":"\/documentation\/maplibre\/mlnpolylinefeature","type":"topic","role":"symbol","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","type":"reference"},{"type":"text","text":" object associates a polyline shape with an optional"},{"type":"text","text":" "},{"text":"identifier and attributes.","type":"text"}],"title":"MLNPolylineFeature","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"url":"\/documentation\/maplibre\/mlnmultipolylinefeature","type":"topic","role":"symbol","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature"},{"type":"text","text":" object associates a multipolyline shape with an"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"title":"MLNMultiPolylineFeature","fragments":[{"text":"MLNMultiPolylineFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"fragments":[{"text":"MLNStyle","kind":"identifier"}],"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"title":"MLNStyle","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"role":"symbol","title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/title":{"role":"symbol","fragments":[{"text":"title","kind":"identifier"}],"abstract":[{"type":"text","text":"The title of the shape annotation."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title","url":"\/documentation\/maplibre\/mlnshape\/title","title":"title","kind":"symbol"}}}