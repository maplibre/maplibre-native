pe\/title","kind":"symbol","title":"title","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/title","abstract":[{"text":"The title of the shape annotation.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"kind":"symbol","title":"MLNShapeCollectionFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","isActive":true},{"text":" object associates a shape collection with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnshapecollectionfeature","fragments":[{"text":"MLNShapeCollectionFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation/coordinate":{"title":"coordinate","abstract":[{"type":"text","text":"The coordinate point of the shape, specified as a latitude and longitude."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation\/coordinate","url":"\/documentation\/maplibre\/mlnpointannotation\/coordinate","role":"symbol","kind":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"coordinate"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"fragments":[{"text":"MLNShape","kind":"identifier"}],"title":"MLNShape","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"url":"\/documentation\/maplibre\/mlnshape","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"url":"\/documentation\/maplibre\/mlnpointcollectionfeature","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature"},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"title":"MLNPointCollectionFeature","role":"symbol","type":"topic","fragments":[{"text":"MLNPointCollectionFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature"}}}