ts":[{"kind":"identifier","text":"MLNShape"}],"kind":"symbol","title":"MLNShape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","role":"symbol","url":"\/documentation\/maplibre\/mlnshape","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"type":"topic","kind":"symbol","role":"symbol","title":"MLNStyle","fragments":[{"kind":"identifier","text":"MLNStyle"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","abstract":[{"type":"text","text":"The proxy object for the current map style."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","isActive":true},{"type":"text","text":" object associates a multipolygon shape with an"},{"text":" ","type":"text"},{"type":"text","text":"optional identifier and attributes."}],"type":"topic","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygonfeature","title":"MLNMultiPolygonFeature","fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"reference","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"text":" ","type":"text"},{"type":"text","text":"orchard."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","role":"symbol","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpointcollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"title":"MLNPointCollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation/coordinate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation\/coordinate","kind":"symbol","abstract":[{"type":"text","text":"The coordinate point of the shape, specified as a latitude and longitude."}],"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnpointannotation\/coordinate","title":"coordinate","fragments":[{"kind":"identifier","text":"coordinate"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"text":" ","type":"text"},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object, while the surrounding"},{"text":" ","type":"text"},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"type":"topic","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygon","title":"MLNMultiPolygon","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}]}}}