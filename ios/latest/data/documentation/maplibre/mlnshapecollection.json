ultiPolylineFeature":{"url":"\/documentation\/maplibre\/mlnmultipolylinefeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature"},{"type":"text","text":" object associates a multipolyline shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"kind":"symbol","title":"MLNMultiPolylineFeature","role":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNMultiPolylineFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"type":"topic","kind":"symbol","title":"MLNPointAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","role":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"type":"topic","url":"\/documentation\/maplibre\/mlnpolyline","kind":"symbol","title":"MLNPolyline","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"}}}