c://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"title":"MLNPolylineFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","fragments":[{"text":"MLNPolylineFeature","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","type":"reference","isActive":true},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"url":"\/documentation\/maplibre\/mlnpolylinefeature","type":"topic","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"abstract":[{"type":"text","text":"The proxy object for the current map style."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","role":"symbol","fragments":[{"text":"MLNStyle","kind":"identifier"}],"type":"topic","title":"MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","role":"symbol","url":"\/documentation\/maplibre\/mlnmultipolylinefeature","fragments":[{"text":"MLNMultiPolylineFeature","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","type":"reference","isActive":true},{"text":" object associates a multipolyline shape with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"title":"MLNMultiPolylineFeature","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection/shapeCollectionWithShapes:":{"type":"topic","navigatorTitle":[{"text":"shapeCollectionWithShapes:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshapecollection\/shapecollectionwithshapes:","kind":"symbol","abstract":[{"type":"text","text":"Creates and returns a shape collection consisting of the given shapes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection\/shapeCollectionWithShapes:","role":"symbol","fragments":[{"text":"+ ","kind":"text"},{"text":"shapeCollectionWithShapes:","kind":"identifier"}],"title":"shapeCollectionWithShapes:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"type":"text","text":" object, while the surrounding"},{"text":" ","type":"text"},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"topic","url":"\/documentation\/maplibre\/mlnmultipolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"kind":"symbol","title":"MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection/shapes":{"url":"\/documentation\/maplibre\/mlnshapecollection\/shapes","type":"topic","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/d