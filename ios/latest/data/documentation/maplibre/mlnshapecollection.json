t\/documentation\/MapLibre\/MLNShapeCollection","role":"symbol","fragments":[{"text":"MLNShapeCollection","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"type":"text","text":" "},{"type":"text","text":"distinct but related shapes that are instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":". The constituent"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","title":"MLNPointCollection","type":"topic","fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNCircleStyleLayer":{"title":"MLNCircleStyleLayer","fragments":[{"text":"MLNCircleStyleLayer","kind":"identi