yProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview","type":"topic","title":"MLNMapView","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","title":"MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"text":" ","type":"text"},{"type":"text","text":"orchard."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","role":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline","title":"MLNMultiPolyline","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"text":" ","type":"text"},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"text":" object.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"type":"topic","title":"addAnnotation:","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","navigatorTitle":[{"text":"addAnnotation:","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"type":"topic","url":"\/documentation\/maplibre\/mlnpolyline","title":"MLNPolyline","kind":"symbol","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"text":" ","type":"text"},{"type":"text","text":"specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the line segments that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"text":" ","type":"text"},{"type":"text","text":"path along which something moves."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","role":"symbol","fragments":[{"text":"MLNPolyline","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","title":"MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol"}}}