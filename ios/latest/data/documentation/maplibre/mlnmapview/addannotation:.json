ic","abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","type":"topic","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","kind":"symbol","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"title":"MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","title":"MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"title":"MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}],"fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"url":"\/documentation\/maplibre\/mlnpolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","title":"MLNPolyline","type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"kind":"symbol"}}}