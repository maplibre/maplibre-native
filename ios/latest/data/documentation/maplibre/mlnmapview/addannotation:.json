yProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"kind":"symbol","type":"topic","abstract":[{"text":"Adds an annotation to the map view.","type":"text"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addAnnotation:"}],"title":"addAnnotation:","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"url":"\/documentation\/maplibre\/mlnpointcollection","title":"MLNPointCollection","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"type":"text","text":" "},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"text":" ","type":"text"},{"type":"text","text":"orchard."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that","type":"text"},{"type":"text","text":" "},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"url":"\/documentation\/maplibre\/mlnpolyline","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"role":"symbol","type":"topic","title":"MLNPolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"kind":"symbol","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"url":"\/documentation\/maplibre\/mlnmultipolyline","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"type":"text","text":" "},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"text":" object.","type":"text"}],"type":"topic","title":"MLNMultiPolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview","fragments":[{"kind":"identifier","text":"MLNMapView"}],"role":"symbol","type":"topic","title":"MLNMapView"}}}