ocumentation\/MapLibre\/MLNMapView\/addAnnotation:","abstract":[{"text":"Adds an annotation to the map view.","type":"text"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","kind":"symbol","title":"addAnnotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"title":"MLNPointCollection","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpointcollection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNPointCollection"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","role":"symbol","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"fragments":[{"text":"MLNMapView","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","type":"topic","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"title":"MLNPolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmultipolyline","type":"topic","title":"MLNMultiPolyline","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]}}}