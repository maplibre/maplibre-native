lection"},{"type":"text","text":" objects cannot be added to the map view at this time."},{"type":"text","text":" "},{"type":"text","text":"Any multipoint, multipolyline, multipolygon, shape or point collection"},{"type":"text","text":" "},{"type":"text","text":"object that is specified is silently ignored."}],"type":"paragraph"}],"type":"aside","style":"note"},{"anchor":"Related-examples","text":"Related examples","level":4,"type":"heading"},{"name":"To Do","content":[{"type":"paragraph","inlineContent":[{"text":"Add a line annotation from GeoJSON.","type":"text"}]}],"type":"aside","style":"note"},{"name":"To Do","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Add an annotation to an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"type":"text","text":" object."}]}],"type":"aside","style":"note"}]}],"sections":[],"kind":"symbol","metadata":{"roleHeading":"Instance Method","symbolKind":"method","modules":[{"name":"MapLibre"}],"role":"symbol","externalID":"c:objc(cs)MLNMapView(im)addAnnotation:","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"title":"addAnnotation:","remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L1747"}},"abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","interfaceLanguage":"occ"},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/addannotation:"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","type":"topic","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifi