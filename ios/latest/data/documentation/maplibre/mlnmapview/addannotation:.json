"\/documentation\/maplibre\/mlnpointcollection","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"role":"symbol","title":"MLNPointCollection","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnpolyline","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"type":"topic","title":"MLNPolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","type":"topic","role":"symbol","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}]}}}