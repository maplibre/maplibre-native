yProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","title":"addAnnotation:","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances. The"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"url":"\/documentation\/maplibre\/mlnpointcollection","title":"MLNPointCollection","kind":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"type":"text","text":" "},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object."}],"url":"\/documentation\/maplibre\/mlnmultipolyline","title":"MLNMultiPolyline","kind":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"title":"MLNMapView","fragments":[{"kind":"identifier","text":"MLNMapView"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"fragments":[{"kind":"identifier","text":"MLNPolyline"}],"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"text":" ","type":"text"},{"type":"text","text":"path along which something moves."}],"kind":"symbol","title":"MLNPolyline","url":"\/documentation\/maplibre\/mlnpolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","type":"topic","title":"MLNAnnotation"}}}