e","content":[{"inlineContent":[{"text":"","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":", ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":", ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":", and","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"text":" objects cannot be added to the map view at this time.","type":"text"},{"text":" ","type":"text"},{"text":"Any multipoint, multipolyline, multipolygon, shape or point collection","type":"text"},{"text":" ","type":"text"},{"text":"object that is specified is silently ignored.","type":"text"}],"type":"paragraph"}],"style":"note"},{"type":"heading","level":4,"text":"Related examples","anchor":"Related-examples"},{"type":"aside","name":"To Do","content":[{"type":"paragraph","inlineContent":[{"text":"Add a line annotation from GeoJSON.","type":"text"}]}],"style":"note"},{"type":"aside","name":"To Do","content":[{"inlineContent":[{"type":"text","text":"Add an annotation to an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"type":"text","text":" object."}],"type":"paragraph"}],"style":"note"}],"kind":"content"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotation:","kind":"identifier"}],"navigatorTitle":[{"text":"addAnnotation:","kind":"identifier"}],"title":"addAnnotation:","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","abstract":[{"type":"text","text":"Adds an annotation to the map view."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","role":"symbol","title":"MLNAnnotation","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","type":"topic","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"title":"MLNPointCollection","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnpointcollection","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","type":"topic","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"kind":"symbol","title":"MLNMultiPolyline","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"text":" object.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"title":"MLNPolyline","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"type":"topic","url":"\/documentation\/maplibre\/mlnpolyline","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}]}}}