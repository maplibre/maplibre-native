pe":"topic","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"text":" object.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"title":"MLNMultiPolyline"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","url":"\/documentation\/maplibre","type":"topic","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","url":"\/documentation\/maplibre\/mlnpointcollection","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","title":"MLNPointCollection","kind":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"title":"MLNPolyline","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","url":"\/documentation\/maplibre\/mlnpolyline","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotation:"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"title":"addAnnotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","role":"symbol","abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"kind":"symbol","title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"kind":"identifier","text":"MLNMapView"}]}}}