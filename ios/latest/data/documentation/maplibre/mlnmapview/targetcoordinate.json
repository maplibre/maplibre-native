{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"fragments":[{"text":"targetCoordinate","kind":"identifier"}],"navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"role":"symbol","externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","modules":[{"name":"MapLibre"}],"symbolKind":"property","remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L752"},"title":"targetCoordinate","roleHeading":"Instance Property"},"kind":"symbol","sections":[],"schemaVersion":{"minor":3,"patch":0,"major":0},"abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"text":"location is being tracked.","type":"text"}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"text":"@property","kind":"keyword"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"text":", ","kind":"text"},{"text":"assign","kind":"keyword"},{"text":") ","kind":"text"},{"text":"CLLocationCoordinate2D","preciseIdentifier":"c:@S@CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":" ","kind":"text"},{"text":"targetCoordinate","kind":"identifier"},{"text":";","kind":"text"}],"platforms":["iOS"]}],"kind":"declarations"},{"content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"By default, this property is set to an invalid coordinate, indicating that"},{"type":"text","text":" "},{"type":"text","text":"there is no target. In course tracking mode, the targ