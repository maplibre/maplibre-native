t"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"text":" ","type":"text"},{"code":"-setTargetCoordinate:animated:","type":"codeVoice"},{"text":" method instead.","type":"text"}]}]}],"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"sections":[],"kind":"symbol","metadata":{"roleHeading":"Instance Property","symbolKind":"property","externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"fragments":[{"text":"targetCoordinate","kind":"identifier"}],"title":"targetCoordinate","role":"symbol","remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L742"}},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"title":"targetCoordinate","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/targetcoordinate","kind":"symbol","fragments":[{"kind":"identifier","text":"targetCoordinate"}],"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"doc://org.swift.MyProject/documentation/