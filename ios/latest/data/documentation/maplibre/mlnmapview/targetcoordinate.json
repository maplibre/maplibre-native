":"paragraph","inlineContent":[{"text":"Changing the value of this property updates the map view with an animated","type":"text"},{"text":" ","type":"text"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-setTargetCoordinate:animated:"},{"text":" method instead.","type":"text"}]}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"text":"location is being tracked.","type":"text"}],"metadata":{"role":"symbol","symbolKind":"property","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L713"},"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","roleHeading":"Instance Property","title":"targetCoordinate"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"type":"topic","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/targetcoordinate","abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"fragments":[{"kind":"identifier","text":"targetCoordinate"}],"title":"targetCoordinate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"doc://org.swift.MyProject/documentation/