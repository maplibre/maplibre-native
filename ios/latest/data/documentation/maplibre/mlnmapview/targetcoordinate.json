t"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"text":" ","type":"text"},{"code":"-setTargetCoordinate:animated:","type":"codeVoice"},{"text":" method instead.","type":"text"}]}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"sections":[],"metadata":{"modules":[{"name":"MapLibre"}],"title":"targetCoordinate","remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L713"},"navigatorTitle":[{"kind":"identifier","text":"targetCoordinate"}],"role":"symbol","symbolKind":"property","roleHeading":"Instance Property","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate"},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"kind":"symbol","schemaVersion":{"minor":3,"patch":0,"major":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNUserTrackingMode/MLNUserTrackingModeFollowWithCourse":{"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnusertrackingmode\/mlnusertrackingmodefollowwithcourse","type":"topic","title":"MLNUserTrackingModeFollowWithCourse","fragments":[{"kind":"identifier","text":"MLNUserTrackingModeFollowWithCourse"}],"abstract":[],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserTrackingMode\/MLNUserTrackingModeFollowWithCourse"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"title":"target