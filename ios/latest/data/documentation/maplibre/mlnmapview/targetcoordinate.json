t"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"type":"text","text":" "},{"code":"-setTargetCoordinate:animated:","type":"codeVoice"},{"type":"text","text":" method instead."}]}]}],"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"metadata":{"roleHeading":"Instance Property","role":"symbol","externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","title":"targetCoordinate","fragments":[{"kind":"identifier","text":"targetCoordinate"}],"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"symbolKind":"property"},"abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"type":"topic","title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"url":"\/documentation\/maplibre\/mlnmapview\/targetcoordinate","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"role":"symbol","type":"topic","title":"targetC