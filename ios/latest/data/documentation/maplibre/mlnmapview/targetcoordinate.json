{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L752","fileName":"MLNMapView.h"},"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","role":"symbol","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"symbolKind":"property","title":"targetCoordinate","navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"roleHeading":"Instance Property","modules":[{"name":"MapLibre"}]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"kind":"symbol","abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"@property","kind":"keyword"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"text":", ","kind":"text"},{"text":"assign","kind":"keyword"},{"text":") ","kind":"text"},{"text":"CLLocationCoordinate2D","kind":"typeIdentifier","preciseIdentifier":"c:@S@CLLocationCoordinate2D"},{"text":" ","kind":"text"},{"text":"targetCoordinate","kind":"identifier"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"By default, this property is set to an invalid coordinate, indicating that"},{"type":"text","text":" "},{"type":"text","text":"there is no target. In course tracking mode, the targ