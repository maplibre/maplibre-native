ext"},{"text":" ","type":"text"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-setTargetCoordinate:animated:"},{"type":"text","text":" method instead."}],"type":"paragraph"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"sections":[],"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"metadata":{"title":"targetCoordinate","externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","navigatorTitle":[{"kind":"identifier","text":"targetCoordinate"}],"symbolKind":"property","modules":[{"name":"MapLibre"}],"fragments":[{"text":"targetCoordinate","kind":"identifier"}],"role":"symbol","roleHeading":"Instance Property"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"fragments":[{"kind":"identifier","text":"targetCoordinate"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/targetcoordinate","type":"topic","abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"text":" ","type":"text"},{"type":"text","text":"location is being tracked."}],"title":"targetCoordinate","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","url":"\/documentation\/maplibre","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documen