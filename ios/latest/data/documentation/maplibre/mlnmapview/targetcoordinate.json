{"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"sections":[],"abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"text":"location is being tracked.","type":"text"}],"metadata":{"role":"symbol","modules":[{"name":"MapLibre"}],"symbolKind":"property","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","title":"targetCoordinate","navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L742","fileName":"MLNMapView.h"},"roleHeading":"Instance Property"},"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"kind":"keyword","text":"nonatomic"},{"kind":"text","text":", "},{"kind":"keyword","text":"assign"},{"kind":"text","text":") "},{"kind":"typeIdentifier","text":"CLLocationCoordinate2D","preciseIdentifier":"c:@S@CLLocationCoordinate2D"},{"kind":"text","text":" "},{"kind":"identifier","text":"targetCoordinate"},{"kind":"text","text":";"}]}]},{"kind":"content","content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"By default, this property is set to an invalid coordinate, indicating that"},{"type":"text","text":" "},{"type":"text","text":"there is no target. In course tracking mode, the target