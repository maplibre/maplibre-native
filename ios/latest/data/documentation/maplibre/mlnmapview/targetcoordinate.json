{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"fragments":[{"text":"targetCoordinate","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Property","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"targetCoordinate"}],"remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L752"},"title":"targetCoordinate","externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","symbolKind":"property"},"sections":[],"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"text":"location is being tracked.","type":"text"}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"@property","kind":"keyword"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"text":", ","kind":"text"},{"text":"assign","kind":"keyword"},{"text":") ","kind":"text"},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":" ","kind":"text"},{"text":"targetCoordinate","kind":"identifier"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"By default, this property is set to an invalid coordinate, indicating that"},{"type":"text","text":" "},{"type":"text","text":"there is no target. In course tracking mode, the target