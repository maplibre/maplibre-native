ive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserTrackingMode\/MLNUserTrackingModeFollowWithCourse"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Changing the value of this property updates the map view with an animated"},{"text":" ","type":"text"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"type":"text","text":" "},{"code":"-setTargetCoordinate:animated:","type":"codeVoice"},{"type":"text","text":" method instead."}]}]}],"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"text":"location is being tracked.","type":"text"}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"modules":[{"name":"MapLibre"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","roleHeading":"Instance Property","navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"title":"targetCoordinate","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"symbolKind":"property","role":"symbol"},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNUserTrackingMode/MLNUserTrackingModeFollowWithCourse":{"url":"\/documentation\/maplibre\/mlnusertrackingmode\/mlnusertrackingmodefollowwithcourse","title":"MLNUserTrackingModeFollowWithCourse","abstract":[],"role":"symbol","fragments":[{"text":"MLNUserTrackingModeFollowWithCourse","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserTrackingMode\/MLNUserTrackingModeFollowWithCourse","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","title":"MLNMapView","kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"type":"text","text":"An interactive, customizable map view w