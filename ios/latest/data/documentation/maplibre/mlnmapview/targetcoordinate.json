t"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"-setTargetCoordinate:animated:"},{"type":"text","text":" method instead."}]}]}],"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"text":" ","type":"text"},{"type":"text","text":"location is being tracked."}],"schemaVersion":{"patch":0,"major":0,"minor":3},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"kind":"symbol","metadata":{"symbolKind":"property","title":"targetCoordinate","modules":[{"name":"MapLibre"}],"role":"symbol","navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","roleHeading":"Instance Property","fragments":[{"text":"targetCoordinate","kind":"identifier"}]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"title":"targetCoordinate","role":"symbol","fragments":[{"text":"targetCoordinate","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/targetcoordinate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","type":"topic","abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"text":"An interactive, customizable m