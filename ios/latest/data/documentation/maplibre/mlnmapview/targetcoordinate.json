t"},{"text":"transition. If you don’t want to animate the change, use the","type":"text"},{"text":" ","type":"text"},{"code":"-setTargetCoordinate:animated:","type":"codeVoice"},{"text":" method instead.","type":"text"}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"text":"The geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"text":"location is being tracked.","type":"text"}],"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"metadata":{"remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L752"},"role":"symbol","modules":[{"name":"MapLibre"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","roleHeading":"Instance Property","fragments":[{"kind":"identifier","text":"targetCoordinate"}],"title":"targetCoordinate","navigatorTitle":[{"text":"targetCoordinate","kind":"identifier"}],"symbolKind":"property"},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/targetCoordinate":{"url":"\/documentation\/maplibre\/mlnmapview\/targetcoordinate","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoord