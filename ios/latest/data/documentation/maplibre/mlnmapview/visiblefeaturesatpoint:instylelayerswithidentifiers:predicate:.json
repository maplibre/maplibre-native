identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature"},{"text":"  protocol is used to provide details about geographic features","type":"text"},{"type":"text","text":" "},{"type":"text","text":"contained in an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" or ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" object. Each concrete"},{"text":" ","type":"text"},{"text":"subclass of ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"text":"feature object associates a shape with an optional identifier and attributes.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNFeature"}],"type":"topic","title":"MLNFeature","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","url":"\/documentation\/maplibre\/mlnfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"kind":"symbol","title":"MLNStyle","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnstyle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","fragments":[{"text":"MLNStyle","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"text":" ","type":"text"},{"text":"in ","type":"text"},{"type":"text","text":"Mapbox Vector Tile"},{"text":" format","type":"text"},{"type":"text","text":" "},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"text":" ","type":"text"},{"text":"the","type":"text"},{"type":"text","text":" "},{"text":"TileJSON specification","type":"text"},{"text":".","type":"text"},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object along with one or more","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true,"type":"reference"},{"text":" objects. A vector style layer defines the appearance of","type":"text"},{"text":" ","type":"text"},{"text":"any content supplied by the vector tile source.","type":"text"}],"title":"MLNVectorTileSource","url":"\/documentation\/maplibre\/mlnvectortilesource","fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}],"role":"symbol","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"text":"MLNMapView","kind":"identifier"}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Appleâ€™s MapKit."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"title":"MLNRasterTileSource","kind":"symbol","type":"topic","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource"},{"type":"text","text":" is a map content source that supplies raster image tiles"},{"type":"text","text":" "},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the"},{"type":"text","text":" "},{"text":"TileJSON specification","type":"text"},{"text":".","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"A raster tile source is added to an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object along with one or more","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","type":"reference","isActive":true},{"type":"text","text":" objects. Use a raster style layer to control the"},{"text":" ","type":"text"},{"type":"text","text":"appearance of content supplied by the raster tile source."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","fragments":[{"text":"MLNRasterTileSource","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnrastertilesource","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","kind":"symbol","title":"MLNShapeSource","url":"\/documentation\/maplibre\/mlnshapesource","role":"symbol","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" or "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":","},{"type":"text","text":" "},{"text":"or they may be defined by local or external","type":"text"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"text":" code. A shape source is added to an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"type":"text","text":" object along with an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference"},{"text":" object. The vector style","type":"text"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"type":"codeVoice","code":"shape"},{"text":" or ","type":"text"},{"type":"codeVoice","code":"URL"},{"type":"text","text":" property."}],"type":"topic"}}}