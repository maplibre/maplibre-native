{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/visiblefeaturesatpoint:instylelayerswithidentifiers:predicate:"]}],"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:"},"metadata":{"fragments":[{"text":"- ","kind":"text"},{"text":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"role":"symbol","navigatorTitle":[{"text":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"symbolKind":"method","title":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","roleHeading":"Instance Method","externalID":"c:objc(cs)MLNMapView(im)visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:"},"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"type":"text","text":"Returns an array of rendered map features that intersect with a given point,"},{"type":"text","text":" "},{"type":"text","text":"restricted to the given style layers and filtered by the given predicate."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:Q$objc(cs)NSArray","text":"NSArray<id<MLNFeature>>"},{"text":" *) ","kind":"text"},{"text":"visibleFeaturesAtPoint:","kind":"identifier"},{"kind":"text","text":"("},{"text":"CGPoint","kind":"typeIdentifier","preciseIdentifier":"c:@S@CGPoint"},{"kind":"text","text":") "},{"kind":"internalParam","text":"point"},{"kind":"text","text":" "},{"text":"inStyleLayersWithIdentifiers:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"NSSet<NSString *>","preciseIdentifier":"c:Q$objc(cs)NSSet","kind":"typeIdentifier"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"styleLayerIdentifiers"},{"kind":"text","text":" "},{"kind":"identifier","text":"predicate:"},{"kind":"text","text":"("},{"preciseIdentifier":"c:objc(cs)NSPredicate","text":"NSPredicate","kind":"typeIdentifier"},{"kind":"text","text":" *) "},{"text":"predicate","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"]}]},{"parameters":[{"name":"point","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A point expressed in the map viewâ€™s coordinate system."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A set of strings that correspond to the names of"},{"type":"text","text":" "},{"type":"text","text":"layers defined in the current style. Only the features contained in these"},{"text":" ","type":"text"},{"type":"text","text":"layers are included in the returned array."}]}],"name":"styleLayerIdentifiers"},{"content":[{"type":"paragraph","inlineContent":[{"text":"A predicate to filter the returned features.","type":"text"}]}],"name":"predicate"}],"kind":"parameters"},{"content":[{"text":"Return Value","type":"heading","anchor":"return-value","level":2},{"inlineContent":[{"text":"An array of objects conforming to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true,"type":"reference"},{"type":"text","text":" protocol that"},{"type":"text","text":" "},{"text":"represent features in the sources used by the current style.","type":"text"}],"type":"paragraph"}],"kind":"content"},{"kind":"content","content":[{"level":2,"text":"Discussion","anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"Each object in the returned array represents a feature rendered by the"},{"text":" ","type":"text"},{"type":"text","text":"current style and provides access to attributes specified by the relevant map"},{"type":"text","text":" "},{"text":"content sources. The returned array includes features loaded by","type":"text"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"text":" and ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" objects but does not include"},{"text":" ","type":"text"},{"text":"anything from ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","isActive":true,"type":"reference"},{"text":" objects, or from video or canvas sources,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"which are unsupported by this SDK."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The returned features are drawn by a style layer in the current style. For"},{"type":"text","text":" "},{"text":"example, suppose the current style uses the","type":"text"},{"text":" ","type":"text"},{"text":"Mapbox Streets source","type":"text"},{"type":"text","text":","},{"text":" ","type":"text"},{"text":"but none of the specified style layers includes features that have the ","type":"text"},{"type":"codeVoice","code":"maki"},{"type":"text","text":" "},{"text":"property set to ","type":"text"},{"type":"codeVoice","code":"bus"},{"type":"text","text":". If you pass a point corresponding to the location of a"},{"type":"text","text":" "},{"type":"text","text":"bus stop into this method, the bus stop feature does not appear in the"},{"type":"text","text":" "},{"text":"resulting array. On the other hand, if the style does include bus stops, an","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true,"type":"reference"},{"type":"text","text":" object representing that bus stop is returned and its"},{"text":" ","type":"text"},{"code":"featureAttributes","type":"codeVoice"},{"type":"text","text":" dictionary has the "},{"code":"maki","type":"codeVoice"},{"type":"text","text":" key set to "},{"code":"bus","type":"codeVoice"},{"type":"text","text":" (along with"},{"text":" ","type":"text"},{"type":"text","text":"other attributes). The dictionary contains only the attributes provided by the"},{"type":"text","text":" "},{"text":"tile source; it does not include computed attribute values or rules about how","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the feature is rendered by the current style."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"The returned array is sorted by z-order, starting with the topmost rendered"},{"text":" ","type":"text"},{"type":"text","text":"feature and ending with the bottommost rendered feature. A feature that is"},{"type":"text","text":" "},{"type":"text","text":"rendered multiple times due to wrapping across the antimeridian at low zoom"},{"type":"text","text":" "},{"type":"text","text":"levels is included only once, subject to the caveat that follows."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Features come from tiled vector data or GeoJSON data that is converted to tiles"},{"type":"text","text":" "},{"text":"internally, so feature geometries are clipped at tile boundaries and features","type":"text"},{"text":" ","type":"text"},{"text":"may appear duplicated across tiles. For example, suppose the specified point","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"lies along a road that spans the screen. The resulting array includes those"},{"type":"text","text":" "},{"text":"parts of the road that lie within the map tile that contain the specified","type":"text"},{"type":"text","text":" "},{"type":"text","text":"point, even if the road extends into other tiles."}]},{"inlineContent":[{"text":"To find out the layer names in a particular style, view the style in","type":"text"},{"type":"text","text":" "},{"text":"Maputnik","type":"text"},{"text":".","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Only visible features are returned. To obtain features regardless of"},{"type":"text","text":" "},{"type":"text","text":"visibility, use the"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource\/featuresInSourceLayersWithIdentifiers:predicate:","type":"reference","isActive":true},{"type":"text","text":" and"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/featuresMatchingPredicate:","type":"reference","isActive":true},{"text":" methods on the relevant sources.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"The returned features may also include features corresponding to annotations.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"These features are not object-equal to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" objects that were","type":"text"},{"type":"text","text":" "},{"text":"originally added to the map. To query the map for annotations, use","type":"text"},{"type":"text","text":" "},{"code":"visibleAnnotations","type":"codeVoice"},{"text":" or ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleAnnotationsInRect:","isActive":true,"type":"reference"},{"type":"text","text":"."}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"type":"topic","title":"MLNRasterStyleLayer","url":"\/documentation\/maplibre\/mlnrasterstylelayer","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true},{"type":"text","text":" is a style layer that renders georeferenced raster"},{"text":" ","type":"text"},{"type":"text","text":"imagery on the map, especially raster tiles."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","fragments":[{"text":"MLNRasterStyleLayer","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"title":"MLNVectorTileSource","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnvectortilesource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}],"abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"text":" ","type":"text"},{"text":"in ","type":"text"},{"type":"text","text":"Mapbox Vector Tile"},{"text":" format","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"text":"the","type":"text"},{"text":" ","type":"text"},{"text":"TileJSON specification","type":"text"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true,"type":"reference"},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"text":" ","type":"text"},{"type":"text","text":"any content supplied by the vector tile source."}]},"d