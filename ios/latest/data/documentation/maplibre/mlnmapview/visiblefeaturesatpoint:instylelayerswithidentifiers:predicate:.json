"\/documentation\/maplibre\/mlnfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","kind":"symbol","type":"topic","title":"MLNFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/visibleAnnotationsInRect:":{"abstract":[{"type":"text","text":"Returns the list of annotations associated with the receiver that intersect with"},{"text":" ","type":"text"},{"type":"text","text":"the given rectangle."}],"url":"\/documentation\/maplibre\/mlnmapview\/visibleannotationsinrect:","title":"visibleAnnotationsInRect:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"visibleAnnotationsInRect:"}],"role":"symbol","kind":"symbol","navigatorTitle":[{"text":"visibleAnnotationsInRect:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleAnnotationsInRect:","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","title":"MLNRasterStyleLayer","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNRasterStyleLayer"}],"type":"topic","url":"\/documentation\/maplibre\/mlnrasterstylelayer","role":"symbol","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer"},{"text":" is a style layer that renders georeferenced raster","type":"text"},{"type":"text","text":" "},{"type":"text","text":"imagery on the map, especially raster tiles."}]}}}