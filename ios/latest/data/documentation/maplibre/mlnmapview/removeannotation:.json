ion,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotation:":{"url":"\/documentation\/maplibre\/mlnmapview\/removeannotation:","role":"symbol","type":"topic","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removeAnnotation:"}],"navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","title":"removeAnnotation:","abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","role":"collection","type":"topic","url":"\/documentation\/maplibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"}}}