LNMapView"]]},"metadata":{"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","symbolKind":"method","roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"}],"navigatorTitle":[{"text":"removeAnnotation:","kind":"identifier"}],"title":"removeAnnotation:","role":"symbol"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"type":"topic","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotation:":{"navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"type":"topic","title":"removeAnnotation:","kind":"symbol","abstract":[{"text":"Removes an annotation from the map view, deselecting it if it is selected.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapview\/removeannotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"removeAnnotation:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","role":"symbol","kind":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"title":"MLNAnnotation","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","url":"\/documentation\/maplibre","type":"topic","role":"collection","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]}}}