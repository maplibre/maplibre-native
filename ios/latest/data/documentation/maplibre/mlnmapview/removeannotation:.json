{"schemaVersion":{"patch":0,"minor":3,"major":0},"primaryContentSections":[{"declarations":[{"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"kind":"identifier","text":"removeAnnotation:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"id<MLNAnnotation>","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation object to remove. This object must conform"},{"type":"text","text":" "},{"type":"text","text":"to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol"}]}],"name":"annotation"}],"kind":"parameters"},{"kind":"content","content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"Removing an annotation object dissociates it from the map view entirely,"},{"type":"text","text":" "},{"type":"text","text":"preventing it from being displayed on the map. Thus you would typically call"},{"type":"text","text":" "},{"type":"text","text":"this method only when you want to hide or delete a given annotation."}],"type":"paragraph"}]}],"metadata":{"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L1820","fileName":"MLNMapView.h"},"roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"removeAnnotation:","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"removeAnnotation:","externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","symbolKind":"method"},"kind":"symbol","hierar