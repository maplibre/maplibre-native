{"schemaVersion":{"minor":3,"major":0,"patch":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"kind":"identifier","text":"removeAnnotation:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"id<MLNAnnotation>","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"kind":"text","text":") "},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}]},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The annotation object to remove. This object must conform","type":"text"},{"text":" ","type":"text"},{"text":"to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol","type":"text"}]}],"name":"annotation"}],"kind":"parameters"},{"content":[{"text":"Discussion","anchor":"discussion","level":2,"type":"heading"},{"inlineContent":[{"text":"Removing an annotation object dissociates it from the map view entirely,","type":"text"},{"text":" ","type":"text"},{"text":"preventing it from being displayed on the map. Thus you would typically call","type":"text"},{"text":" ","type":"text"},{"text":"this method only when you want to hide or delete a given annotation.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","interfaceLanguage":"occ"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"metadata":{"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","roleHeading":"Instance Method","title":"removeAnnotation:","role":"symbol","modules":[{"name":"MapLibre"}],"symbolKind":"method","remoteSource":{"fileName":"MLNMapView.h","url":"http