{"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","metadata":{"symbolKind":"method","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removeAnnotation:"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","title":"removeAnnotation:","roleHeading":"Instance Method","role":"symbol","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"removeAnnotation:","kind":"identifier"}]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"text":"removeAnnotation:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","kind":"typeIdentifier","text":"id<MLNAnnotation>"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"content":[{"inlineContent":[{"type":"text","text":"The annotation object to remove. This object must conform"},{"type":"text","text":" "},{"type":"text","text":"to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol","type":"text"}],"type":"paragraph"}],"name":"annotation"}]},{"content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Removing an annotation object dissociates it from the map view entirely,"},{"text":" ","type":"text"},{"text":"preventing it from being displayed on the map. Thus you would typically call","type":"text"},{"type":"text","text":" "},{"type":"text","text":"this method only when you want to hide or delete a given annotation."}]}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","interfaceLanguage":"occ"},"sections":[],"hierarchy"