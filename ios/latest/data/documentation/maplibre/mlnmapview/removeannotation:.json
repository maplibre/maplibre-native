{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","interfaceLanguage":"occ"},"schemaVersion":{"patch":0,"minor":3,"major":0},"metadata":{"symbolKind":"method","role":"symbol","title":"removeAnnotation:","roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"}]},"abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}],"sections":[],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"text":"removeAnnotation:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"annotation"},{"kind":"text","text":";"}]}]},{"kind":"parameters","parameters":[{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation object to remove. This object must conform"},{"text":" ","type":"text"},{"type":"text","text":"to the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol","type":"text"}]}]}]},{"kind":"content","content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"inlineContent":[{"type":"text","text":"Removing an annotation object dissociates it from the map view entirely,"},{"text":" ","type":"text"},{"text":"prev