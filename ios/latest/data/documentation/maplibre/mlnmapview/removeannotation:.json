{"schemaVersion":{"major":0,"minor":3,"patch":0},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/removeannotation:"]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","interfaceLanguage":"occ"},"metadata":{"title":"removeAnnotation:","symbolKind":"method","modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","role":"symbol","roleHeading":"Instance Method","navigatorTitle":[{"text":"removeAnnotation:","kind":"identifier"}]},"sections":[],"abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}],"kind":"symbol","primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"kind":"text","text":") "},{"text":"removeAnnotation:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"kind":"text","text":";"}]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"The annotation object to remove. This object must conform"},{"text":" ","type":"text"},{"text":"to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol"}],"type":"paragraph"}],"name":"annotation"}],"kind":"parameters"},{"kind":"content","content":[{"level":2,"type":"heading","anchor":"discussion","text":"Discussion"},{"type":"paragrap