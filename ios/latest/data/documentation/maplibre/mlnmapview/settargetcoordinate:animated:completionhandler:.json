{"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:completionHandler:","interfaceLanguage":"occ"},"kind":"symbol","metadata":{"role":"symbol","externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:completionHandler:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setTargetCoordinate:animated:completionHandler:"}],"symbolKind":"method","navigatorTitle":[{"kind":"identifier","text":"setTargetCoordinate:animated:completionHandler:"}],"modules":[{"name":"MapLibre"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L789","fileName":"MLNMapView.h"},"title":"setTargetCoordinate:animated:completionHandler:","roleHeading":"Instance Method"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"setTargetCoordinate:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D"},{"kind":"text","text":") "},{"kind":"internalParam","text":"targetCoordinate"},{"kind":"text","text":" "},{"kind":"identifier","text":"animated:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL","text":"BOOL"},{"kind":"text","text":") "},{"kind":"internalParam","text":"animated"},{"kind":"text","text":" "},{"kind":"identifier","text":"completionHandler:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":" (^)()) "},{"kind":"internalParam","text":"completion"},{"kind":"text","text":";"}],"languages":["occ"],"platforms":["iOS"]}]},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"The target coordinate to fit within the viewport."}],"type":"paragraph"}],"name":"targetCoordinate"}