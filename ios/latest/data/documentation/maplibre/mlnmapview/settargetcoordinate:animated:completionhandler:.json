{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:completionHandler:","interfaceLanguage":"occ"},"metadata":{"role":"symbol","symbolKind":"method","externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:completionHandler:","fragments":[{"text":"- ","kind":"text"},{"text":"setTargetCoordinate:animated:completionHandler:","kind":"identifier"}],"title":"setTargetCoordinate:animated:completionHandler:","roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"setTargetCoordinate:animated:completionHandler:","kind":"identifier"}]},"abstract":[{"text":"Sets the geographic coordinate that is the subject of observation as the user","type":"text"},{"text":" ","type":"text"},{"text":"location is being tracked, with an optional transition animation and completion","type":"text"},{"text":" ","type":"text"},{"text":"handler.","type":"text"}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"setTargetCoordinate:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D"},{"kind":"text","text":") "},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"animated:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier","text":"BOOL"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"animated"},{"text":" ","kind":"text"},{"kind":"identifier","text":"completionHandler:"},{"kind":"text","text":