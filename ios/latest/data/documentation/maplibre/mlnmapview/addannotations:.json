ions:","title":"addAnnotations:","type":"topic","abstract":[{"text":"Adds an array of annotations to the map view.","type":"text"}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotations:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"type":"topic","kind":"symbol","title":"MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"url":"\/documentation\/maplibre\/mlnpolyline","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","kind":"symbol","role":"collection","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"title":"MLNMultiPolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":" object."}],"url":"\/documentation\/maplibre\/mlnmultipolyline","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"url":"\/documentation\/maplibre\/mlnmultipoint","title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference","isActive":true},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"role":"symbol","kind":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","role":"symbol","kind":"symbol","title":"MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview"}}}