content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"style":"note","name":"Note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":""},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":", "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":", and "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" objects"},{"type":"text","text":" "},{"type":"text","text":"cannot be added to the map view at this time. Nor can "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"type":"text","text":"are specified are silently ignored."}]}],"type":"aside"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/addannotations:"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"title":"MLNPolyline","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnpolyline","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":" object."}],"url":"\/documentation\/maplibre\/mlnmultipolyline","title":"MLNMultiPolyline","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotations:":{"kind":"symbol","abstract":[{"text":"Adds an array of annotations to the map view.","type":"text"}],"fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotations:","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:","role":"symbol","navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotations:","title":"addAnnotations:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference"},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","title":"MLNMultiPoint","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","title":"MapLibre"}}}