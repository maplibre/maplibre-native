":"reference","isActive":true},{"type":"text","text":" "},{"text":"objects that are not instances of ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"text":"are specified are silently ignored.","type":"text"}]}]}],"kind":"content"}],"metadata":{"externalID":"c:objc(cs)MLNMapView(im)addAnnotations:","roleHeading":"Instance Method","symbolKind":"method","role":"symbol","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotations:"}],"fragments":[{"kind":"text","text":"- "},{"text":"addAnnotations:","kind":"identifier"}],"title":"addAnnotations:"},"sections":[],"kind":"symbol","variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/addannotations:"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:","interfaceLanguage":"occ"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"schemaVersion":{"patch":0,"minor":3,"major":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"role":"collection","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/