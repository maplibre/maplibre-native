":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","kind":"symbol","title":"MLNMultiPoint","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotations:":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:","title":"addAnnotations:","url":"\/documentation\/maplibre\/mlnmapview\/addannotations:","navigatorTitle":[{"kind":"identifier","text":"addAnnotations:"}],"type":"topic","abstract":[{"type":"text","text":"Adds an array of annotations to the map view."}],"kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotations:"}]}}}