ift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"url":"\/documentation\/maplibre\/mlnmultipolyline","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","title":"MLNMultiPolyline","kind":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":" object."}],"type":"topic","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","title":"MLNMultiPoint"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","type":"topic","url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"}