Content":[{"type":"text","text":""},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":", "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":", and "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":" objects"},{"type":"text","text":" "},{"type":"text","text":"cannot be added to the map view at this time. Nor can "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference","isActive":true},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"type":"text","text":"are specified are silently ignored."}],"type":"paragraph"}]}],"kind":"content"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/addannotations:"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"role":"symbol","title":"MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","type":"topic","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotations:":{"abstract":[{"text":"Adds an array of annotations to the map view.","type":"text"}],"title":"addAnnotations:","navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotations:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotations:","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmultipoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","role":"symbol","type":"topic","kind":"symbol","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"title":"MLNMultiPoint"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","title":"MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","type":"topic","title":"MapLibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"url":"\/documentation\/maplibre\/mlnpolyline","role":"symbol","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"type":"topic","title":"MLNPolyline"}}}