":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotations:":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotations:"}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotations:","navigatorTitle":[{"kind":"identifier","text":"addAnnotations:"}],"title":"addAnnotations:","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:","kind":"symbol","abstract":[{"type":"text","text":"Adds an array of annotations to the map view."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"role":"symbol","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true,"type":"reference"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"type":"topic","title":"MLNMultiPoint","kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"}}}