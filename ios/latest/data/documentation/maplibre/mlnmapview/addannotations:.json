n\/MapLibre\/MLNMultiPoint","isActive":true},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":". Any"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"type":"text","text":" "},{"type":"text","text":"are specified are silently ignored."}],"type":"paragraph"}],"type":"aside","name":"Note"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/addannotations:"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"title":"addAnnotations:","roleHeading":"Instance Method","symbolKind":"method","navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L1808","fileName":"MLNMapView.h"},"role":"symbol","modules":[{"name":"MapLibre"}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addAnnotations:"}],"externalID":"c:objc(cs)MLNMapView(im)addAnnotations:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapVi