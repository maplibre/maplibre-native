ntent":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Removing annotation objects dissociates them from the map view entirely,"},{"text":" ","type":"text"},{"type":"text","text":"preventing them from being displayed on the map. Thus you would typically"},{"type":"text","text":" "},{"text":"call this method only when you want to hide or delete the given annotations.","type":"text"}]}],"kind":"content"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic","kind":"symbol","title":"MapLibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"kind":"symbol","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"title":"MLNMapView","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removeAnnotations:"}],"url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","title":"removeAnnotations:","abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"text":" ","type":"text"},{"text":"annotations in the array.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","role":"symbol","type":"topic","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}]}}}