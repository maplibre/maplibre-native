ifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","interfaceLanguage":"occ"},"metadata":{"externalID":"c:objc(cs)MLNMapView(im)removeAnnotations:","fragments":[{"kind":"text","text":"- "},{"text":"removeAnnotations:","kind":"identifier"}],"symbolKind":"method","role":"symbol","modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"title":"removeAnnotations:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removeAnnotations:"}],"type":"topic","abstract":[{"text":"Removes an array of annotations from the map view, deselecting any selected","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"title":"removeAnnotations:","url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","navigatorTitle":[{"kind":"identifier","text":"removeAnnotations:"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"kind":"identifier","text":"MLNMapView"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","role":"collection","url":"\/documentation\/maplibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","kind":"symbol"}}}