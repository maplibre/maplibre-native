xt":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","title":"removeAnnotations:","kind":"symbol","type":"topic","abstract":[{"text":"Removes an array of annotations from the map view, deselecting any selected","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","role":"symbol","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"fragments":[{"kind":"text","text":"- "},{"text":"removeAnnotations:","kind":"identifier"}]}}}