ontent":[{"anchor":"discussion","type":"heading","text":"Discussion","level":2},{"inlineContent":[{"text":"Removing annotation objects dissociates them from the map view entirely,","type":"text"},{"text":" ","type":"text"},{"text":"preventing them from being displayed on the map. Thus you would typically","type":"text"},{"text":" ","type":"text"},{"text":"call this method only when you want to hide or delete the given annotations.","type":"text"}],"type":"paragraph"}]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/remov