n\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol.","type":"text"}]}],"name":"annotations"}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Removing annotation objects dissociates them from the map view entirely,","type":"text"},{"text":" ","type":"text"},{"text":"preventing them from being displayed on the map. Thus you would typically","type":"text"},{"text":" ","type":"text"},{"text":"call this method only when you want to hide or delete the given annotations.","type":"text"}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"fragments":[{"text":"- ","kind":"text"},{"text":"removeAnnotations:","kind":"identifier"}],"abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"type":"topic","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","title":"removeAnnotations:","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"role":"collection","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text",