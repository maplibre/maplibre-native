ctions":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"kind":"symbol","abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"text":" ","type":"text"},{"type":"text","text":"annotations in the array."}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/removeannotations:"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","title":"removeAnnotations:","url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"kind":"symbol","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"fragments":[{"kind":"text","text":"- "},{"text":"removeAnnotations:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","role":"collection","url":"\/documentation\/maplibre","type":"topic","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","title":"MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"type":"text"