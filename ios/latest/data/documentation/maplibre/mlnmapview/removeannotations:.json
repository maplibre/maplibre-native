{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"primaryContentSections":[{"declarations":[{"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"removeAnnotations:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:Q$objc(cs)NSArray","text":"NSArray<id<MLNAnnotation>>","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"text":"annotations","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"content":[{"type":"paragraph","inlineContent":[{"text":"The array of annotation objects to remove. Objects in the","type":"text"},{"text":" ","type":"text"},{"text":"array must conform to the ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol.","type":"text"}]}],"name":"annotations"}]},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","type":"heading","level":2},{"inlineContent":[{"text":"Removing annotation objects dissociates them from the map view entirely,","type":"text"},{"text":" ","type":"text"},{"text":"preventing them from being displayed on the map. Thus you would typically","type":"text"},{"text":" ","type":"text"},{"text":"call this method only when you want to hide or delete the given annotations.","type":"text"}],"type":"paragraph"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","interfaceLanguage":"occ"},"abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"kind":"symb