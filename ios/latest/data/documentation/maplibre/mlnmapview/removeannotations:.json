nd":"content","content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Removing annotation objects dissociates them from the map view entirely,"},{"text":" ","type":"text"},{"type":"text","text":"preventing them from being displayed on the map. Thus you would typically"},{"type":"text","text":" "},{"type":"text","text":"call this method only when you want to hide or delete the given annotations."}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","title":"MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"text":"provided by Apple’s MapKit.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"removeAnnotations:","kind":"identifier"}],"abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"text":" ","type":"text"},{"text":"annotations in the array.","type":"text"}],"type":"topic","title":"removeAnnotations:","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","role":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]}}}