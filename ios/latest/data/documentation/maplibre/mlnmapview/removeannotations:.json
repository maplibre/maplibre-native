nd":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"text":"Removing annotation objects dissociates them from the map view entirely,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"preventing them from being displayed on the map. Thus you would typically"},{"type":"text","text":" "},{"text":"call this method only when you want to hide or delete the given annotations.","type":"text"}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"type":"text","text":" "},{"text":"annotations in the array.","type":"text"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotations:"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"removeAnnotations:"}],"type":"topic","title":"removeAnnotations:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","kind":"symbol","title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"kind":"identifier","text":"MLNMapView"}],"type":"topic","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"text":"provided by Apple’s MapKit.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","kind":"symbol","role":"symbol"}}}