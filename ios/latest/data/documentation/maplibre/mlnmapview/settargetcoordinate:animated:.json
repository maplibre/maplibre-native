{"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:","interfaceLanguage":"occ"},"metadata":{"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"roleHeading":"Instance Method","navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"role":"symbol","symbolKind":"method","modules":[{"name":"MapLibre"}],"title":"setTargetCoordinate:animated:"},"abstract":[{"type":"text","text":"Deprecated. Sets the geographic coordinate that is the subject of observation as"},{"text":" ","type":"text"},{"type":"text","text":"the user location is being tracked, with an optional transition animation."}],"kind":"symbol","schemaVersion":{"major":0,"minor":3,"patch":0},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"]}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"kind":"text","text":") "},{"kind":"identifier","text":"setTargetCoordinate:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"animated:"},{"text":"(","kind":"text"},{"text":"BOOL","preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"parameters":[{"name":"targetCoordinate","content":[{"type":"paragraph","inlineContent":[{"text":"The target coordinate to fit within the viewport.","type":"text"}]}]},{"name":"animated","content":[{"inlineContent