{"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"]}],"sections":[],"abstract":[{"type":"text","text":"Deprecated. Sets the geographic coordinate that is the subject of observation as"},{"type":"text","text":" "},{"type":"text","text":"the user location is being tracked, with an optional transition animation."}],"schemaVersion":{"patch":0,"minor":3,"major":0},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"text":"void","preciseIdentifier":"c:v","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setTargetCoordinate:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"CLLocationCoordinate2D","preciseIdentifier":"c:@S@CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"animated:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"BOOL","preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"animated","kind":"internalParam"},{"text":";","kind":"text"}]}],"kind":"declarations"},{"parameters":[{"name":"targetCoordinate","content":[{"inlineContent":[{"text":"The target coordinate to fit within the viewport.","type":"text"}],"type":"paragraph"}]},{"name":"animated","content":[{"inlineContent":[{"type":"text","text":"If "},{"type":"codeVoice","code":"YES"},{"type":"text","text":", the map animates to fit the target within the map"},{"type":"text","text":" "},{"type":"text","text":"view. If "},{"type":"codeVoice","code":"NO"},{"type":"text","text"