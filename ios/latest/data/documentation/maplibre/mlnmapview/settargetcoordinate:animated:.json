{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"abstract":[{"type":"text","text":"Deprecated. Sets the geographic coordinate that is the subject of observation as"},{"type":"text","text":" "},{"type":"text","text":"the user location is being tracked, with an optional transition animation."}],"metadata":{"remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L775"},"navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","title":"setTargetCoordinate:animated:","roleHeading":"Instance Method","platforms":[{"deprecated":true,"unavailable":false,"beta":false}],"modules":[{"name":"MapLibre"}],"role":"symbol","symbolKind":"method","fragments":[{"text":"- ","kind":"text"},{"text":"setTargetCoordinate:animated:","kind":"identifier"}]},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setTargetCoordinate:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"animated:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@T@BOOL","text":"BOOL","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"animated","kind":"internalParam"},{"text":";","kind":"text"}]}],"kind":"declarations"},{"parameters":[{"name":"targetCoordinate","content":[{"inlineContent":[{"type":"text","text":"The target coordinate to fit within the viewport."}],"type":"paragraph"}]},{"n