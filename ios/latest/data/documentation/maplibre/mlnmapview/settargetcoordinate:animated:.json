{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"text":"Deprecated. Sets the geographic coordinate that is the subject of observation as","type":"text"},{"text":" ","type":"text"},{"text":"the user location is being tracked, with an optional transition animation.","type":"text"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:","interfaceLanguage":"occ"},"metadata":{"title":"setTargetCoordinate:animated:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","symbolKind":"method","remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L775"},"platforms":[{"unavailable":false,"deprecated":true,"beta":false}]},"kind":"symbol","primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"setTargetCoordinate:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D"},{"kind":"text","text":") "},{"kind":"internalParam","text":"targetCoordinate"},{"kind":"text","text":" "},{"kind":"identifier","text":"animated:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL","text":"BOOL"},{"kind":"text","text":") "},{"kind":"internalParam","text":"animated"},{"kind":"text","text":";"}],"languages":["occ"]}],"kind":