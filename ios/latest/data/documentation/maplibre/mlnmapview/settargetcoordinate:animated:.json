{"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:","interfaceLanguage":"occ"},"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"title":"setTargetCoordinate:animated:","modules":[{"name":"MapLibre"}],"fragments":[{"kind":"text","text":"- "},{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"role":"symbol","roleHeading":"Instance Method","symbolKind":"method"},"abstract":[{"type":"text","text":"Deprecated. Sets the geographic coordinate that is the subject of observation as"},{"text":" ","type":"text"},{"text":"the user location is being tracked, with an optional transition animation.","type":"text"}],"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"text":") ","kind":"text"},{"kind":"identifier","text":"setTargetCoordinate:"},{"kind":"text","text":"("},{"text":"CLLocationCoordinate2D","kind":"typeIdentifier","preciseIdentifier":"c:@S@CLLocationCoordinate2D"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"animated:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"BOOL","preciseIdentifier":"c:@T@BOOL"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"animated"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"kind":"parameters","parameters":[{"name":"targetCoordinate","con