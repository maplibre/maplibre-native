{"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"],"traits":[{"interfaceLanguage":"occ"}]}],"sections":[],"metadata":{"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"roleHeading":"Instance Method","role":"symbol","modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"symbolKind":"method","title":"setTargetCoordinate:animated:"},"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:","interfaceLanguage":"occ"},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"text":"void","preciseIdentifier":"c:v","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setTargetCoordinate:","kind":"identifier"},{"kind":"text","text":"("},{"text":"CLLocationCoordinate2D","kind":"typeIdentifier","preciseIdentifier":"c:@S@CLLocationCoordinate2D"},{"kind":"text","text":") "},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"animated:"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"BOOL","preciseIdentifier":"c:@T@BOOL"},{"kind":"text","text":") "},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The target coordinate to fit within the viewport."}]}],"name":"targetCoordinate"},{"content":[{"type":"paragraph","inlineContent":[{"text":"If ","type":"text"},{"code":"YES","type":"codeVoice"},{"text":", the map animates to fit the target within the map","type":"text"},{"type":"text","text":" "},{"text":"view. If ","type":"text"},{"code":"NO","type":"codeVoice"},{"type":"text","text":"