{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:"},"kind":"symbol","sections":[],"metadata":{"modules":[{"name":"MapLibre"}],"symbolKind":"method","remoteSource":{"fileName":"MLNMapView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapView.h#L765"},"navigatorTitle":[{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"role":"symbol","externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","roleHeading":"Instance Method","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"title":"setTargetCoordinate:animated:","platforms":[{"deprecated":true,"beta":false,"unavailable":false}]},"schemaVersion":{"major":0,"patch":0,"minor":3},"abstract":[{"type":"text","text":"Deprecated. Sets the geographic coordinate that is the subject of observation as"},{"type":"text","text":" "},{"type":"text","text":"the user location is being tracked, with an optional transition animation."}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"],"traits":[{"interfaceLanguage":"occ"}]}],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setTargetCoordinate:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"animated:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c