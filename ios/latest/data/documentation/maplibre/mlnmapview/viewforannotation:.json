notation","content":[{"inlineContent":[{"text":"The annotation associated with the view.","type":"text"},{"text":" ","type":"text"},{"text":"Annotation must conform to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol.","type":"text"}],"type":"paragraph"}]}],"kind":"parameters"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/viewForAnnotation:":{"navigatorTitle":[{"text":"viewForAnnotation:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/viewforannotation:","fragments":[{"text":"- ","kind":"text"},{"text":"viewForAnnotation:","kind":"identifier"}],"kind":"symbol","type":"topic","title":"viewForAnnotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/viewForAnnotation:","role":"symbol","abstract":[{"type":"text","text":"Returns an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" if the given annotation is currently associated"},{"type":"text","text":" "},{"type":"text","text":"with a view, otherwise nil."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"title":"MLNAnnotation","kind":"symbol","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"title":"MLNAnnotationView","kind":"symbol","role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"type":"topic","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview"}}}