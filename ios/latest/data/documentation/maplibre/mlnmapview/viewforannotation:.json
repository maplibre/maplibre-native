meters":[{"name":"annotation","content":[{"inlineContent":[{"type":"text","text":"The annotation associated with the view."},{"type":"text","text":" "},{"type":"text","text":"Annotation must conform to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol."}],"type":"paragraph"}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/viewForAnnotation:":{"abstract":[{"type":"text","text":"Returns an "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" if the given annotation is currently associated"},{"type":"text","text":" "},{"type":"text","text":"with a view, otherwise nil."}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapview\/viewforannotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/viewForAnnotation:","title":"viewForAnnotation:","role":"symbol","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"viewForAnnotation:"}],"navigatorTitle":[{"kind":"identifier","text":"viewForAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","