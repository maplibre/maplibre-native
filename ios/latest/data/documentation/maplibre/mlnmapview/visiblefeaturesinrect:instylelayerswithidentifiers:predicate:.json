ype":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true,"type":"reference"},{"text":" object. Each concrete","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"subclass of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" in turn has a subclass that conforms to this protocol. A","type":"text"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource/featuresMatchingPredicate:":{"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/featuresMatchingPredicate:","title":"featuresMatchingPredicate:","url":"\/documentation\/maplibre\/mlnshapesource\/featuresmatchingpredicate:","role":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"featuresMatchingPredicate:","kind":"identifier"}],"navigatorTitle":[{"text":"featuresMatchingPredicate:","kind":"identifier"}],"abstract":[{"text":"Returns an array of map features for this source, filtered by the given","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"predicate."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"title":"MLNRasterStyleLayer","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","url":"\/documentation\/maplibre\/mlnrasterstylelayer","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer"},{"type":"text","text":" is a style layer that renders georeferenced raster"},{"type":"text","text":" "},{"text":"imagery on the map, especially raster tiles.","type":"text"}],"type":"topic","kind":"symbol","fragments":[{"text":"MLNRasterStyleLayer","kind":"identifier"}],"role":"symbol"