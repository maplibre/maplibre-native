nce"},{"type":"text","text":" or "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"type":"text","text":" object. Each concrete"},{"text":" ","type":"text"},{"type":"text","text":"subclass of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}],"url":"\/documentation\/maplibre\/mlnfeature","role":"symbol","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","title":"MLNFeature","fragments":[{"text":"MLNFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"role":"symbol","type":"topic","fragments":[{"text":"MLNStyle","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"title":"MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:":{"abstract":[{"text":"Returns an array of rendered map features that intersect with the given","type":"text"},{"type":"text","text":" "},{"type":"text","text":"rectangle, restricted to the given style layers and filtered by the given"},{"type":"text","text":" "},{"type":"text","text":"predicate."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","type":"topic","url":"\/documentation\/maplibre\/mlnmapview\/visiblefeaturesinrect:instylelayerswithidentifiers:predicate:","role":"symbol","navigatorTitle":[{"kind":"identifie