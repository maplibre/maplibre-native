 more"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer"},{"type":"text","text":" objects. Use a raster style layer to control the"},{"type":"text","text":" "},{"type":"text","text":"appearance of content supplied by the raster tile source."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","title":"MLNRasterTileSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"title":"MLNFeature","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true,"type":"reference"},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"text":" ","type":"text"},{"type":"text","text":"contained in an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"text":" or ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"type":"text","text":" object. Each concrete"},{"type":"text","text":" "},{"type":"text","text":"subclass of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"text":" in turn has a subclass that conforms to this protocol. A","type":"text"},{"type":"text","text":" "},{"text":"feature object associates a shape with an optional identifier and attributes.","type":"text"}],"role":"symbol","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNFeature"}],"type":"topic","url":"\/documentation\/maplibre\/mlnfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnrasterstylelayer","fragments":[{"text":"ML