type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the"},{"text":" ","type":"text"},{"type":"text","text":"TileJSON specification"},{"text":".","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"A vector tile source is added to an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference"},{"text":" object along with one or more","type":"text"},{"text":" ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","type":"reference"},{"text":" objects. A vector style layer defines the appearance of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnvectortilesource","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"role":"symbol","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"kind":"symbol","title":"MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","type":"topic","fragments":[{"kind":"identifier","text":"MLNStyle"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNFeature"}],"abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature"},{"text":"  protocol is used to provide details about geographic features","type":"text"},{"type":"text","text":" "},{"text":"contained in an ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference"},{"type":"text","text":" or "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"text":" object. Each concrete","type":"text"},{"type":"text","text":" "},{"type":"text","text":"subclass of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","url":"\/documentation\/maplibre\/mlnfeature","title":"MLNFeature","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"text":"provided by Apple’s MapKit.","type":"text"}],"type":"topic","title":"MLNMapView","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","kind":"symbol","type":"topic","role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","type":"reference","isActive":true},{"type":"text","