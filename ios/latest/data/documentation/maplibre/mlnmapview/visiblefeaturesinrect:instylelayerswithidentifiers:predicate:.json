pe":"text"},{"text":"Mapbox Vector Tile","type":"text"},{"text":" format","type":"text"},{"text":" ","type":"text"},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"text":"the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"TileJSON specification"},{"text":".","type":"text"},{"type":"text","text":" "},{"text":"A vector tile source is added to an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"text":" object along with one or more","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true,"type":"reference"},{"text":" objects. A vector style layer defines the appearance of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"url":"\/documentation\/maplibre\/mlnvectortilesource","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","role":"symbol","fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","type":"topic","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource/featuresInSourceLayersWithIdentifiers:predicate:":{"kind":"symbol","role":"symbol","title":"featuresInSourceLayersWithIdentifiers:predicate:","type":"topic","navigatorTitle":[{"text":"featuresInSourceLayersWithIdentifiers:predicate