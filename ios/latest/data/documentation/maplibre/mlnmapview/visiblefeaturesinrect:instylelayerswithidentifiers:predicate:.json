lass of ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":" in turn has a subclass that conforms to this protocol. A","type":"text"},{"type":"text","text":" "},{"text":"feature object associates a shape with an optional identifier and attributes.","type":"text"}],"url":"\/documentation\/maplibre\/mlnfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"kind":"symbol","abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"type":"topic","title":"MLNStyle","role":"symbol","fragments":[{"kind":"identifier","text":"MLNStyle"}],"url":"\/documentation\/maplibre\/mlnstyle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"}}}