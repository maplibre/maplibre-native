{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel"},"metadata":{"symbolKind":"property","fragments":[{"text":"zoomLevel","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)zoomLevel","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"zoomLevel"}],"roleHeading":"Instance Property","role":"symbol","title":"zoomLevel"},"schemaVersion":{"major":0,"patch":0,"minor":3},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"@property"},{"text":" (","kind":"text"},{"kind":"keyword","text":"nonatomic"},{"text":") ","kind":"text"},{"text":"double","preciseIdentifier":"c:d","kind":"typeIdentifier"},{"text":" ","kind":"text"},{"kind":"identifier","text":"zoomLevel"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}]},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"In addition to affecting the visual size and detail of features on the map,"},{"text":" ","type":"text"},{"text":"the zoom level affects the size of the vector tiles that are loaded. At zoom","type":"text"},{"text":" ","type":"text"},{"text":"level 0, each tile covers the entire world map; at zoom level 1, it covers ¼","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"of the world; at zoom level 2, "},{"text":"1","type":"text"},{"text":"⁄","type":"text"},{"type":"text","text":"16"},{"text":" of the world, and","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"so on."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Changing the value of this property updates the map view immediately. If you"},{"text":" ","type":"text"},{"type":"text","text":"want to animate the change, use the "},{"code":"-setZoomLevel:animated:","type":"codeVoice"},{"text":" method instead.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"sections":[],"abstract":[{"text":"The zoom level of the receiver.","type":"text"}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/zoomlevel"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"type":"topic","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"kind":"symbol","fragments":[{"text":"zoomLevel","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","type":"topic","title":"zoomLevel","abstract":[{"type":"text","text":"The zoom level of the receiver."}],"role":"symbol"}}}