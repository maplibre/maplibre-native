Level","interfaceLanguage":"occ"},"metadata":{"role":"symbol","fragments":[{"kind":"identifier","text":"zoomLevel"}],"roleHeading":"Instance Property","title":"zoomLevel","externalID":"c:objc(cs)MLNMapView(py)zoomLevel","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"zoomLevel","kind":"identifier"}],"symbolKind":"property"},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"zoomLevel"}],"title":"zoomLevel","url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","type":"topic","abstract":[{"text":"The zoom level of the receiver.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"fragments":[{"text":"MLNMapView","kind":"identifier"}],"role":"symbol","title":"MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","type":"topic","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]}}}