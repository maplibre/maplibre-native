umentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"type":"text","text":"The zoom level of the receiver."}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","interfaceLanguage":"occ"},"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","role":"symbol","abstract":[{"type":"text","text":"The zoom level of the receiver."}],"title":"zoomLevel","type":"topic","fragments":[{"kind":"identifier","text":"zoomLevel"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","role":"collection","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"type":"topic","title":"MLNMapView","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"kind":"identifier","text":"MLNMapView"}],"kind":"symbol"}}}