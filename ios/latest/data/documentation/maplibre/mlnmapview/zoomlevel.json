{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"type":"text","text":"The zoom level of the receiver."}],"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","interfaceLanguage":"occ"},"schemaVersion":{"patch":0,"minor":3,"major":0},"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"text":"nonatomic","kind":"keyword"},{"text":") ","kind":"text"},{"text":"double","kind":"typeIdentifier","preciseIdentifier":"c:d"},{"kind":"text","text":" "},{"kind":"identifier","text":"zoomLevel"},{"kind":"text","text":";"}],"languages":["occ"],"platforms":["iOS"]}]},{"content":[{"text":"Discussion","type":"heading","anchor":"discussion","level":2},{"type":"paragraph","inlineContent":[{"text":"In addition to affecting the visual size and detail of features on the map,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the zoom level affects the size of the vector tiles that are loaded. At zoom"},{"text":" ","type":"text"},{"text":"level 0, each tile covers the entire world map; at zoom level 1, it covers ¼","type":"text"},{"type":"text","text":" "},{"text":"of the world; at zoom level 2, ","type":"text"},{"type":"text","text":"1"},{"type":"text","text":"⁄"},{"type":"text","text":"16"},{"type":"text","text":" of the world, and"},{"type":"text","text":" "},{"type":"text","text":"so on."}]},{"inlineContent":[{"text":"Changing the value of this property updates the map view immediately. If you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"want to animate the change, use the "},{"code":"-setZoomLevel:animated:","type":"codeVoice"},{"type":"text","text":" method instead."}],"type":"paragraph"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/zoomlevel"]}],"metadata":{"fragments":[{"text":"zoomLevel","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)zoomLevel","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"zoomLevel"}],"roleHeading":"Instance Property","title":"zoomLevel","symbolKind":"property","role":"symbol"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","title":"zoomLevel","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","kind":"symbol","abstract":[{"text":"The zoom level of the receiver.","type":"text"}],"fragments":[{"kind":"identifier","text":"zoomLevel"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"type":"topic","role":"collection","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"type":"topic"}}}