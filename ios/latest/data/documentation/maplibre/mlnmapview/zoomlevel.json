"},{"type":"text","text":"want to animate the change, use the "},{"code":"-setZoomLevel:animated:","type":"codeVoice"},{"type":"text","text":" method instead."}]}]}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/zoomlevel"]}],"abstract":[{"type":"text","text":"The zoom level of the receiver."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","title":"MapLibre","type":"topic","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","title":"MLNMapView","type":"topic","url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","fragments":[{"text":"zoomLevel","kind":"identifier"}],"title":"zoomLevel","kind":"symbol","abstract":[{"type":"text","text":"The zoom level of the receiver."}],"role":"symbol"}}}