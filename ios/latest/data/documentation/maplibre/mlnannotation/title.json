{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"kind":"symbol","schemaVersion":{"minor":3,"major":0,"patch":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"kind":"keyword","text":"nonatomic"},{"kind":"text","text":", "},{"kind":"keyword","text":"copy"},{"kind":"text","text":", "},{"kind":"keyword","text":"readonly"},{"kind":"text","text":") "},{"kind":"typeIdentifier","text":"NSString","preciseIdentifier":"c:objc(cs)NSString"},{"kind":"text","text":" * "},{"kind":"identifier","text":"title"},{"kind":"text","text":";"}]}]},{"kind":"content","content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","text":"Although this property is optional, if you support the selection of annotations"},{"type":"text","text":" "},{"type":"text","text":"in your map view, you are expected to provide this property. This string is"},{"type":"text","text":" "},{"type":"text","text":"displayed in the callout for the associated annotation."}],"type":"paragraph"}]}],"abstract":[{"text":"The string containing the annotation’s title.","type":"text"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","interfaceLanguage":"occ"},"sections":[],"metadata":{"modules":[{"name":"MapLibre"}],"fragments":[{"kind":"identifier","text":"title"}],"roleHeading":"Instance Property","navigatorTitle":[{"kind":"identifier","text":"title"}],"title":"title","role":"symbol","symbolKind":"property","externalID":"c:objc(pl)MLNAnnotation(py)title"},"variants":[{"paths":["\/documentation\/maplibre\/mlnannotation\/title"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProje