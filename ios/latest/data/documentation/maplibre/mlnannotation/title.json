yProject\/documentation\/MapLibre\/MLNAnnotation\/title","abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation\/title","role":"symbol","kind":"symbol","title":"title","fragments":[{"kind":"identifier","text":"title"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","kind":"symbol"}}}