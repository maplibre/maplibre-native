{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"schemaVersion":{"minor":3,"patch":0,"major":0},"sections":[],"abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"@property","kind":"keyword"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"text":", ","kind":"text"},{"text":"copy","kind":"keyword"},{"text":", ","kind":"text"},{"text":"readonly","kind":"keyword"},{"text":") ","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSString","text":"NSString","kind":"typeIdentifier"},{"text":" * ","kind":"text"},{"text":"title","kind":"identifier"},{"text":";","kind":"text"}],"languages":["occ"]}]},{"kind":"content","content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Although this property is optional, if you support the selection of annotations"},{"type":"text","text":" "},{"type":"text","text":"in your map view, you are expected to provide this property. This string is"},{"type":"text","text":" "},{"type":"text","text":"displayed in the callout for the associated annotation."}]}]}],"variants":[{"paths":["\/documentation\/maplibre\/mlnannotation\/title"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","interfaceLanguage":"occ"},"metadata":{"role":"symbol","title":"title","modules":[{"name":"MapLibre"}],"roleHeading":"Instance Property","symbolKind":"property","externalID":"c:objc(pl)MLNAnnotation(py)title","fragments":[{"text":"title","kind":"identifier"}],"navigatorTitle":[{"text":"title","kind":"identifier"}]},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"type":"topic","url":"\/documenta