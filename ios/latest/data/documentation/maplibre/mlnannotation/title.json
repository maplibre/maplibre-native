{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"@property","kind":"keyword"},{"text":" (","kind":"text"},{"kind":"keyword","text":"nonatomic"},{"kind":"text","text":", "},{"kind":"keyword","text":"copy"},{"text":", ","kind":"text"},{"kind":"keyword","text":"readonly"},{"text":") ","kind":"text"},{"preciseIdentifier":"c:objc(cs)NSString","text":"NSString","kind":"typeIdentifier"},{"text":" * ","kind":"text"},{"kind":"identifier","text":"title"},{"text":";","kind":"text"}]}]},{"content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Although this property is optional, if you support the selection of annotations"},{"text":" ","type":"text"},{"type":"text","text":"in your map view, you are expected to provide this property. This string is"},{"type":"text","text":" "},{"text":"displayed in the callout for the associated annotation.","type":"text"}]}],"kind":"content"}],"kind":"symbol","abstract":[{"text":"The string containing the annotation’s title.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotation\/title"]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","interfaceLanguage":"occ"},"metadata":{"role":"symbol","modules":[{"name":"MapLibre"}],"externalID":"c:objc(pl)MLNAnnotation(py)title","roleHeading":"Instance Property","symbolKind":"property","title":"title","fragments":[{"text":"title","kind":"identifier"}],"navigatorTitle":[{"text":"title","kind":"identifier"}]},"sections":[],"schemaVersion":{"patch":0,"major":0,"minor":3},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","abstract":[{"type":"text","text"