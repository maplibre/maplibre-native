{"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"@property","kind":"keyword"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"text":", ","kind":"text"},{"text":"copy","kind":"keyword"},{"text":", ","kind":"text"},{"text":"readonly","kind":"keyword"},{"text":") ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString","text":"NSString"},{"kind":"text","text":" * "},{"text":"title","kind":"identifier"},{"kind":"text","text":";"}]}],"kind":"declarations"},{"kind":"content","content":[{"text":"Discussion","anchor":"discussion","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Although this property is optional, if you support the selection of annotations"},{"type":"text","text":" "},{"type":"text","text":"in your map view, you are expected to provide this property. This string is"},{"text":" ","type":"text"},{"text":"displayed in the callout for the associated annotation.","type":"text"}]}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","interfaceLanguage":"occ"},"sections":[],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotation\/title"]}],"abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"metadata":{"externalID":"c:objc(pl)MLNAnnotation(py)title","symbolKind":"property","fragments":[{"kind":"identifier","text":"title"}],"roleHeading":"Instance Property","modules":[{"name":"MapLibre"}],"title":"title","navigatorTitle":[{"kind":"identifier","text":"title"}],"role":"symbol"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"identifier":"doc:\/\/org.swift.M