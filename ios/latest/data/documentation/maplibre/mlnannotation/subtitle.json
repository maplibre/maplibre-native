tle":"MapLibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/subtitle":{"kind":"symbol","title":"subtitle","abstract":[{"text":"The string containing the annotation’s subtitle.","type":"text"}],"fragments":[{"text":"subtitle","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation\/subtitle","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/subtitle","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]}}}