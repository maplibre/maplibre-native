rl":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/subtitle":{"abstract":[{"text":"The string containing the annotation’s subtitle.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/subtitle","role":"symbol","title":"subtitle","type":"topic","fragments":[{"text":"subtitle","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation\/subtitle"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"kind":"symbol","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol","title":"MLNAnnotation"}}}