bute is 5"},{"type":"text","text":" "},{"type":"text","text":"or 10 and whose "},{"code":"ele","type":"codeVoice"},{"type":"text","text":" attribute is at least 1,500, you could create an"},{"type":"text","text":" "},{"code":"NSCompoundPredicate","type":"codeVoice"},{"type":"text","text":" along these lines:"}],"type":"paragraph"},{"code":["let layer = MLNLineStyleLayer(identifier: \"contour\", source: terrain)","layer.sourceLayerIdentifier = \"contours\"","layer.predicate = NSPredicate(format: \"(index == 5 || index == 10) && CAST(ele, 'NSNumber') >=","1500.0\") mapView.style?.addLayer(layer)"],"syntax":"swift","type":"codeListing"}]}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnvectorstylelayer\/predicate"]}],"metadata":{"title":"predicate","roleHeading":"Instance Property","symbolKind":"property","externalID":"c:objc(cs)MLNVectorStyleLayer(py)predicate","navigatorTitle":[{"kind":"identifier","text":"predicate"}],"modules":[{"name":"MapLibre"}],"fragments":[{"text":"predicate","kind":"identifier"}],"role":"symbol"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorStyleLayer/predicate":{"role":"symbol","title":"predicate","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer\/predicate","abstract":[{"text":"The style layer’s predicate.","type":"text"}],"type":"topic","fragments":[{"text":"predicate","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnvectorstylelayer\/predicate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"fragments":[{"kind":"identifier","text":