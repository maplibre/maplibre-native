stract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","type":"reference"},{"text":" is a style layer that renders georeferenced raster","type":"text"},{"type":"text","text":" "},{"type":"text","text":"imagery on the map, especially raster tiles."}],"title":"MLNRasterStyleLayer","url":"\/documentation\/maplibre\/mlnrasterstylelayer","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","type":"topic","role":"symbol","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNRasterStyleLayer"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle/addSource:":{"abstract":[{"text":"Adds a new source to the current style.","type":"text"}],"title":"addSource:","url":"\/documentation\/maplibre\/mlnstyle\/addsource:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle\/addSource:","type":"topic","role":"symbol","navigatorTitle":[{"text":"addSource:","kind":"identifier"}],"kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"addSource:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource/initWithIdentifier:configurationURL:":{"abstract":[{"text":"Returns a raster tile source initialized with an identifier and configuration","type":"text"},{"type":"text","text":" "},{"type":"text","text":"URL."}],"title":"initWithIdentifier:configurationURL:","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource\/initWithIdentifier:configurationURL:","url":"\/documentation\/maplibre\/mlnrastertilesource\/initwithidentifier:configurationurl:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"initWithIdentifier:configurationURL:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"initWithIdentifier:configurationURL:"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"url":"\/documentation\/maplibre\/mlnrastertilesource","title":"MLNRasterTileSource","type":"topic","fragments":[{"kind":"identifier","text":"MLNRasterTileSource"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","isActive":true,"type":"reference"},{"type":"text","text":" is a map content source that supplies raster image tiles"},{"text":" ","type":"text"},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"text":" ","type":"text"},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"text":" ","type":"text"},{"text":"A raster tile source is added to an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true,"type":"reference"},{"type":"text","text":" objects. Use a raster style layer to control the"},{"text":" ","type":"text"},{"text":"appearance of content supplied by the raster tile source.","type":"text"}],"kind":"symbol","role":"symbol"}}}