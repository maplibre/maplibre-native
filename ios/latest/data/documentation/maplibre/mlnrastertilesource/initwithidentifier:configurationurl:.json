ayer":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnrasterstylelayer","fragments":[{"text":"MLNRasterStyleLayer","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","title":"MLNRasterStyleLayer","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true,"type":"reference"},{"text":" is a style layer that renders georeferenced raster","type":"text"},{"text":" ","type":"text"},{"text":"imagery on the map, especially raster tiles.","type":"text"}],"role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle/addSource:":{"abstract":[{"text":"Adds a new source to the current style.","type":"text"}],"url":"\/documentation\/maplibre\/mlnstyle\/addsource:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle\/addSource:","type":"topic","fragments":[{"text":"- ","kind":"text"},{"text":"addSource:","kind":"identifier"}],"navigatorTitle":[{"text":"addSource:","kind":"identifier"}],"title":"addSource:","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource/initWithIdentifier:configurationURL:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource\/initWithIdentifier:configurationURL:","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnrastertilesource\/initwithidentifier:configurationurl:","fragments":[{"text":"- ","kind":"text"},{"text":"initWithIdentifier:configurationURL:","kind":"identifier"}],"abstract":[{"text":"Returns a raster tile source initialized with an identifier and configuration","type":"text"},{"text":" ","type":"text"},{"text":"URL.","type":"text"}],"title":"initWithIdentifier:configurationURL:","navigatorTitle":[{"text":"initWithIdentifier:configurationURL:","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"title":"MLNRasterTileSource","url":"\/documentation\/maplibre\/mlnrastertilesource","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","isActive":true,"type":"reference"},{"type":"text","text":" is a map content source that supplies raster image tiles"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A raster tile source is added to an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true,"type":"reference"},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true,"type":"reference"},{"type":"text","text":" objects. Use a raster style layer to control the"},{"type":"text","text":" "},{"type":"text","text":"appearance of content supplied by the raster tile source."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","type":"topic","kind":"symbol","fragments":[{"text":"MLNRasterTileSource","kind":"identifier"}],"role":"symbol"}}}