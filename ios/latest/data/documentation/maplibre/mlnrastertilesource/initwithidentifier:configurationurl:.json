ments":[{"text":"MLNRasterStyleLayer","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true},{"type":"text","text":" is a style layer that renders georeferenced raster"},{"type":"text","text":" "},{"type":"text","text":"imagery on the map, especially raster tiles."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNTileSourceOptionTileSize":{"type":"topic","url":"\/documentation\/maplibre\/mlntilesourceoptiontilesize","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing a floating-point number that specifies the"},{"type":"text","text":" "},{"type":"text","text":"width and height (measured in points) at which the map displays each raster"},{"type":"text","text":" "},{"type":"text","text":"image tile when the map’s zoom level is an integer. The raster tile source"},{"type":"text","text":" "},{"type":"text","text":"scales its images up or down when the map’s zoom level falls between two"},{"type":"text","text":" "},{"type":"text","text":"integers."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNTileSourceOptionTileSize","fragments":[{"text":"MLNTileSourceOptionTileSize","kind":"identifier"}],"title":"MLNTileSourceOptionTileSize"}}}