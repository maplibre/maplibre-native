nlineContent":[{"type":"text","text":"A string that uniquely identifies the source in the style to"},{"type":"text","text":" "},{"type":"text","text":"which it is added."}]}]},{"name":"configurationURL","content":[{"type":"paragraph","inlineContent":[{"text":"A URL to a TileJSON configuration file describing the","type":"text"},{"text":" ","type":"text"},{"text":"source’s contents and other metadata.","type":"text"}]}]}]},{"kind":"content","content":[{"text":"Return Value","anchor":"return-value","type":"heading","level":2},{"inlineContent":[{"text":"An initialized raster tile source.","type":"text"}],"type":"paragraph"}]},{"kind":"content","content":[{"text":"Discussion","level":2,"type":"heading","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"After initializing and configuring the source, add it to a map view’s style"},{"type":"text","text":" "},{"type":"text","text":"using the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle\/addSource:","isActive":true},{"type":"text","text":" method."}],"type":"paragraph"},{"inlineContent":[{"text":"The URL may be a full HTTP or HTTPS URL or canonical URL. The URL should","type":"text"},{"text":" ","type":"text"},{"text":"point to a JSON file that conforms to the","type":"text"},{"text":" ","type":"text"},{"text":"TileJSON specification","type":"text"},{"text":".","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"If a Mapbox URL is specified, this source uses a tile size of 256. For all","type":"text"},{"text":" ","type":"text"},{"text":"other tilesets, the default value is 512. (See the","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNTileSourceOptionTileSize","isActive":true},{"text":" documentation for more information about tile","type":"text"},{"text":" ","type":"text"},{"text":"sizes.) If you need to use a tile size other than the default, use the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-initWithIdentifier:configurationURL:tileSize:"},{"text":" method.","type":"text"}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","url":"\/documentation\/maplibre","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle/addSource:":{"kind":"symbol","title":"addSource:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle\/addSource:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"addSource:"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"addSource:"}],"role":"symbol","abstract":[{"type":"text","text":"Adds a new source to the current style."}],"url":"\/documentation\/maplibre\/mlnstyle\/addsource:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"title":"MLNRasterStyleLayer","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","type":"reference"},{"text":" is a style layer that renders georeferenced raster","type":"text"},{"text":" ","type":"text"},{"text":"imagery on the map, especially raster tiles.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","role":"symbol","kind":"symbol","fragments":[{"text":"MLNRasterStyleLayer","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnrasterstylelayer"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","fragments":[{"text":"MLNRasterTileSource","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnrastertilesource","kind":"symbol","role":"symbol","type":"topic","title":"MLNRasterTileSource","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","isActive":true},{"text":" is a map content source that supplies raster image tiles","type":"text"},{"text":" ","type":"text"},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"text":"the","type":"text"},{"text":" ","type":"text"},{"text":"TileJSON specification","type":"text"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"A raster tile source is added to an ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object along with one or more","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true},{"text":" objects. Use a raster style layer to control the","type":"text"},{"text":" ","type":"text"},{"text":"appearance of content supplied by the raster tile source.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"title":"MLNStyle","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","fragments":[{"text":"MLNStyle","kind":"identifier"}],"role":"symbol","abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource/initWithIdentifier:configurationURL:":{"title":"initWithIdentifier:configurationURL:","role":"symbol","navigatorTitle":[{"text":"initWithIdentifier:configurationURL:","kind":"identifier"}],"type":"top