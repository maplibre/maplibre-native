ileSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","kind":"symbol","url":"\/documentation\/maplibre\/mlnrastertilesource","type":"topic","role":"symbol","title":"MLNRasterTileSource","fragments":[{"kind":"identifier","text":"MLNRasterTileSource"}],"abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","isActive":true},{"text":" is a map content source that supplies raster image tiles","type":"text"},{"text":" ","type":"text"},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"text":"the","type":"text"},{"text":" ","type":"text"},{"text":"TileJSON specification","type":"text"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"A raster tile source is added to an ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object along with one or more","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true},{"text":" objects. Use a raster style layer to control the","type":"text"},{"text":" ","type":"text"},{"text":"appearance of content supplied by the raster tile source.","type":"text"}]}}}