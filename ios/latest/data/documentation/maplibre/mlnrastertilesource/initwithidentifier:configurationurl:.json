rceoptiontilesize","kind":"symbol","title":"MLNTileSourceOptionTileSize","fragments":[{"kind":"identifier","text":"MLNTileSourceOptionTileSize"}],"type":"topic","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing a floating-point number that specifies the"},{"type":"text","text":" "},{"type":"text","text":"width and height (measured in points) at which the map displays each raster"},{"type":"text","text":" "},{"type":"text","text":"image tile when the map’s zoom level is an integer. The raster tile source"},{"type":"text","text":" "},{"type":"text","text":"scales its images up or down when the map’s zoom level falls between two"},{"type":"text","text":" "},{"type":"text","text":"integers."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"url":"\/documentation\/maplibre\/mlnstyle","title":"MLNStyle","fragments":[{"kind":"identifier","text":"MLNStyle"}],"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic"}}}