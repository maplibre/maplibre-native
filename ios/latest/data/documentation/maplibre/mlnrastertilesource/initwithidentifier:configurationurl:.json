:"\/documentation\/maplibre\/mlntilesourceoptiontilesize","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNTileSourceOptionTileSize","type":"topic","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing a floating-point number that specifies the"},{"text":" ","type":"text"},{"text":"width and height (measured in points) at which the map displays each raster","type":"text"},{"text":" ","type":"text"},{"text":"image tile when the map’s zoom level is an integer. The raster tile source","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"scales its images up or down when the map’s zoom level falls between two"},{"text":" ","type":"text"},{"type":"text","text":"integers."}],"role":"symbol","fragments":[{"text":"MLNTileSourceOptionTileSize","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"role":"symbol","url":"\/documentation\/maplibre\/mlnrasterstylelayer","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","fragments":[{"kind":"identifier","text":"MLNRasterStyleLayer"}],"title":"MLNRasterStyleLayer","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","type":"reference"},{"type":"text","text":" is a style layer that renders georeferenced raster"},{"type":"text","text":" "},{"type":"text","text":"imagery on the map, especially raster tiles."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterTileSource":{"kind":"symbol","fragments":[{"text":"MLNRasterTileSource","kind":"identifier"}],"title":"MLNRasterTileSource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","url":"\/documentation\/maplibre\/mlnrastertilesource","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentati