eference"},{"type":"text","text":" property in order for any style"},{"text":" ","type":"text"},{"text":"layers that use the source to be redrawn.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"If ","type":"text"},{"type":"codeVoice","code":"count"},{"text":" is greater than the ","type":"text"},{"type":"codeVoice","code":"length"},{"text":" field of ","type":"text"},{"type":"codeVoice","code":"range"},{"type":"text","text":", some vertices will"},{"text":" ","type":"text"},{"type":"text","text":"effectively be inserted into the shape. On the other hand, if "},{"code":"count","type":"codeVoice"},{"type":"text","text":" is less"},{"type":"text","text":" "},{"type":"text","text":"than the "},{"type":"codeVoice","code":"length"},{"text":" field of ","type":"text"},{"code":"range","type":"codeVoice"},{"text":", some vertices will effectively be removed.","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"If ","type":"text"},{"type":"codeVoice","code":"range"},{"text":" extends beyond the shape’s ","type":"text"},{"type":"codeVoice","code":"coordinates"},{"text":" property, an","type":"text"},{"type":"text","text":" "},{"code":"NSRangeException","type":"codeVoice"},{"text":" is raised. If you want to append new vertices to the shape,","type":"text"},{"type":"text","text":" "},{"text":"use the ","type":"text"},{"type":"codeVoice","code":"-appendCoordinates:count:"},{"type":"text","text":" method."}]}],"kind":"content"}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmultipoint\/replacecoordinatesinrange:withcoordinates:count:"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","metadata":{"modules":[{"name":"MapLibre"}],"symbolKind":"method","title":"replaceCoordinatesInRange:withCoordinates:count:","fragments":[{"text":"- ","kind":"text"},{"text":"replaceCoordinatesInRange:withCoordinates:count:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"replaceCoordinates