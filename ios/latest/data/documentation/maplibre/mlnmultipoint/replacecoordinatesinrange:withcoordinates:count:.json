":"paragraph"},{"inlineContent":[{"text":"If ","type":"text"},{"type":"codeVoice","code":"count"},{"type":"text","text":" is greater than the "},{"code":"length","type":"codeVoice"},{"type":"text","text":" field of "},{"type":"codeVoice","code":"range"},{"type":"text","text":", some vertices will"},{"text":" ","type":"text"},{"text":"effectively be inserted into the shape. On the other hand, if ","type":"text"},{"type":"codeVoice","code":"count"},{"text":" is less","type":"text"},{"type":"text","text":" "},{"type":"text","text":"than the "},{"type":"codeVoice","code":"length"},{"text":" field of ","type":"text"},{"code":"range","type":"codeVoice"},{"type":"text","text":", some vertices will effectively be removed."}],"type":"paragraph"},{"inlineContent":[{"text":"If ","type":"text"},{"code":"range","type":"codeVoice"},{"type":"text","text":" extends beyond the shape’s "},{"code":"coordinates","type":"codeVoice"},{"text":" property, an","type":"text"},{"type":"text","text":" "},{"code":"NSRangeException","type":"codeVoice"},{"text":" is raised. If you want to append new vertices to the shape,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"use the "},{"type":"codeVoice","code":"-appendCoordinates:count:"},{"text":" method.","type":"text"}],"type":"paragraph"}]}],"kind":"symbol","schemaVersion":{"patch":0,"major":0,"minor":3},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmultipoint\/replacecoordinatesinrange:withcoordinates:count:"]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint\/replaceCoordinatesInRange:withCoordinates:count:","interfaceLanguage":"occ"},"metadata":{"role":"symbol","externalID":"c:objc(cs)MLNMultiPoint(im)replaceCoordinatesInRange:withCoordinates:count:","symbolKind":"method","modules":[{"name":"MapLibre"}],"title":"replaceCoordinatesInRange:withCoordinates:count:","fragments":[{"kind":"text","text":"- "},{"text":"replaceCoordinatesInRange:with