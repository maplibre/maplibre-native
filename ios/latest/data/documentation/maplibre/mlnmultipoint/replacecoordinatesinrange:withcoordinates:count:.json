{"kind":"symbol","abstract":[{"text":"Replaces the vertices at the given range in the shape with the specified number","type":"text"},{"text":" ","type":"text"},{"text":"of vertices from a given C array.","type":"text"}],"schemaVersion":{"major":0,"patch":0,"minor":3},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"replaceCoordinatesInRange:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@S@_NSRange","text":"NSRange","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"range","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"withCoordinates:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"const","kind":"keyword"},{"text":" ","kind":"text"},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":" *) ","kind":"text"},{"text":"coords","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"count:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@T@NSUInteger","text":"NSUInteger","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"count","kind":"internalParam"},{"text":";","kind":"text"}]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"range","content":[{"type":"paragraph","inlineContent":[{"text":"The range of vertices to replace. The ","type":"text"},{"code":"location","type":"codeVoice"},{"text":" field indicates","type":"text"},{"text":" ","type":"text"},{"text":"the first vertex you are replacing, with ","type":"text"},{"code":"0","type":"codeVoice"},{"text":" being the first vertex, ","type":"text"},{"code":"1","type":"codeVoice"},{"text":" ","type":"text"},{"text":"being the second vertex, and so on. The ","type":"text"},{"code":"length","type":"codeVoice"},{"text":" field indicates the number","type":"text"},{"text"