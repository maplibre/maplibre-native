":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"title":"MLNShape","fragments":[{"text":"MLNShape","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshape"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"title":"MLNMultiPoint","url":"\/documentation\/maplibre\/mlnmultipoint","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"title":"MLNMultiPolygonFeature","url":"\/documentation\/maplibre\/mlnmultipolygonfeature","kind":"symbol","type":"topic","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature"},{"text":" object associates a multipolygon shape with an","type":"text"},{"text":" ","type":"text"},{"text":"optional identifier and attributes.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","role":"symbol","fragments":[{"text":"MLNMultiPolygonFeature","kind":"identif