symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/intersectsOverlayBounds:":{"title":"intersectsOverlayBounds:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"intersectsOverlayBounds:"}],"abstract":[{"text":"Returns a Boolean indicating whether the specified rectangle intersects the","type":"text"},{"type":"text","text":" "},{"text":"receiver’s shape.","type":"text"}],"role":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"intersectsOverlayBounds:"}],"url":"\/documentation\/maplibre\/mlnoverlay\/intersectsoverlaybounds:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/coordinate":{"title":"coordinate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/coordinate","kind":"symbol","fragments":[{"text":"coordinate","kind":"identifier"}],"abstract":[{"type":"text","text":"A coordinate representing the overlay. (required) (read-only)"}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/coordinate"}}}