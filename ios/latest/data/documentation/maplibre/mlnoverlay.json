ifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnoverlay"]}],"topicSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/coordinate","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"],"anchor":"Instance-Properties","title":"Instance Properties","generated":true},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:"],"anchor":"Instance-Methods","title":"Instance Methods","generated":true}],"kind":"symbol","schemaVersion":{"minor":3,"patch":0,"major":0},"metadata":{"externalID":"c:objc(pl)MLNOverlay","navigatorTitle":[{"text":"MLNOverlay","kind":"identifier"}],"symbolKind":"protocol","role":"symbol","title":"MLNOverlay","roleHeading":"Protocol","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"modules":[{"name":"MapLibre"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"kind":"symbol","role":"symbol","type":"topic","title":"MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"role":"symbol","type":"topic","title":"MLNOverlay","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"text":" ","type":"text"},{"text":"take the form of a polyline or polygon.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"url":"\/documentation\/maplibre\/mlnpolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"topic","title":"MLNPolyline","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/coordinate":{"kind":"symbol","title":"coordinate","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/coordinate","abstract":[{"type":"text","text":"A coordinate representing the overlay. (required) (read-only)"}],"fragments":[{"kind":"identifier","text":"coordinate"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/coordinate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnpointcollection","title":"MLNPointCollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/intersectsOverlayBounds:":{"role":"symbol","abstract":[{"type":"text","text":"Returns a Boolean indicating whether the specified rectangle intersects the"},{"type":"text","text":" "},{"type":"text","text":"receiver’s shape."}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/intersectsoverlaybounds:","title":"intersectsOverlayBounds:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"intersectsOverlayBounds:"}],"navigatorTitle":[{"kind":"identifier","text":"intersectsOverlayBounds:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference","isActive":true},{"type":"text","text":" object represents a closed shape consisting of four or more"},{"type":"text","text":" "},{"type":"text","text":"vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the edges that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use a polygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"building, a lake, or an area you want to highlight."}],"title":"MLNPolygon","fragments":[{"kind":"identifier","text":"MLNPolygon"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true,"type":"reference"},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"title":"MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"kind":"symbol","type":"topic","title":"overlayBounds","fragments":[{"text":"overlayBounds","kind":"identifier"}]}}}