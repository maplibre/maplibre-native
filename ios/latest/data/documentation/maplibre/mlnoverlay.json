pe":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"text":" ","type":"text"},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"title":"MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol"}}}