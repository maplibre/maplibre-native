xt"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true,"type":"reference"},{"text":" object, while the surrounding","type":"text"},{"text":" ","type":"text"},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","role":"symbol","title":"MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/intersectsOverlayBounds:":{"url":"\/documentation\/maplibre\/mlnoverlay\/intersectsoverlaybounds:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:","role":"symbol","type":"topic","abstract":[{"type":"text","text":"Returns a Boolean indicating whether the specified rectangle intersects the"},{"type":"text","text":" "},{"type":"text","text":"receiver’s shape."}],"kind":"symbol","title":"intersectsOverlayBounds:","fragments":[{"text":"- ","kind":"text"},{"text":"intersectsOverlayBounds:","kind":"identifier"}],"navigatorTitle":[{"text":"intersectsOverlayBounds:","kind":"identifier"}]}}}