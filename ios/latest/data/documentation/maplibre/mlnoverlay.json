ation\/MapLibre\/MLNOverlay\/coordinate","abstract":[{"text":"A coordinate representing the overlay. (required) (read-only)","type":"text"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay\/coordinate","type":"topic","fragments":[{"text":"coordinate","kind":"identifier"}],"title":"coordinate","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","title":"overlayBounds","kind":"symbol","type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"overlayBounds"}],"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","title":"MapLibre","type":"topic"}}}