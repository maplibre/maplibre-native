ents":[{"kind":"identifier","text":"MLNPointCollection"}],"role":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"url":"\/documentation\/maplibre\/mlnpointcollection","title":"MLNPointCollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"title":"MLNOverlay","role":"symbol","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"kind":"symbol","type":"topic","role":"symbol","title":"MLNPolyline","fragments":[{"text":"MLNPolyline","kind":"id