on\/MapLibre\/MLNPolyline"},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/intersectsOverlayBounds:":{"title":"intersectsOverlayBounds:","abstract":[{"type":"text","text":"Returns a Boolean indicating whether the specified rectangle intersects the"},{"text":" ","type":"text"},{"text":"receiver’s shape.","type":"text"}],"navigatorTitle":[{"text":"intersectsOverlayBounds:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"intersectsOverlayBounds:"}],"url":"\/documentation\/maplibre\/mlnoverlay\/intersectsoverlaybounds:","kind":"symbol","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","url":"\/documentation\/maplibre","type":"topic","kind":"symbol","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"title":"MLNPointCollection","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances. The","type":"text"},{"type":"text","text":" "},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"title":"overlayBounds","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","fragments":[{"text":"overlayBounds","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","kind":"symbol","role":"symbol","type":"topic"}}}