verlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"role":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"type":"text","text":" "},{"text":"orchard.","type":"text"}],"kind":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnpointcollection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","title":"MLNPointCollection"}}}