":"text"},{"type":"text","text":" "},{"type":"text","text":"trace a bus route along city streets. This SDK defines several concrete classes"},{"text":" ","type":"text"},{"type":"text","text":"that conform to this protocol and define standard shapes."}]}]}],"schemaVersion":{"major":0,"patch":0,"minor":3},"metadata":{"navigatorTitle":[{"kind":"identifier","text":"MLNOverlay"}],"title":"MLNOverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"symbolKind":"protocol","roleHeading":"Protocol","role":"symbol","externalID":"c:objc(pl)MLNOverlay","modules":[{"name":"MapLibre"}]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","interfaceLanguage":"occ"},"sections":[],"kind":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"text":" ","type":"text"},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"type":"text","text":" "},{"text":"take the form of a polyline or polygon.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/intersectsOverlayBounds:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:","title":"intersectsOverlayBounds:","url":"\/documentation\/maplibre\/mlnoverlay\/intersectsoverlaybounds:","fragments":[{"text":"- ","kind":"text"},{"text":"intersectsOverlayBounds:","kind":"identifier"}],"type":"topic","role":"symbol","kind":"symbol","abstract":[{"text":"Returns a Boolean indicating whether the specified rectangle intersects the","type":"text"},{"type":"text","text":" "},{"text":"receiver’s shape.","type":"text"}],"navigatorTitle":[