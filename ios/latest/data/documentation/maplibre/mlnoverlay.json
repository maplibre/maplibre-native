{"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"@protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"MLNOverlay","kind":"identifier"},{"text":" <","kind":"text"},{"text":"MLNAnnotation","kind":"typeIdentifier","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","preciseIdentifier":"c:objc(pl)MLNAnnotation"},{"text":">","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"content":[{"anchor":"overview","type":"heading","text":"Overview","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"You use overlays to layer more sophisticated content on top of a map view. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use an overlay to show the boundaries of a national park or"},{"type":"text","text":" "},{"type":"text","text":"trace a bus route along city streets. This SDK defines several concrete classes"},{"type":"text","text":" "},{"type":"text","text":"that conform to this protocol and define standard shapes."}]}],"kind":"content"}],"kind":"symbol","sections":[],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnoverlay"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"relationshipsSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"],"type":"inheritsFrom","kind":"relationships","title":"Inherits From"},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"],"type":"conformingTypes","kind":"relationships","title":"Conforming Types"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre"]]},"topicSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/coordinate","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"],"generated":true,"anchor":"Instance-Properties","title":"Instance Properties"},{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/intersectsOverlayBounds:"],"generated":true,"anchor":"Instance-Methods","title":"Instance Methods"}],"metadata":{"roleHeading":"Protocol","externalID":"c:objc(pl)MLNOverlay","modules":[{"name":"MapLibre"}],"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"MLNOverlay"}],"title":"MLNOverlay","symbolKind":"protocol","fragments":[{"kind":"identifier","text":"MLNOverlay"}]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"url":"\/documentation\/maplibre\/mlnoverlay","role":"symbol","type":"topic","kind":"symbol","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"text":" ","type":"text"},{"text":"take the form of a polyline or polygon.","type":"text"}],"title":"MLNOverlay"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","title":"MLNAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"role":"symbol","title":"MLNMultiPolygon","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"text":" ","type":"text"},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"text":" object, while the surrounding","type":"text"},{"text":" ","type":"text"},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygon"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","title":"MapLibre","type":"topic","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"title":"MLNMultiPolyline","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"text":" object.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"title":"MLNPointCollection","role":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnpointcollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"kind":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNPolygon"}],"url":"\/documentation\/maplibre\/mlnpolygon","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","