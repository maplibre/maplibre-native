xt"},{"text":"annotation’s image, use the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference","isActive":true},{"text":" class.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNImageSource","fragments":[{"text":"MLNImageSource","kind":"identifier"}],"title":"MLNImageSource","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","title":"MapLibre","url":"\/documentation\/maplibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"url":"\/documentation\/maplibre\/mlnannotationimage","fragments":[{"kind":"identifier","text":"MLNAnnotationImage"}],"role":"symbol","type":"topic","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true,"type":"reference"},{"text":" class is responsible for presenting point-based","type":"text"},{"text":" ","type":"text"},{"text":"annotations visually on a map view. Annotation image objects wrap ","type":"text"},{"code":"UIImage","type":"codeVoice"},{"text":" ","type":"text"},{"text":"objects and may be recycled later and put into a reuse queue that is maintained","type":"text"},{"text":" ","type":"text"},{"text":"by the map view.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","title":"MLNAnnotationImage"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource/initWithIdentifier:features:options:":{"title":"initWithIdentifier:features:options:","navigatorTitle":[{"kind":"identifier","text":"initWithIdentifier:features:options:"}],"type":"topic","role":"symbol","kind":"symbol