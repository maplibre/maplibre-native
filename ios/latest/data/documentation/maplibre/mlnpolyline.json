ctive":true},{"text":" object. To group multiple polylines together in one shape, use an","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"text":" or ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","isActive":true},{"text":" object.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"To make the polyline go across the antimeridian or international date line,","type":"text"},{"text":" ","type":"text"},{"text":"specify some longitudes less than −180 degrees or greater than 180 degrees.","type":"text"},{"text":" ","type":"text"},{"text":"For example, a polyline that stretches from Tokyo to San Francisco would have","type":"text"},{"text":" ","type":"text"},{"text":"coordinates of (35.68476, -220.24257) and (37.78428, -122.41310).","type":"text"}],"type":"paragraph"},{"code":["let coordinates = [","    CLLocationCoordinate2D(latitude: 35.68476, longitude: -220.24257),","    CLLocationCoordinate2D(latitude: 37.78428, longitude: -122.41310)","]","let polyline = MLNPolyline(coordinates: coordinates, count: UInt(coordinates.count))"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"A polyline is known as a","type":"text"},{"text":" ","type":"text"},{"text":"LineString","type":"text"},{"text":" ","type":"text"},{"text":"geometry in GeoJSON.","type":"text"}],"type":"paragraph"},{"type":"heading","level":4,"text":"Related examples","anchor":"Related-examples"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/LineOnUserTap","isActive":true}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnimatedLineExample","isActive":true}],"type":"paragraph"}]}]}],"kind":"content"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addOverlay:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addOverlay:","type":"topic","url":"\/documentation\/maplibre\/mlnmapview\/addoverlay:","fragments":[{"text":"- ","kind":"text"},{"text":"addOverlay:","kind":"identifier"}],"kind":"symbol","navigatorTitle":[{"text":"addOverlay:","kind":"identifier"}],"title":"addOverlay:","role":"symbol","abstract":[{"type":"text","text":"Adds a single overlay object to the map."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmultipoint","title":"MLNMultiPoint","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}]},"doc://org.swift.MyProject/documentation/MapL