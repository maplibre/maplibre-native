intCollectionFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","isActive":true},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnpointcollectionfeature","title":"MLNPointCollectionFeature","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},{"type":"text","text":" object associates a polyline shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnpolylinefeature","title":"MLNPolylineFeature","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","isActive":true},{"type":"text","text":" object associates an empty shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnemptyfeature","title":"MLNEmptyFeature","fragments":[{"text":"MLNEmptyFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"role":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"text":" ","type":"text"},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"title":"MLNPointAnnotation","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNLineStyleLayer":{"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","isActive":true,"type":"reference"},{"text":" is a style layer that renders one or more stroked","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"polylines on the map."}],"title":"MLNLineStyleLayer","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","fragments":[{"text":"MLNLineStyleLayer","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnlinestylelayer","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"url":"\/documentation\/maplibre\/mlnoverlay","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identif