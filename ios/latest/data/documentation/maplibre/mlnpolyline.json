lygonfeature","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature"},{"type":"text","text":" object associates a multipolygon shape with an"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"title":"MLNMultiPolygonFeature","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"url":"\/documentation\/maplibre\/mlnshapecollectionfeature","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","fragments":[{"text":"MLNShapeCollectionFeature","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","isActive":true},{"type":"text","text":" object associates a shape collection with an"},{"text":" ","type":"text"},{"type":"text","text":"optional identifier and attributes."}],"title":"MLNShapeCollectionFeature","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorStyleLayer":{"title":"MLNVectorStyleLayer","fragments":[{"kind":"identifier","text":"MLNVectorStyleLayer"}],"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true,"type":"reference"},{"type":"text","text":" is an abstract superclass for style layers whose content"},{"text":" ","type":"text"},{"type":"text","text":"is defined by an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" or ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference","isActive":true},{"type":"text","text":" object."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","role":"symbol","url":"\/documentation\/maplibre\/mlnvectorstylelayer","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addAnnotation:"}],"abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"title":"addAnnotation:","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"url":"\/documentation\/maplibre\/mlnpointannotation","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"type":"text","text":" "},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"title":"MLNPointAnnotation","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"url":"\/documentation\/maplibre\/mlnemptyfeature","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","fragments":[{"kind":"identifier","text":"MLNEmptyFeature"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","isActive":true,"type":"reference"},{"text":" object associates an empty shape with an optional","type":"text"},{"type":"text","text":" "},{"text":"identifier and attributes.","type":"text"}],"title":"MLNEmptyFeature","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"url":"\/documentation\/maplibre\/mlnmultipolygon","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"title":"MLNMultiPolygon","role":"symbol"}}}