MyProject/documentation/MapLibre/MLNVectorStyleLayer":{"fragments":[{"kind":"identifier","text":"MLNVectorStyleLayer"}],"url":"\/documentation\/maplibre\/mlnvectorstylelayer","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","role":"symbol","title":"MLNVectorStyleLayer","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true,"type":"reference"},{"text":" is an abstract superclass for style layers whose content","type":"text"},{"type":"text","text":" "},{"type":"text","text":"is defined by an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" or ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":" object."}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true,"type":"reference"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"text":" ","type":"text"},{"type":"text","text":"composed of multiple vertices."}],"fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"title":"MLNMultiPoint","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","role":"symbol","url":"\/documentation\/maplibre\/mlnmultipoint","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","kind":"symbol","title":"MLNPolyline","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"text":" object represents a shape consisting of two or more v