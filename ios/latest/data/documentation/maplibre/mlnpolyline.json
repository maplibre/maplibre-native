n:":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotation:","kind":"identifier"}],"abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"title":"addAnnotation:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer":{"role":"symbol","url":"\/documentation\/maplibre\/mlnsymbolstylelayer","fragments":[{"kind":"identifier","text":"MLNSymbolStyleLayer"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","type":"reference"},{"text":" is a style layer that renders icon and text labels","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"at points or along lines on the map."}],"title":"MLNSymbolStyleLayer","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"title":"MLNEmptyFeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNEmptyFeature"}],"url":"\/documentation\/maplibre\/mlnemptyfeature","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates an empty shape with an optional"},{"type":"text","text":" "},{"text":"identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"title":"MLNPolylineFeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","kind":"symbol","role":"symbol","fragments":[{"text":"MLNPolylineFeature","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnpolylinefeature","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates a polyline shape with an optional"},{"type":"text","text":" "},{"text":"identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"title":"MLNMultiPolygon","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"text":" ","type":"text"},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"text":" object, while the surrounding","type":"text"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"text":" ","type":"text"},{"type":"text","text":"group of disconnected but related buildings."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"title":"MLNPointAnnotation","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibr