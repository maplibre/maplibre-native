e/MLNShapeCollection":{"type":"topic","fragments":[{"kind":"identifier","text":"MLNShapeCollection"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","url":"\/documentation\/maplibre\/mlnshapecollection","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"reference"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"text":" ","type":"text"},{"type":"text","text":"distinct but related shapes that are instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":". The constituent"},{"type":"text","text":" "},{"text":"shapes can be a mixture of different kinds of shapes.","type":"text"}],"kind":"symbol","title":"MLNShapeCollection"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnimatedLineExample":{"title":"Animated Line","type":"topic","abstract":[{"text":"Add an animated line to a map","type":"text"}],"role":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnimatedLineExample","kind":"article","url":"\/documentation\/maplibre-native-for-ios\/animatedlineexample"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","url":"\/documentation\/maplibre\/mlnshape","title":"MLNShape","fragments":[{"text":"MLNShape","kind":"identifier"}],"abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","title":"MLNMultiPolyline","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmultipolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","title":"MLNPointCollection","kind":"symbol","role":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnpointcollection","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances. The","type":"text"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"orchard."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:lineWidthForPolylineAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:lineWidthForPolylineAnnotation:","title":"mapView:lineWidthForPolylineAnnotation:","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:lineWidthForPolylineAnnotation:","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:lineWidthForPolylineAnnotation:"}],"type":"topic","abstract":[{"text":"Returns the line width in points to use when rendering the outline of a","type":"text"},{"type":"text","text":" "},{"text":"polyline annotation.","type":"text"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:linewidthforpolylineannotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"url":"\/documentation\/maplibre\/mlnstyle","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"topic","fragments":[{"text":"MLNStyle","kind":"identifier"}],"title":"MLNStyle","role":"symbol","abstract":[{"text":"The proxy object for the current map style.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addOverlay:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addOverlay:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addOverlay:"}],"title":"addOverlay:","navigatorTitle":[{"text":"addOverlay:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/addoverlay:","kind":"symbol","abstract":[{"text":"Adds a single overlay object to the map.","type":"text"}],"type":"topic","role":"symbol"}}}