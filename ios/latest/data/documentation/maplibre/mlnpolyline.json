oject/documentation/MapLibre/MLNPointAnnotation":{"abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"text":" ","type":"text"},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","kind":"symbol","role":"symbol","title":"MLNPointAnnotation","type":"topic","url":"\/documentation\/maplibre\/mlnpointannotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"text":" ","type":"text"},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"type":"text","text":" "},{"text":"group of disconnected but related buildings.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmultipolygon","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"role":"symbol","title":"MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object represents a closed shape consisting of four or more"},{"type":"text","text":" "},{"type":"text","text":"vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the edges that"},{"type":"text","text":" "},{"text":"connect them. For example, you could use a polygon shape to represent a","type":"text"},{"type":"text","text":" "},{"text":"building, a lake, or an area you want to highlight.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNPolygon"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","kind":"symbol","role":"symbol","title":"MLNPolygon","type":"topic","url":"\/documentation\/maplibre\/mlnpolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNLineStyleLayer":{"title":"MLNLineStyleLayer","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer"},{"type":"text","text":" is a style layer that renders one or more stroked"},{"type":"text","text":" "},{"type":"text","text":"polylines on the map."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","type":"topic","url":"\/documentation\/maplibre\/mlnlinestylelayer","role":"symbol","fragments":[{"text":"MLNLineStyleLayer","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":","},{"text":" ","type":"text"},{"text":"or they may be defined by local or external","type":"text"},{"type":"text","text":" "},{"text":"GeoJSON","type":"text"},{"type":"text","text":" code. A shape source is added to an"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference"},{"text":" object. The vector style","type":"text"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"text":"can update a shape source by setting its ","type":"text"},{"type":"codeVoice","code":"shape"},{"type":"text","text":" or "},{"type":"codeVoice","code":"URL"},{"type":"text","text":" property."}],"title":"MLNShapeSource","type":"topic","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","url":"\/documentation\/maplibre\/mlnshapesource","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","type":"reference","isActive":true},{"text":" object associates a point collection with an","type":"text"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"url":"\/documentation\/maplibre\/mlnpointcollectionfeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}],"role":"symbol","title":"MLNPointCollectionFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addOverlay:":{"fragments":[{"text":"- ","kind":"text"},{"text":"addOverlay:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/addoverlay:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addOverlay:","kind":"symbol","role":"symbol","type":"topic","abstract":[{"type":"text","text":"Adds a single overlay object to the map."}],"navigatorTitle":[{"kind":"identifier","text":"addOverlay:"}],"title":"addOverlay:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer":{"title":"MLNSymbolStyleLayer","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer"},{"type":"text","text":" is a style layer that renders icon and text labels"},{"type":"text","text":" "},{"type":"text","text":"at points or along lines on the map."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","type":"topic","url":"\/documentation\/maplibre\/mlnsymbolstylelayer","role":"symbol","fragments":[{"kind":"identifier","text":"MLNSymbolStyleLayer"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"text":" ","type":"text"},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}],"fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","kind":"symbol","role":"symbol","title":"MLNMultiPolyline","type":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/LineOnUserTap":{"title":"Add Line on User Tap","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/LineOnUserTap","url":"\/documentation\/maplibre-native-for-ios\/lineonusertap","abstract":[{"type":"text","text":"Demonstrating adding "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"text":" annotations and responding to user input.","type":"text"}],"role":"article","kind":"article","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","role":"collection"}}}