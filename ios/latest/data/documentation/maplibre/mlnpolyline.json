ragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","kind":"symbol","url":"\/documentation\/maplibre\/mlnpolylinefeature","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","type":"reference"},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollection"}],"kind":"symbol","title":"MLNShapeCollection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","url":"\/documentation\/maplibre\/mlnshapecollection","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"reference"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"type":"text","text":" "},{"type":"text","text":"distinct but related shapes that are instances of "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference"},{"type":"text","text":". The constituent"},{"type":"text","text":" "},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"fragments":[{"text":"MLNPolyline","kind":"identifier"}],"title":"MLNPolyline","url":"\/documentation\/maplibre\/mlnpolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"topic","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"fragments":[{"kind":"identifier","text":"MLNMultiPolylineFeature"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolylinefeature","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature"},{"type":"text","text":" object associates a multipolyline shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","role":"symbol","type":"topic","title":"MLNMultiPolylineFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"type":"topic","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"text":" is a map content source that supplies vector shapes to be","type":"text"},{"text":" ","type":"text"},{"text":"shown on the map. The shapes may be instances of ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"text":" or ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"text":",","type":"text"},{"text":" ","type":"text"},{"text":"or they may be defined by local or external","type":"text"},{"text":" ","type":"text"},{"text":"GeoJSON","type":"text"},{"text":" code. A shape source is added to an","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object along with an ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object. The vector style","type":"text"},{"text":" ","type":"text"},{"text":"layer defines the appearance of any content supplied by the shape source. You","type":"text"},{"text":" ","type":"text"},{"text":"can update a shape source by setting its ","type":"text"},{"type":"codeVoice","code":"shape"},{"text":" or ","type":"text"},{"type":"codeVoice","code":"URL"},{"text":" property.","type":"text"}],"url":"\/documentation\/maplibre\/mlnshapesource","role":"symbol","title":"MLNShapeSource","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"url":"\/documentation\/maplibre\/mlnpolygon","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","fragments":[{"text":"MLNPolygon","kind":"identifier"}],"type":"topic","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference"},{"text":" object represents a closed shape consisting of four or more","type":"text"},{"text":" ","type":"text"},{"text":"vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the edges that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use a polygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"building, a lake, or an area you want to highlight.","type":"text"}],"role":"symbol","title":"MLNPolygon"}}}