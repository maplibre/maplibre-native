},"schemaVersion":{"major":0,"minor":3,"patch":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"@interface"},{"kind":"text","text":" "},{"kind":"identifier","text":"MLNPolyline"},{"kind":"text","text":" : "},{"preciseIdentifier":"c:objc(cs)MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","kind":"typeIdentifier","text":"MLNMultiPoint"}],"languages":["occ"]}]},{"kind":"content","content":[{"type":"heading","anchor":"overview","level":2,"text":"Overview"},{"type":"paragraph","inlineContent":[{"text":"You can add polyline shapes to the map by adding them to an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" ","type":"text"},{"text":"object. Configure the appearance of an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":"’s or","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true,"type":"reference"},{"text":"’s polylines collectively using an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","isActive":true,"type":"reference"},{"text":" or","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","isActive":true,"type":"reference"},{"text":" object. To access a polyline’s attributes, use an","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","isActive":true,"type":"reference"},{"text":" object.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Alternatively, you can add a polyline overlay directly to a map view using the"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:"},{"type":"text","text":" or "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addOverlay:"},{"type":"text","text":" methods. Configure"},{"type":"text","text":" "},{"type":"text","text":"a polyline overlay’s appearance using"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:strokeColorForShapeAnnotation:"},{"type":"text","text":" and"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:lineWidthForPolylineAnnotation:"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The vertices are automatically connected in the order in which you provide"},{"type":"text","text":" "},{"type":"text","text":"them. The first and last vertices are not connected to each other, but you can"},{"type":"text","text":" "},{"type":"text","text":"specify the same "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" as the first and last vertices in"},{"type":"text","text":" "},{"type":"text","text":"order to close the polyline. To fill the space within the shape, use an"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object. To group multiple polylines together in one shape, use an"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","isActi