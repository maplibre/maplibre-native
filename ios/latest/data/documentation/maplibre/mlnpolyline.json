type":"text"}],"role":"symbol","title":"MLNVectorTileSource","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"title":"MLNMultiPolyline","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","abstract":[{"text":"An ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}],"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"title":"MLNPolylineFeature","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},{"type":"text","text":" object associates a polyline shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"type":"topic","url":"\/documentation\/maplibre\/mlnpolylinefeature","kind":"symbol"}}}