","abstract":[{"type":"text","text":"Label placement relative to its geometry."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/symbolPlacement":{"kind":"symbol","fragments":[{"kind":"identifier","text":"symbolPlacement"}],"title":"symbolPlacement","abstract":[{"text":"Label placement relative to its geometry.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/symbolPlacement","role":"symbol","url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/symbolplacement","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnsymbolstylelayer","fragments":[{"text":"MLNSymbolStyleLayer","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","isActive":true},{"text":" is a style layer that renders icon and text labels","type":"text"},{"type":"text","text":" "},{"type":"text","text":"at points or along lines on the map."}],"title":"MLNSymbolStyleLayer","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","title":"MapLibre","kind":"symbol","url":"\/documentation\/maplibre"}}}