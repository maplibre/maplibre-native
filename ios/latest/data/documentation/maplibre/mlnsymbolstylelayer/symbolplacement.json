{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnsymbolstylelayer\/symbolplacement"]}],"sections":[],"abstract":[{"type":"text","text":"Label placement relative to its geometry."}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/symbolPlacement","interfaceLanguage":"occ"},"kind":"symbol","metadata":{"externalID":"c:objc(cs)MLNSymbolStyleLayer(py)symbolPlacement","title":"symbolPlacement","fragments":[{"kind":"identifier","text":"symbolPlacement"}],"navigatorTitle":[{"kind":"identifier","text":"symbolPlacement"}],"roleHeading":"Instance Property","modules":[{"name":"MapLibre"}],"role":"symbol","symbolKind":"property"},"schemaVersion":{"patch":0,"major":0,"minor":3},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"kind":"keyword","text":"nonatomic"},{"kind":"text","text":") "},{"kind":"typeIdentifier","text":"NSExpression","preciseIdentifier":"c:objc(cs)NSExpression"},{"kind":"text","text":" * "},{"kind":"identifier","text":"symbolPlacement"},{"kind":"text","text":";"}],"languages":["occ"]}]},{"kind":"content","content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"inlineContent":[{"type":"text","text":"The default value of this property is an expression that evaluates to "},{"type":"codeVoice","code":"point"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"Set this property to "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" to reset it to the default value."}],"type":"paragraph"},{"inlineContent":[{"text":"You can set this property to an expression containing any of the following:","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Constant "},{"type":"codeVoice","code":"MLNSymbolPlacement"},{"type":"text","text":" values"},{"type":"text","text":" "},{"type":"text","text":"Any of the following constant string values:"},{"type":"text","text":" "},{"type":"codeVoice","code":"point"},{"type":"text","text":": The label is placed at the point where the geometry is located."},{"type":"text","text":" "},{"type":"codeVoice","code":"line"},{"type":"text","text":": The label is placed along the line of the geometry. Can only be"},{"type":"text","text":" "},{"type":"text","text":"used on "},{"type":"codeVoice","code":"LineString"},{"type":"text","text":" and "},{"type":"codeVoice","code":"Polygon"},{"type":"text","text":" geometries."},{"type":"text","text":" "},{"type":"codeVoice","code":"line-center"},{"type":"text","text":": The label is placed at the center of the line of the"},{"type":"text","text":" "},{"type":"text","text":"geometry. Can only be used on "},{"type":"codeVoice","code":"LineString"},{"type":"text","text":" and "},{"type":"codeVoice","code":"Polygon"},{"type":"text","text":" geometries. Note that"},{"type":"text","text":" "},{"type":"text","text":"a single feature in a vector tile may contain multiple line geometries."},{"type":"text","text":" "},{"type":"text","text":"Predefined functions, including mathematical and string operators"},{"type":"text","text":" "},{"type":"text","text":"Conditional expressions"},{"type":"text","text":" "},{"type":"text","text":"Variable assignments and references to assigned variables"},{"type":"text","text":" "},{"type":"text","text":"Step functions applied to the "},{"type":"codeVoice","code":"$zoomLevel"},{"type":"text","text":" variable"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"This property does not support applying interpolation functions to the"},{"type":"text","text":" "},{"code":"$zoomLevel","type":"codeVoice"},{"type":"text","text":" variable or applying interpolation or step functions to feature"},{"type":"text","text":" "},{"type":"text","text":"attributes."}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer":{"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","isActive":true,"type":"reference"},{"text":" is a style layer that renders icon and text labels","type":"text"},{"text":" ","type":"text"},{"text":"at points or along lines on the map.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNSymbolStyleLayer"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnsymbolstylelayer","title":"MLNSymbolStyleLayer"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/symbolPlacement":{"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/symbolPlacement","abstract":[{"text":"Label placement relative to its geometry.","type":"text"}],"url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/symbolplacement","role":"symbol","title":"symbolPlacement","fragments":[{"kind":"identifier","text":"symbolPlacement"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","url":"\/documentation\/maplibre","type":"topic","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol"}}}