/org.swift.MyProject/documentation/MapLibre/MLNAnnotation/subtitle":{"type":"topic","fragments":[{"text":"subtitle","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation\/subtitle","abstract":[{"text":"The string containing the annotation’s subtitle.","type":"text"}],"role":"symbol","kind":"symbol","title":"subtitle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/subtitle"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","fragments":[{"kind":"identifier","text":"MLNShape"}],"url":"\/documentation\/maplibre\/mlnshape","kind":"symbol","type":"topic","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"title":"MLNShape"},"doc://org.swift.MyProject/objc(pl)NSObject":{"title":"NSObject","identifier":"doc:\/\/org.swift.MyProject\/objc(pl)NSObject","type":"unresolvable"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","fragments":[{"text":"MLNUserLocation","kind":"identifier"}],"title":"MLNUserLocation","abstract":[{"type":"text","text":"The MLNUserLocation class defines a specific type of annotation that identifies"},{"type":"text","text":" "},{"type":"text","text":"the user’s current location. You do not create instances of this class"},{"type":"text","text":" "},{"type":"text","text":"directly. Instead, you retrieve an existing "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"type":"text","text":" object from the"},{"type":"text","text":" "},{"type":"codeVoice","code":"userLocation"},{"type":"text","text":" property of the map view displayed in your application."}],"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnuserlocation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"type":"topic","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"title":"MLNOverlay","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"type":"topic","fragments":[{"text":"title","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation\/title","title":"title","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"kind":"symbol","role":"symbol","fragments":[{"text":"MLNFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"type":"text","text":" "},{"type":"text","text":"contained in an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"type":"text","text":" or "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" object. Each concrete"},{"type":"text","text":" "},{"type":"text","text":"subclass of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}],"title":"MLNFeature","url":"\/documentation\/maplibre\/mlnfeature","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/M