 of"},{"text":" ","type":"text"},{"type":"text","text":"any content supplied by the vector tile source."}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"title":"MLNVectorTileSource","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/coordinate":{"title":"coordinate","abstract":[{"text":"The center point (specified as a map coordinate) of the annotation. (required)","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"(read-only)"}],"kind":"symbol","fragments":[{"text":"coordinate","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation\/coordinate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/coordinate","type":"topic"}}}