ectorTileSource":{"type":"topic","role":"symbol","title":"MLNVectorTileSource","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true,"type":"reference"},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"text":"in ","type":"text"},{"text":"Mapbox Vector Tile","type":"text"},{"type":"text","text":" format"},{"text":" ","type":"text"},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"type":"text","text":" "},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"text":"the","type":"text"},{"type":"text","text":" "},{"text":"TileJSON specification","type":"text"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"A vector tile source is added to an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true,"type":"reference"},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer"},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","url":"\/documentation\/maplibre\/mlnvectortilesource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/coordinate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/coordinate","role":"symbol","title":"coordinate","fragments":[{"kind":"identifier","text":"coordinate"}],"abstract":[{"type":"text","text":"The center point (specified as a map coordinate) of the annotation. (required)"},{"type":"text","text":" "},{"text":"(read-only)","type":"text"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotation\/coordinate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"fragments":[{"text":"title","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation\/title","abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"title":"title"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNUserLocation"}],"type":"topic","abstract":[{"type":"text","text":"The MLNUserLocation class defines a specific type of annotation that identifies"},{"text":" ","type":"text"},{"type":"text","text":"the user’s current location. You do not create instances of this class"},{"type":"text","text":" "},{"type":"text","text":"directly. Instead, you retrieve an existing "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true},{"text":" object from the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"userLocation"},{"type":"text","text":" property of the map view displayed in your application."}],"title":"MLNUserLocation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","url":"\/documentation\/maplibre\/mlnuserlocation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol","title":"MLNOverlay","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/subtitle":{"fragments":[{"text":"subtitle","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/subtitle","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation\/subtitle","abstract":[{"type":"text","text":"The string containing the annotation’s subtitle."}],"title":"subtitle"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/objc(pl)NSObject":{"type":"unresolvable","title":"NSObject","identifier":"doc:\/\/org.swift.MyProject\/objc(pl)NSObject"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","kind":"symbol","url":"\/documentation\/maplibre\/mlnshape","fragments":[{"kind":"identifier","text":"MLNShape"}],"role":"symbol","title":"MLNShape","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text