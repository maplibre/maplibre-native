shape","fragments":[{"text":"MLNShape","kind":"identifier"}],"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"type":"text","text":" "},{"text":"the content that forms the base map.","type":"text"}],"title":"MLNShape","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"role":"symbol","kind":"symbol","title":"title","fragments":[{"text":"title","kind":"identifier"}],"abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation\/title","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title"},"doc://org.swift.MyProject/objc(pl)NSObject":{"identifier":"doc:\/\/org.swift.MyProject\/objc(pl)NSObject","type":"unresolvable","title":"NSObject"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/coordinate":{"role":"symbol","kind":"symbol","title":"coordinate","fragments":[{"kind":"identifier","text":"coordinate"}],"abstract":[{"type":"text","text":"The center point (specified as a map coordinate) of the annotation. (required)"},{"type":"text","text":" "},{"text":"(read-only)","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation\/coordinate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/coordinate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/subtitle":{"role":"symbol","kind":"symbol","title":"subtitle","fragments":[{"text":"subtitle","kind":"identifier"}],"abstract":[{"text":"The string containing the annotation’s subtitle