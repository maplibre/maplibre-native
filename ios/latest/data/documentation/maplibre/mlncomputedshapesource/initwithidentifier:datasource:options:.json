Project\/documentation\/MapLibre\/MLNShapeSourceOptionWrapsCoordinates","url":"\/documentation\/maplibre\/mlnshapesourceoptionwrapscoordinates","fragments":[{"text":"MLNShapeSourceOptionWrapsCoordinates","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionSimplificationTolerance":{"title":"MLNShapeSourceOptionSimplificationTolerance","fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionSimplificationTolerance"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionsimplificationtolerance","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionSimplificationTolerance","kind":"symbol","type":"topic","role":"symbol","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing a double; specifies the Douglas-Peucker","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"simplification tolerance. A greater value produces simpler geometries and"},{"type":"text","text":" "},{"type":"text","text":"improves performance. The default value is 0.375."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMinimumZoomLevel":{"title":"MLNShapeSourceOptionMinimumZoomLevel","fragments":[{"text":"MLNShapeSourceOptionMinimumZoomLevel","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionminimumzoomlevel","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMinimumZoomLevel","role":"symbol","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the minimum zoom level at"},{"text":" ","type":"text"},{"type":"text","text":"which to create vector tiles. The default value is 0."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionBuffer":{"title":"MLNShapeSourceOptionBuffer","fragments":[{"text":"MLNShapeSourceOptionBuffer","kind":"iden