umentation\/maplibre\/mlnshapesourceoptionminimumzoomlevel","kind":"symbol","role":"symbol","type":"topic","title":"MLNShapeSourceOptionMinimumZoomLevel","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMinimumZoomLevel","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing an integer; specifies the minimum zoom level at","type":"text"},{"text":" ","type":"text"},{"text":"which to create vector tiles. The default value is 0.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionSimplificationTolerance":{"abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"NSNumber"},{"text":" object containing a double; specifies the Douglas-Peucker","type":"text"},{"text":" ","type":"text"},{"text":"simplification tolerance. A greater value produces simpler geometries and","type":"text"},{"text":" ","type":"text"},{"text":"improves performance. The default value is 0.375.","type":"text"}],"title":"MLNShapeSourceOptionSimplificationTolerance","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionSimplificationTolerance"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionsimplificationtolerance","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionSimplificationTolerance"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionWrapsCoordinates":{"type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionWrapsCoordinates","ur