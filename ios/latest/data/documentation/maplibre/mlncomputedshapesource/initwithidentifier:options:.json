Project\/documentation\/MapLibre\/MLNShapeSourceOptionWrapsCoordinates","fragments":[{"text":"MLNShapeSourceOptionWrapsCoordinates","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionwrapscoordinates","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionSimplificationTolerance":{"kind":"symbol","fragments":[{"text":"MLNShapeSourceOptionSimplificationTolerance","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnshapesourceoptionsimplificationtolerance","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionSimplificationTolerance","title":"MLNShapeSourceOptionSimplificationTolerance","abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing a double; specifies the Douglas-Peucker"},{"type":"text","text":" "},{"type":"text","text":"simplification tolerance. A greater value produces simpler geometries and"},{"type":"text","text":" "},{"type":"text","text":"improves performance. The default value is 0.375."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionBuffer":{"abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"NSNumber"},{"text":" object containing an integer; specifies the size of the tile","type":"text"},{"text":" ","type":"text"},{"text":"buffer on each side. A value of 0 produces no buffer. A value of 512 produces a","type":"text"},{"text":" ","type":"text"},{"text":"buffer as wide as the tile itself. Larger values produce fewer rendering","type":"text"},{"text":" ","type":"text"},{"text":"artifacts near tile edges and slower performance. The default value is 128.","type":"text"}],"kind":"symbol","role":"symbol","fragments":[{"text":"MLNShapeSourceOptionBuffer","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionBuffer","url":"\/documentation\/maplibre\/mlnshapesourceoptionbuffer","title":"MLNShapeSourceOptionBuffer"}}}