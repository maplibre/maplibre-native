ole":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection/pointCollectionWithCoordinates:count:":{"role":"symbol","title":"pointCollectionWithCoordinates:count:","kind":"symbol","abstract":[{"text":"Creates and returns a ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object from the specified set of","type":"text"},{"text":" ","type":"text"},{"text":"coordinates.","type":"text"}],"url":"\/documentation\/maplibre\/mlnpointcollection\/pointcollectionwithcoordinates:count:","type":"topic","navigatorTitle":[{"text":"pointCollectionWithCoordinates:count:","kind":"identifier"}],"fragments":[{"text":"+ ","kind":"text"},{"text":"pointCollectionWithCoordinates:count:","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection\/pointCollectionWithCoordinates:count:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection/coordinates":{"kind":"symbol","type":"topic","title":"coordinates","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection\/coordinates","role":"symbol","abstract":[{"text":"The array of coordinates associated with the shape.","type":"text"}],"fragments":[{"kind":"identifier","text":"coordinates"}],"url":"\/documentation\/maplibre\/mlnpointcollection\/coordinates"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon","role":"symbol","title":"MLNMultiPolygon","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference","isActive":true},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"type":"topic"}}}