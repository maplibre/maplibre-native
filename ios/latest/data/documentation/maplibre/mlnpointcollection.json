w zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"kind":"symbol","type":"topic","role":"symbol","title":"MLNPointAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnshapecollectionfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","isActive":true},{"type":"text","text":" object associates a shape collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"role":"symbol","fragments":[{"text":"MLNShapeCollectionFeature","kind":"identifier"}],"title":"MLNShapeCollectionFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},{"type":"text","text":" object associates a polyline shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"type":"topic","url":"\/documentation\/maplibre\/mlnpolylinefeature","role":"symbol","title":"MLNPolylineFeature","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","abstract":[{"t