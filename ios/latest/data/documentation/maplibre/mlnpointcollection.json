ultiPoint":{"title":"MLNMultiPoint","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection/pointCollectionWithCoordinates:count:":{"fragments":[{"kind":"text","text":"+ "},{"kind":"identifier","text":"pointCollectionWithCoordinates:count:"}],"kind":"symbol","abstract":[{"type":"text","text":"Creates and returns a "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"reference","isActive":true},{"type":"text","text":" object from the specified set of"},{"type":"text","text":" "},{"type":"text","text":"coordinates."}],"navigatorTitle":[{"kind":"identifier","text":"pointCollectionWithCoordinates:count:"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection\/pointCollectionWithCoordinates:count:","url":"\/documentation\/maplibre\/mlnpointcollection\/pointcollectionwithcoordinates:count:","title":"pointCollectionWithCoordinates:count:","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorStyleLayer":{"type":"topic","title":"MLNVectorStyleLayer","kind":"symbol","fragments":[{"text":"MLNVectorStyleLayer","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnvectorstylelayer","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","role":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","type":"reference","isActive":true},{"type":"text","text":" is an abstract superclass for style layers whose content"},{"type":"text","text":" "},{"type":"text","text":"is defined by an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference","isActive":true},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference","isActive":true},{"type":"text","text":" object."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"fragments":[{"text":"MLNStyle","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnstyle","kind":"symbol","title":"MLNStyle","abstract":[{"type":"text","text":"The proxy object for the current map style."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"url":"\/documentation\/maplibre\/mlnpolyline","kind":"symbol","title":"MLNPolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"role":"symbol","fragments":[{"text":"MLNPolyline","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"url":"\/documentat