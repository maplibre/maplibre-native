gments":[{"kind":"identifier","text":"MLNShapeSourceOptionMaximumZoomLevelForClustering"}],"abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the maximum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to cluster points if clustering is enabled. Defaults to one zoom level"},{"type":"text","text":" "},{"type":"text","text":"less than the value of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","type":"reference","isActive":true},{"type":"text","text":" so that, at the"},{"type":"text","text":" "},{"type":"text","text":"maximum zoom level, the shapes are not clustered."}],"kind":"symbol","role":"symbol","title":"MLNShapeSourceOptionMaximumZoomLevelForClustering","url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevelforclustering","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevelForClustering"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionSimplificationTolerance":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnshapesourceoptionsimplificationtolerance","title":"MLNShapeSourceOptionSimplificationTolerance","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionSimplificationTolerance","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing a double; specifies the Douglas-Peucker"},{"type":"text","text":" "},{"type":"text","text":"simplification tolerance. A greater value produces simpler geometries and"},{"type":"text","text":" "},{"type":"text","text":"improves performance. The default value is 0.375."}],"role":"symbol","fragments":[{"text":"MLNShapeSourceOptionSimplificationTolerance","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionClustered":{"abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing a Boolean enabling or disabling clustering."},{"type":"text","text":" "},{"type":"text","text":"If the "},{"type":"codeVoice","code":"shape"},{"type":"text","text":" property contains point shapes, setting this option to"},{"type":"text","text":" "},{"type":"codeVoice","code":"YES"},{"type":"text","text":" clusters the points by radius into groups. The default value is "},{"type":"codeVoice","code":"NO"},{"text":".","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnshapesourceoptionclustered","fragments":[{"text":"MLNShapeSourceOptionClustered","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClustered","role":"symbol","title":"MLNShapeSourceOptionClustered","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionBuffer":{"role":"symbol","title":"MLNShapeSourceOptionBuffer","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the size of the tile"},{"type":"text","text":" "},{"type":"text","text":"buffer on each side. A value of 0 produces no buffer. A value of 512 produces a"},{"type":"text","text":" "},{"type":"text","text":"buffer as wide as the tile itself. Larger values produce fewer rendering"},{"type":"text","text":" "},{"type":"text","text":"artifacts near tile edges and slower performance. The default value is 128."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionBuffer","url":"\/documentation\/maplibre\/mlnshapesourceoptionbuffer","type":"topic","fragments":[{"text":"MLNShapeSourceOptionBuffer","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"r