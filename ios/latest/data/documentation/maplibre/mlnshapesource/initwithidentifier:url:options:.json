usterRadius":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClusterRadius","role":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the radius of each"},{"type":"text","text":" "},{"type":"text","text":"cluster if clustering is enabled. A value of 512 produces a radius equal to"},{"text":" ","type":"text"},{"text":"the width of a tile. The default value is 50.","type":"text"}],"type":"topic","fragments":[{"text":"MLNShapeSourceOptionClusterRadius","kind":"identifier"}],"kind":"symbol","title":"MLNShapeSourceOptionClusterRadius","url":"\/documentation\/maplibre\/mlnshapesourceoptionclusterradius"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMaximumZoomLevel":{"url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevel","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionMaximumZoomLevel"}],"abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the maximum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to create vector tiles. A greater value produces greater detail at high"},{"type":"text","text":" "},{"type":"text","text":"zoom levels. The default value is 18."}],"title":"MLNShapeSourceOptionMaximumZoomLevel","kind":"symbol"}}}