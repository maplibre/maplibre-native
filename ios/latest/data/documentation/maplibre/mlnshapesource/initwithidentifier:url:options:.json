tionClustered"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionBuffer":{"type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionbuffer","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionBuffer","fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionBuffer"}],"title":"MLNShapeSourceOptionBuffer","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the size of the tile"},{"type":"text","text":" "},{"type":"text","text":"buffer on each side. A value of 0 produces no buffer. A value of 512 produces a"},{"type":"text","text":" "},{"type":"text","text":"buffer as wide as the tile itself. Larger values produce fewer rendering"},{"type":"text","text":" "},{"type":"text","text":"artifacts near tile edges and slower performance. The default value is 128."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"title":"MLNShape","url":"\/documentation\/maplibre\/mlnshape","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","abstract":[{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNShape"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"title":"MLNShapeSource","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"typ