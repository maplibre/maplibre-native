wift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMinimumZoomLevel":{"role":"symbol","title":"MLNShapeSourceOptionMinimumZoomLevel","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMinimumZoomLevel","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing an integer; specifies the minimum zoom level at","type":"text"},{"text":" ","type":"text"},{"text":"which to create vector tiles. The default value is 0.","type":"text"}],"kind":"symbol","type":"topic","fragments":[{"text":"MLNShapeSourceOptionMinimumZoomLevel","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionminimumzoomlevel"},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","role":"collection","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionSimplificationTolerance":{"title":"MLNShapeSourceOptionSimplificationTolerance","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"NSNumber"},{"text":" object containing a double; specifies the Douglas-Peucker","type":"text"},{"text":" ","type":"text"},{"text":"simplification tolerance. A greater value produces simpler geometries and","type":"text"},{"text":" ","type":"text"},{"text":"improves performance. The default value is 0.375.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionSimplificationTolerance"}],"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnshapesourceoptionsimplificationtolerance","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionSimplificationTolerance","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape