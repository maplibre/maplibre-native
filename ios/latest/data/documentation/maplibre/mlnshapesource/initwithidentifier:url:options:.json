type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionBuffer","url":"\/documentation\/maplibre\/mlnshapesourceoptionbuffer","role":"symbol","fragments":[{"text":"MLNShapeSourceOptionBuffer","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the size of the tile"},{"type":"text","text":" "},{"type":"text","text":"buffer on each side. A value of 0 produces no buffer. A value of 512 produces a"},{"type":"text","text":" "},{"type":"text","text":"buffer as wide as the tile itself. Larger values produce fewer rendering"},{"type":"text","text":" "},{"type":"text","text":"artifacts near tile edges and slower performance. The default value is 128."}],"title":"MLNShapeSourceOptionBuffer"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource/initWithIdentifier:URL:options:":{"fragments":[{"text":"- ","kind":"text"},{"text":"initWithIdentifier:URL:options:","kind":"identifier"}],"role":"symbol","kind":"symbol","title":"initWithIdentifier:URL:options:","abstract":[{"type":"text","text":"Returns a shape source with an identifier, URL, and dictionary of options for"},{"type":"text","text":" "},{"type":"text","text":"the source."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/initWithIdentifier:URL:options:","type":"topic","url":"\/documentation\/maplibre\/mlnshapesource\/initwithidentifier:url:options:","navigatorTitle":[{"text":"initWithIdentifier:URL:options:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionClusterRadius":{"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionclusterradius","abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the radius of each"},{"type":"text","text":" "},{"type":"text","text":"cluster if clustering is enabled. A value of 512 produces a radius equal to"},{"type":"text","text":" "},{"type":"text","text":"the width of a tile. The default value is 50."}],"role":"symbol","title":"MLNShapeSourceOptionClusterRadius","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClusterRadius","fragments":[{"text":"MLNShapeSourceOptionClusterRadius","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"kind":"symbol","fragments":[{"text":"MLNShape","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshape","type":"topic","role":"symbol","title":"MLNShape","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnshapesource","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSource"}],"abstract":[{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference"},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference"},{"type":"text","text":" or "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"type":"codeVoice","code":"shape"},{"type":"text","text":" or "},{"type":"codeVoice","code":"URL"},{"type":"text","text":" property."}],"title":"MLNShapeSource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMaximumZoomLevelForClustering":{"role":"symbol","kind":"symbol","fragments":[{"text":"MLNShapeSourceOptionMaximumZoomLevelForClustering","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevelforclustering","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevelForClustering","title":"MLNShapeSourceOptionMaximumZoomLevelForClustering","abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the maximum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to cluster points if clustering is enabled. Defaults to one zoom level"},{"type":"text","text":" "},{"type":"text","text":"less than the value of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","isActive":true},{"type":"text","text":" so that, at the"},{"type":"text","text":" "},{"type":"text","text":"maximum zoom level, the shapes are not clustered."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMinimumZoomLevel":{"kind":"symbol","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionminimumzoomlevel","fragments":[{"text":"MLNShapeSourceOptionMinimumZoomLevel","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMinimumZoomLevel","abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the minimum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to create vector tiles. The default value is 0."}],"title":"MLNShapeSourceOptionMinimumZoomLevel"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMaximumZoomLevel":{"type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevel","title":"MLNShapeSourceOptionMaximumZoomLevel","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionMaximumZoomLevel"}],"abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing an integer; specifies the maximum zoom level at","type":"text"},{"text":" ","type":"text"},{"text":"which to create vector tiles. A greater value produces greater detail at high","type":"text"},{"text":" ","type":"text"},{"text":"zoom levels. The default value is 18.","type":"text"}],"kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionClustered":{"abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing a Boolean enabling or disabling clustering.","type":"text"},{"text":" ","type":"text"},{"text":"If the ","type":"text"},{"code":"shape","type":"codeVoice"},{"text":" property contains point shapes, setting this option to","type":"text"},{"text":" ","type":"text"},{"code":"YES","type":"codeVoice"},{"text":" clusters the points by radius into groups. The default value is ","type":"text"},{"code":"NO","type":"codeVoice"},{"text":".","type":"text"}],"type":"topic","title":"MLNShapeSourceOptionClustered","url":"\/documentation\/maplibre\/mlnshapesourceoptionclustered","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClustered","fragments":[{"text":"MLNShapeSourceOptionClustered","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionSimplificationTolerance":{"kind":"symbol","role":"symbol","title":"MLNShapeSourceOptionSimplificationTolerance","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionSimplificationTolerance","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"text":" object containing a double; specifies the Douglas-Peucker","type":"text"},{"text":" ","type":"text"},{"text":"simplification tolerance. A greater value produces simpler geometries and","type":"text"},{"text":" ","type":"text"},{"text":"improves performance. The default value is 0.375.","type":"text"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionsimplificationtolerance","fragments":[{"text":"MLNShapeSourceOptionSimplificationTolerance","kind":"identifier"}]}}}