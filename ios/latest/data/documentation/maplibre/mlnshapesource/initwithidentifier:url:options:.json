agments":[{"kind":"identifier","text":"MLNShape"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMinimumZoomLevel":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMinimumZoomLevel","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the minimum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to create vector tiles. The default value is 0."}],"type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionminimumzoomlevel","role":"symbol","fragments":[{"text":"MLNShapeSourceOptionMinimumZoomLevel","kind":"identifier"}],"title":"MLNShapeSourceOptionMinimumZoomLevel","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionClusterRadius":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClusterRadius","kind":"symbol","fragments":[{"text":"MLNShapeSourceOptionClusterRadius","kind":"identifier"}],"title":"MLNShapeSourceOptionClusterRadius","url":"\/documentation\/maplibre\/mlnshapesourceoptionclusterradius","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the radius of each"},{"type":"text","text":" "},{"type":"text","text":"cluster if clustering is enabled. A value of 512 produces a radius equal to"},{"type":"text","text":" "},{"type":"text","text":"the width of a tile. The default value is 50."}],"role":"symbol","type":"topic"}}}