adius":{"abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing an integer; specifies the radius of each","type":"text"},{"text":" ","type":"text"},{"text":"cluster if clustering is enabled. A value of 512 produces a radius equal to","type":"text"},{"text":" ","type":"text"},{"text":"the width of a tile. The default value is 50.","type":"text"}],"type":"topic","role":"symbol","kind":"symbol","title":"MLNShapeSourceOptionClusterRadius","fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionClusterRadius"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionclusterradius","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClusterRadius"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMaximumZoomLevel":{"fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionMaximumZoomLevel"}],"title":"MLNShapeSourceOptionMaximumZoomLevel","abstract":[{"type":"text","text":"An "},{"type":"codeVoice","code":"NSNumber"},{"type":"text","text":" object containing an integer; specifies the maximum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to create vector tiles. A greater value produces greater detail at high"},{"type":"text","text":" "},{"type":"text","text":"zoom levels. The default value is 18."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevel"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMaximumZoomLevelForClustering":{"fragments":[{"kind":"identifier","text":"MLNShapeSourceOptionMaximumZoomLevelForClustering"}],"kind":"symbol","role":"symbol","type":"topic","title":"MLNShapeSourceOptionMaximumZoomLevelForClustering","abstract":[{"type":"text","text":"An "},{"code":"NSNumber","type":"codeVoice"},{"type":"text","text":" object containing an integer; specifies the maximum zoom level at"},{"type":"text","text":" "},{"type":"text","text":"which to cluster points if clustering is enabled. Defaults to one zoom level"},{"type":"text","text":" "},{"type":"text","text":"less than the value of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","isActive":true},{"type":"text","text":" so that, at the"},{"type":"text","text":" "},{"type":"text","text":"maximum zoom level, the shapes are not clustered."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevelForClustering","url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevelforclustering"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionClustered":{"fragments":[{"text":"MLNShapeSourceOptionClustered","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionclustered","title":"MLNShapeSourceOptionClustered","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionClustered","type":"topic","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing a Boolean enabling or disabling clustering.","type":"text"},{"text":" ","type":"text"},{"text":"If the ","type":"text"},{"code":"shape","type":"codeVoice"},{"text":" property contains point shapes, setting this option to","type":"text"},{"text":" ","type":"text"},{"code":"YES","type":"codeVoice"},{"text":" clusters the points by radius into groups. The default value is ","type":"text"},{"code":"NO","type":"codeVoice"},{"text":".","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSource"}],"title":"MLNShapeSource","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"top