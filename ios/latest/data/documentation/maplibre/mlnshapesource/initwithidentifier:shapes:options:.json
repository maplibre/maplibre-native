Libre","kind":"symbol","type":"topic","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionBuffer":{"kind":"symbol","fragments":[{"text":"MLNShapeSourceOptionBuffer","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshapesourceoptionbuffer","type":"topic","title":"MLNShapeSourceOptionBuffer","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionBuffer","abstract":[{"text":"An ","type":"text"},{"code":"NSNumber","type":"codeVoice"},{"text":" object containing an integer; specifies the size of the tile","type":"text"},{"text":" ","type":"text"},{"text":"buffer on each side. A value of 0 produces no buffer. A value of 512 produces a","type":"text"},{"text":" ","type":"text"},{"text":"buffer as wide as the tile itself. Larger values produce fewer rendering","type":"text"},{"text":" ","type":"text"},{"text":"artifacts near tile edges and slower performance. The default value is 128.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSourceOptionMaximumZoomLevel":{"fragments":[{"text":"MLNShapeSourceOptionMaximumZoomLevel","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSourceOptionMaximumZoomLevel","type":"topic","url":"\/documentation\/maplibre\/mlnshapesourceoptionmaximumzoomlevel","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"codeVoice","code":"NSNumber"},{"text":" object containing an integer; specifies the maximum zoom level at","type":"text"},{"text":" ","type":"text"},{"text":"which to create vector tiles. A greater value produces greater detail at high","type":"text"},{"text":" ","type":"text"},{"text":"zoom levels. The default value is 18.","type":"text"}],"role":"symbol","title":"MLNShapeSourceOptionMaximumZoomLevel"},"doc