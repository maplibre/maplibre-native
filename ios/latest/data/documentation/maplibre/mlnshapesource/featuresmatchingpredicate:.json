erence","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map â€” not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"type":"topic","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShape"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","navigatorTitle":[{"text":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/visiblefeaturesatpoint:instylelayerswithidentifiers:predicate:","title":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"abstract":[{"text":"Returns an array of rendered map features that intersect with a given point,","type":"text"},{"text":" ","type":"text"},{"text":"restricted to the given style layers and filtered by the given predicate.","type":"text"}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"title":"MLNShapeSource","type":"topic","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","kind":"symbol","url":"\/documentation\/maplibre\/mlnshapesource","role":"symbol","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"type":"text","text":