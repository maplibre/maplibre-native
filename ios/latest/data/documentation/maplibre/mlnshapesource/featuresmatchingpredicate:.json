 style"},{"text":" ","type":"text"},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"text":" ","type":"text"},{"text":"can update a shape source by setting its ","type":"text"},{"type":"codeVoice","code":"shape"},{"type":"text","text":" or "},{"code":"URL","type":"codeVoice"},{"type":"text","text":" property."}],"fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","title":"MLNShapeSource","url":"\/documentation\/maplibre\/mlnshapesource"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","role":"symbol","fragments":[{"kind":"identifier","text":"MLNFeature"}],"title":"MLNFeature","kind":"symbol","url":"\/documentation\/maplibre\/mlnfeature","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","type":"reference","isActive":true},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"text":" ","type":"text"},{"text":"contained in an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"reference","isActive":true},{"text":" object. Each concrete","type":"text"},{"text":" ","type":"text"},{"text":"subclass of ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":" in turn has a subclass that conforms to this protocol. A","type":"text"},{"type":"text","text":" "},{"type":"text","text":"feature object associates a shape with an optional identifier and attributes."}]},"doc://org.swift.MyProject/documentation/Map