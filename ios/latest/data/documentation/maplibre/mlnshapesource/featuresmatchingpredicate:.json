":"text"},{"text":"obtain only ","type":"text"},{"type":"emphasis","inlineContent":[{"text":"visible","type":"text"}]},{"type":"text","text":" features, use the"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","isActive":true},{"type":"text","text":" "},{"type":"text","text":"or"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","type":"reference","isActive":true},{"type":"text","text":" "},{"type":"text","text":"method."}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","type":"topic","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"url":"\/documentation\/maplibre\/mlnvectortilesource","type":"topic","kind":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"reference","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"text":"in ","type":"text"},{"type":"text","text":"Mapbox Vector Tile"},{"text":" format","type":"text"},{"text":" ","type":"text"},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"text":"the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"text":".",