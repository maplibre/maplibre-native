":"text"},{"text":"obtain only ","type":"text"},{"inlineContent":[{"type":"text","text":"visible"}],"type":"emphasis"},{"text":" features, use the","type":"text"},{"text":" ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","isActive":true},{"text":" ","type":"text"},{"text":"or","type":"text"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","isActive":true},{"type":"text","text":" "},{"text":"method.","type":"text"}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"fragments":[{"kind":"identifier","text":"MLNFeature"}],"title":"MLNFeature","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"text":" ","type":"text"},{"text":"contained in an ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"text":" or ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"type":"text","text":" object. Each concrete"},{"type":"text","text":" "},{"text":"subclass of ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":" in turn has a subclass that conforms to this protocol. A"},{"text":" ","type":"text"},{"text":"feature object associates a shape with an optional identifier and attributes.","type":"text"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnfeature","type":"topic",