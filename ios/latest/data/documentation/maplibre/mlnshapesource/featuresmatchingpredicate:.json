":"text","text":"obtain only "},{"type":"emphasis","inlineContent":[{"text":"visible","type":"text"}]},{"type":"text","text":" features, use the"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","isActive":true,"type":"reference"},{"text":" ","type":"text"},{"type":"text","text":"or"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:"},{"text":" ","type":"text"},{"type":"text","text":"method."}],"type":"paragraph"}]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"type":"topic","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource/featuresMatchingPredicate:":{"abstract":[{"type":"text","text":"Returns an array of map features for this source, filtered by the given"},{"type":"text","text":" "},{"type":"text","text":"predicate."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/featuresMatchingPredicate:","title":"featuresMatchingPredicate:","url":"\/documentation\/maplibre\/mlnshapesource\/featuresmatchingpredicate:","navigatorTitle":[{"text":"featuresMatchingPredicate:","kind":"identifier"}],"role":"symbol","type":"topic","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"featuresMatchingPredicate:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","type