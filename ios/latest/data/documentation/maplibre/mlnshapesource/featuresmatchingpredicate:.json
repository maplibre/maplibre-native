pView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:"},{"type":"text","text":" "},{"type":"text","text":"or"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","type":"reference","isActive":true},{"text":" ","type":"text"},{"type":"text","text":"method."}],"type":"paragraph"}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/featuresMatchingPredicate:","interfaceLanguage":"occ"},"variants":[{"paths":["\/documentation\/maplibre\/mlnshapesource\/featuresmatchingpredicate:"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:":{"abstract":[{"type":"text","text":"Returns an array of rendered map features that intersect with a given point,"},{"text":" ","type":"text"},{"type":"text","text":"restricted to the given style layers and filtered by the given predicate."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","type":"topic","url":"\/documentation\/maplibre\/mlnmapview\/visiblefeaturesatpoint:instylelayerswithidentifiers:predicate:","kind":"symbol","title":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","fragments":[{"text":"- ","kind":"text"},{"text":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"navigatorTitle":[{"text":"visibleFeaturesAtPoint:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","url":"\/documentation\/maplibre\/mlnvectortilesource","title":"MLNVectorTileSource","type":"topic","kind":"symbol","