de":"state"},{"text":" key path. Alternatively, you can add an observer for","type":"text"},{"text":" ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgressChangedNotification","isActive":true,"type":"reference"},{"text":"s about this pack that come from the","type":"text"},{"text":" ","type":"text"},{"text":"default notification center.","type":"text"}]}],"kind":"content"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnofflinepack\/progress"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgress":{"kind":"symbol","abstract":[{"text":"A structure containing information about an offline pack’s current download","type":"text"},{"text":" ","type":"text"},{"text":"progress.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnofflinepackprogress","role":"symbol","fragments":[{"text":"MLNOfflinePackProgress","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgress","title":"MLNOfflinePackProgress"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack/progress":{"role":"symbol","url":"\/documentation\/maplibre\/mlnofflinepack\/progress","fragments":[{"text":"progress","kind":"identifier"}],"title":"progress","abstract":[{"type":"text","text":"The pack’s current progress."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack\/progress","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","title":"MapLibre","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"url":"\/documentation\/m