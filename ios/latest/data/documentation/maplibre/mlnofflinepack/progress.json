":0,"minor":3},"metadata":{"symbolKind":"property","externalID":"c:objc(cs)MLNOfflinePack(py)progress","title":"progress","modules":[{"name":"MapLibre"}],"fragments":[{"text":"progress","kind":"identifier"}],"role":"symbol","roleHeading":"Instance Property","navigatorTitle":[{"kind":"identifier","text":"progress"}]},"variants":[{"paths":["\/documentation\/maplibre\/mlnofflinepack\/progress"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack\/progress","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgress":{"fragments":[{"text":"MLNOfflinePackProgress","kind":"identifier"}],"title":"MLNOfflinePackProgress","url":"\/documentation\/maplibre\/mlnofflinepackprogress","role":"symbol","abstract":[{"type":"text","text":"A structure containing information about an offline pack’s current download"},{"type":"text","text":" "},{"text":"progress.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgress","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgressChangedNotification":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnofflinepackprogresschangednotification","abstract":[{"type":"text","text":"Posted by the shared "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","isActive":true,"type":"reference"},{"type":"text","text":" object when an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"text":" ","type":"text"},{"text":"object’s progress changes. The progress may change due to a resource being","type":"text"},{"type":"text","text":" "},{"type":"text","text":"downloaded or because the pack discovers during the download that more"},{"text":" ","type":"text"},{"type":"text","text":"resources are required for offline viewing. This notification is posted"},{"type":"text","text":" "},{"type":"text","text":"whenever any field in the "},{"type":"codeVoice","code":"progress"},{"type":"text","text":" property changes."}],"type":"topic","title":"MLNOfflinePackProgressChangedNotification","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgressChangedNotification","fragments":[{"text":"MLNOfflinePackProgressChangedNotification","kind":"identifier"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"text":" ","type":"text"},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"text":" ","type":"text"},{"type":"text","text":"object maintains a canonical collection of offline packs in its "},{"code":"packs","type":"codeVoice"},{"text":" property.","type":"text"}],"url":"\/documentation\/maplibre\/mlnofflinestorage","role":"symbol","type":"topic","title":"MLNOfflineStorage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","fragments":[{"kind":"identifier","text":"MLNOfflineStorage"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnofflinepack","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre