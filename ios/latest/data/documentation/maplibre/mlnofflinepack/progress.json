":"text"}]}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack\/progress","interfaceLanguage":"occ"},"metadata":{"navigatorTitle":[{"text":"progress","kind":"identifier"}],"role":"symbol","fragments":[{"kind":"identifier","text":"progress"}],"title":"progress","modules":[{"name":"MapLibre"}],"symbolKind":"property","roleHeading":"Instance Property","externalID":"c:objc(cs)MLNOfflinePack(py)progress"},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnofflinepack\/progress"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgress":{"type":"topic","abstract":[{"type":"text","text":"A structure containing information about an offline pack’s current download"},{"text":" ","type":"text"},{"type":"text","text":"progress."}],"kind":"symbol","title":"MLNOfflinePackProgress","url":"\/documentation\/maplibre\/mlnofflinepackprogress","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgress","role":"symbol","fragments":[{"text":"MLNOfflinePackProgress","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","title":"MLNOfflinePack","url":"\/documentation\/maplibre\/mlnofflinepack","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"type":"topic","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"text":" ","type":"text"},{"text":"a region offline to a local database.","type":"text"}],"role":"symbol","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","kind":"symbol","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/m