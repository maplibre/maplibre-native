ess","title":"MLNOfflinePackProgress","abstract":[{"type":"text","text":"A structure containing information about an offline pack’s current download"},{"text":" ","type":"text"},{"type":"text","text":"progress."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgress","kind":"symbol","role":"symbol","fragments":[{"text":"MLNOfflinePackProgress","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"url":"\/documentation\/maplibre\/mlnofflinepack","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"type":"text","text":" "},{"type":"text","text":"a region offline to a local database."}],"role":"symbol","fragments":[{"text":"MLNOfflinePack","kind":"identifier"}],"type":"topic","title":"MLNOfflinePack","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgressChangedNotification":{"title":"MLNOfflinePackProgressChangedNotification","abstract":[{"text":"Posted by the shared ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage"},{"text":" object when an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"type":"text","text":" "},{"text":"object’s progress changes. The progress may change due to a resource being","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"downloaded or because the pack discovers during the download that more"},{"text":" ","type":"text"},{"text":"resources are required for offline viewing. This notification is posted","type":"text"},{"type":"text","text":" "},{"type":"text","text":"whene