xt"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"a region offline to a local database."}],"url":"\/documentation\/maplibre\/mlnofflinepack","role":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgressChangedNotification":{"abstract":[{"type":"text","text":"Posted by the shared "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage","type":"reference"},{"text":" object when an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"type":"text","text":" "},{"text":"object’s progress changes. The progress may change due to a resource being","type":"text"},{"type":"text","text":" "},{"text":"downloaded or because the pack discovers during the download that more","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"resources are required for offline viewing. This notification is posted"},{"type":"text","text":" "},{"text":"whenever any field in the ","type":"text"},{"code":"progress","type":"codeVoice"},{"text":" property changes.","type":"text"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePackProgressChangedNotification","kind":"symbol","url":"\/documentation\/maplibre\/mlnofflinepackprogresschangednotification","type":"topic","title":"MLNOfflinePackProgressChangedNotification","fragments":[{"kind":"identifier","text":"MLNOfflinePackProgressChangedNotification"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePackProgress":{"type":"topic","title":"MLNOfflinePackProgre