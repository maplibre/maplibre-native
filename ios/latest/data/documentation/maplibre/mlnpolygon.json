ape","type":"codeVoice"},{"type":"text","text":" or "},{"code":"URL","type":"codeVoice"},{"text":" property.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnshapesource","role":"symbol","title":"MLNShapeSource","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon/interiorPolygons":{"abstract":[{"text":"The array of polygons nested inside the receiver.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon\/interiorPolygons","type":"topic","title":"interiorPolygons","role":"symbol","url":"\/documentation\/maplibre\/mlnpolygon\/interiorpolygons","fragments":[{"kind":"identifier","text":"interiorPolygons"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"type":"topic","url":"\/documentation\/maplibre\/mlnpolylinefeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"title":"MLNPolylineFeature","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"identifier and attributes."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"type":"topic","url":"\/documentation\/maplibre\/mlnstyle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNStyle"}],"title":"MLNStyle","role":"symbol","abstract":[{"type":"text","text":"The proxy object for the current map style."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"type":"topic","url":"\/documentation\/maplibre\/mlnmultipolygon","identifier":"doc:\/\/org.