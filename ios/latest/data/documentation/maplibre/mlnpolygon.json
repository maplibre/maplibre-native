//org.swift.MyProject/documentation/MapLibre/MLNShape":{"title":"MLNShape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"topic","fragments":[{"kind":"identifier","text":"MLNShape"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnshape","role":"symbol","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"type":"text","text":" is an abstract class that represents a shape or annotation. Shapes"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"title":"MLNMultiPolyline","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"text":" ","type":"text"},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"text":"reservation): each carriageway would be a distinct ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"text":" object.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"type":"topic","kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object represents a one-dimensional shape located at a","type":"text"},{"text":" ","type":"text"},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"text":"address at high zoom levels, or the location of a long press gesture.","type":"text"}],"title":"MLNPointAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"fragments":[{"text":"MLNStyle","kind":"identifier"}],"abstract":[{"type":"text","text":"The proxy object for the current map style."}],"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","role":"symbol","url":"\/documentation\/maplibre\/mlnstyle","title":"MLNStyle"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorStyleLayer":{"role":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true,"type":"reference"},{"text":" is an abstract superclass for style layers whose content","type":"text"},{"text":" ","type":"text"},{"text":"is defined by an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" or ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true,"type":"reference"},{"text":" object.","type":"text"}],"type":"topic","title":"MLNVectorStyleLayer","kind":"symbol","fragments":[{"text":"MLNVectorStyleLayer","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnvectorstylelayer","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","role":"symbol","kind":"symbol","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"text":" ","type":"text"},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true,"type":"reference"},{"text":" object, while the surrounding","type":"text"},{"text":" ","type":"text"},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"type":"topic","title":"MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"type":"topic","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","title":"MLNPointCollection","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnpointcollection"}}}