":"symbol","title":"MLNPolylineFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","isActive":true},{"text":" object associates a polyline shape with an optional","type":"text"},{"type":"text","text":" "},{"text":"identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygonFeature":{"type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnpolygonfeature","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygonFeature","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygonFeature","type":"reference"},{"text":" object associates a polygon shape with an optional","type":"text"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"title":"MLNPolygonFeature","fragments":[{"text":"MLNPolygonFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","role":"symbol","type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnstyle","abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"title":"MLNStyle","fragments":[{"text":"MLNStyle","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"url":"\/documentation\/maplibre\/mlnshapesource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","kind":"symbol","title":"MLNShapeSource","fragments":[{"text":"MLNShapeSource","kind":"identifier"}],"role":"symbol","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"text":" is a map content source that supplies vector shapes to be","type":"text"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":",","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"or they may be defined by local or external"},{"text":" ","type":"text"},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true,"type":"reference"},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"text":"layer defines the appearance of any content supplied by the shape source. You","type":"text"},{"text":" ","type":"text"},{"text":"can update a shape source by setting its ","type":"text"},{"code":"shape","type":"codeVoice"},{"text":" or ","type":"text"},{"code":"URL","type":"codeVoice"},{"text":" property.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"type":"text","text":" "},{"text":"atoll: the inner island would be one ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true,"type":"reference"},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"kind":"symbol","type":"topic","role":"symbol","title":"MLNMultiPolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","url":"\/documentation\/maplibre\/mlnmultipolygon"}}}