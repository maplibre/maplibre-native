n/MapLibre/MLNPolylineFeature":{"fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","url":"\/documentation\/maplibre\/mlnpolylinefeature","type":"topic","role":"symbol","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","isActive":true,"type":"reference"},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"title":"MLNPolylineFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon/polygonWithCoordinates:count:interiorPolygons:":{"kind":"symbol","title":"polygonWithCoordinates:count:interiorPolygons:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon\/polygonWithCoordinates:count:interiorPolygons:","navigatorTitle":[{"kind":"identifier","text":"polygonWithCoordinates:count:interiorPolygons:"}],"role":"symbol","fragments":[{"kind":"text","text":"+ "},{"kind":"identifier","text":"polygonWithCoordinates:count:interiorPolygons:"}],"abstract":[{"type":"text","text":"Creates and returns an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object from the specified set of"},{"type":"text","text":" "},{"type":"text","text":"coordinates and interior polygons."}],"type":"topic","url":"\/documentation\/maplibre\/mlnpolygon\/polygonwithcoordinates:count:interiorpolygons:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","type":"topic","kind":"symbol","title":"MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}],"url":"\/documentation\/maplibre\/mlnpointcollectionfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","title":"MLNPointCollectionFeature","type":"topic","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","type":"reference","isActive":true},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}],"title":"MLNVectorTileSource","url":"\/documentation\/maplibre\/mlnvectortilesource","role":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"reference","isActive":true},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"type":"text","text":" format"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"type":"text","text":" "},{"type":"text","text":"defined either by an option dictionary or by an external file that conforms to"},{"type":"text","text":" "},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A vector tile source is added to an "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer"},{"type":"text","text":" objects. A vector style layer defines the appearance of"},{"type":"text","text":" "},{"type":"text","text":"any content supplied by the vector tile source."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"role":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","type":"topic","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"url":"\/documentation\/maplibre\/mlnmultipolygonfeature","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","title":"MLNMultiPolygonFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","isActive":true},{"type":"text","text":" object associates a multipolygon shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"role":"symbol","title":"MLNPointCollection","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"type":"text","text":" "},{"type":"text","text":"points in the collection may be related but are not connected spatially. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point collection to represent all the trees in an"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","type":"topic","fragments":[{"text":"MLNPointCollection","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer":{"title":"MLNSymbolStyleLayer","role":"symbol","url":"\/documentation\/maplibre\/mlnsymbolstylelayer","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer","type":"topic","fragments":[{"kind":"identifier","text":"MLNSymbolStyleLayer"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer"},{"type":"text","text":" is a style layer that renders icon and text labels"},{"type":"text","text":" "},{"type":"text","text":"at points or along lines on the map."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"type":"text","text":" object."}],"fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmultipolyline","title":"MLNMultiPolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","title":"MLNMultiPoint","url":"\/documentation\/maplibre\/mlnmultipoint","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:fillColorForPolygonAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:fillColorForPolygonAnnotation:","type":"topic","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:fillColorForPolygonAnnotation:"}],"abstract":[{"text":"Returns the color to use when rendering the fill of a polygon annotation.","type":"text"}],"title":"mapView:fillColorForPolygonAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:fillColorForPolygonAnnotation:"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:fillcolorforpolygonannotation:"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","role":"collection","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","fragments":[{"text":"MLNEmptyFeature","kind":"identifier"}],"kind":"symbol","title":"MLNEmptyFeature","url":"\/documentation\/maplibre\/mlnemptyfeature","type":"topic","abstract":[{"text":"An ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature"},{"text":" object associates an empty shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmultipolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"type":"text","text":" "},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"type":"text","text":" "},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object, while the surrounding"},{"type":"text","text":" "},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"fragments":[{"kind":"identifier","text":"MLNMultiPolygon"}],"title":"MLNMultiPolygon"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygonFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygonFeature","role":"symbol","url":"\/documentation\/maplibre\/mlnpolygonfeature","fragments":[{"kind":"identifier","text":"MLNPolygonFeature"}],"type":"topic","title":"MLNPolygonFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygonFeature"},{"type":"text","text":" object associates a polygon shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:strokeColorForShapeAnnotation:":{"navigatorTitle":[{"text":"mapView:strokeColorForShapeAnnotation:","kind":"identifier"}],"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:strokeColorForShapeAnnotation:","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:strokecolorforshapeannotation:","title":"mapView:strokeColorForShapeAnnotation:","role":"symbol","abstract":[{"text":"Returns the color to use when rendering the outline of a shape annotation.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:strokeColorForShapeAnnotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSource"}],"url":"\/documentation\/maplibre\/mlnshapesource","title":"MLNShapeSource","role":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference","isActive":true},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with an "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"code":"shape","type":"codeVoice"},{"type":"text","text":" or "},{"code":"URL","type":"codeVoice"},{"type":"text","text":" property."}],"type":"topic"}}}