url":"\/documentation\/maplibre\/mlnstyle","type":"topic","title":"MLNStyle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","kind":"symbol","role":"symbol","abstract":[{"type":"text","text":"The proxy object for the current map style."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"title":"MLNPointCollection","role":"symbol","fragments":[{"text":"MLNPointCollection","kind":"identifier"}],"type":"topic","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"text":"disconnected vertices, specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances. The","type":"text"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","kind":"symbol","url":"\/documentation\/maplibre\/mlnpointcollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"fragments":[{"kind":"identifier","text":"MLNPolyline"}],"url":"\/documentation\/maplibre\/mlnpolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","kind":"symbol","role":"symbol","title":"MLNPolyline","abstract":[{"text":"An ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","isActive":true},{"text":" object represents a shape consisting of zero or more","type":"text"},{"text":" ","type":"text"},{"text":"distinct but related shapes that are instances of ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"text":". The constituent","type":"text"},{"text":" ","type":"text"},{"text":"shapes can be a mixture of different kinds of shapes.","type":"text"}],"role":"symbol","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollection"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","title":"MLNShapeCollection","type":"topic","url":"\/documentation\/maplibre\/mlnshapecollection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon/polygonWithCoordinates:count:interiorPolygons:":{"abstract":[{"type":"text","text":"Creates and returns an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference","isActive":true},{"type":"text","text":" object from the specified set of"},{"type":"text","text":" "},{"type":"text","text":"coordinates and interior polygons."}],"role":"symbol","title":"polygonWithCoordinates:count:interiorPolygons:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon\/polygonWithCoordinates:count:interiorPolygons:","url":"\/documentation\/maplibre\/mlnpolygon\/polygonwithcoo