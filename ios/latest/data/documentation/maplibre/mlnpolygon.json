ibutes."}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollectionFeature"}],"title":"MLNShapeCollectionFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","url":"\/documentation\/maplibre\/mlnshapecollectionfeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygonFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygonFeature","role":"symbol","type":"topic","title":"MLNPolygonFeature","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygonFeature"},{"type":"text","text":" object associates a polygon shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnpolygonfeature","fragments":[{"kind":"identifier","text":"MLNPolygonFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMultiPoint"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","url":"\/documentation\/maplibre\/mlnmultipoint","type":"topic","title":"MLNMultiPoint","kind":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint"},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"text":" ","type":"text"},{"text":"composed of multiple vertices.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"type":"topic","url":"\/documentation\/maplibre\/mlnshapesource","title":"MLNShapeSource","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"text":" is a map content source that supplies vector shapes to be","type":"text"},{"text":" ","type":"text"},{"text":"shown on the map. The shapes may be instances of ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":" or ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":",","type":"text"},{"text":" ","type":"text"},{"text":"or they may be defined by local or external","type":"text"},{"text":" ","type":"text"},{"text":"GeoJSON","type":"text"},{"text":" code. A shape source is added to an","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"text":" object along with an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"text":" object. The vector style","type":"text"},{"text":" ","type":"text"},{"text":"layer defines the appearance of any content supplied by the shape source. You","type":"text"},{"text":" ","type":"text"},{"text":"can update a shape source by setting its ","type":"text"},{"type":"codeVoice","code":"shape"},{"text":" or ","type":"text"},{"type":"codeVoice","code":"URL"},{"text":" property.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","role":"symbol","fragments":[{"text":"MLNShapeSource","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygon","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text