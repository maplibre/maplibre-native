apLibre\/MLNEmptyFeature","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","type":"reference","isActive":true},{"type":"text","text":" object associates an empty shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature"},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"text":"identifier and attributes.","type":"text"}],"kind":"symbol","fragments":[{"text":"MLNPolylineFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","title":"MLNPolylineFeature","url":"\/documentation\/maplibre\/mlnpolylinefeature","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"role":"symbol","url":"\/documentation\/maplibre\/mlnmultipolylinefeature","fragments":[{"kind":"identifier","text":"MLNMultiPolylineFeature"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates a multipolyline shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","title":"MLNMultiPolylineFeature","kind":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"type":"text","text":"single geographical coordinate. Depending on how it is used, an"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"type":"topic","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"kind":"symbol","role":"symbol","title":"MLNPointAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource":{"type":"topic","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSource"}],"url":"\/documentation\/maplibre\/mlnshapesource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","kind":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","type":"reference","isActive":true},{"type":"text","text":" is a map content source that supplies vector shapes to be"},{"type":"text","text":" "},{"type":"text","text":"shown on the map. The shapes may be instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference","isActive":true},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"type":"text","text":" object along with an "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference","isActive":true},{"type":"text","text":" object. The vector style"},{"type":"text","text":" "},{"type":"text","text":"layer defines the appearance of any content supplied by the shape source. You"},{"type":"text","text":" "},{"type":"text","text":"can update a shape source by setting its "},{"type":"codeVoice","code":"shape"},{"type":"text","text":" or "},{"type":"codeVoice","code":"URL"},{"type":"text","text":" property."}],"title":"MLNShapeSource"}}}