ments":[{"text":"MLNPolylineFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","kind":"symbol","type":"topic","role":"symbol","title":"MLNVectorTileSource","url":"\/documentation\/maplibre\/mlnvectortilesource","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"},{"type":"text","text":" is a map content source that supplies tiled vector data"},{"text":" ","type":"text"},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"text":" format","type":"text"},{"type":"text","text":" "},{"text":"to be shown on the map. The location of and metadata about the tiles are","type":"text"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the"},{"type":"text","text":" "},{"type":"text","text":"TileJSON specification"},{"text":".","type":"text"},{"text":" ","type":"text"},{"text":"A vector tile source is added to an ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"type":"text","text":" object along with one or more"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true},{"text":" objects. A vector style layer defines the appearance of","type":"text"},{"text":" ","type":"text"},{"text":"any content supplied by the vector tile source.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","title":"MapLibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:strokeColorForShapeAnnotation:":{"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:strokecolorforshapeannotation:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:strokeColorForShapeAnnotation:","title":"mapView:strokeColorForShapeAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:strokeColorForShapeAnnotation:"}],"abstract":[{"type":"text","text":"Returns the color to use when rendering the outline of a shape annotation."}],"role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:strokeColorForShapeAnnotation:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolygon":{"title":"MLNPolygon","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","type":"reference"},{"text":" object represents a closed shape consisting of four or more","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances, and the edges that"},{"text":" ","type":"text"},{"type":"text","text":"connect them. For example, you could use a polygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"building, a lake, or an area you want to highlight."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","fragments":[{"text":"MLNPolygon","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnpolygon","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"title":"MLNShape","url":"\/documentation\/maplibre\/mlnshape","kind":"symbol","rol