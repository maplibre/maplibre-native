s and returns an "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"type":"text","text":" object from the specified set of"},{"type":"text","text":" "},{"type":"text","text":"coordinates."}],"fragments":[{"text":"+ ","kind":"text"},{"text":"polygonWithCoordinates:count:","kind":"identifier"}],"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon\/polygonWithCoordinates:count:","role":"symbol","title":"polygonWithCoordinates:count:","navigatorTitle":[{"kind":"identifier","text":"polygonWithCoordinates:count:"}],"url":"\/documentation\/maplibre\/mlnpolygon\/polygonwithcoordinates:count:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"title":"MLNShapeCollectionFeature","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnshapecollectionfeature","fragments":[{"kind":"identifier","text":"MLNShapeCollectionFeature"}],"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","isActive":true},{"type":"text","text":" object associates a shape collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"fragments":[{"text":"MLNStyle","kind":"identifier"}],"title":"MLNStyle","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","type":"topic","role":"symbol","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addOverlay:":{"navigatorTitle":[{"text":"addOverlay:","kind":"identifier"}],"kind":"symbol","url":"\/documenta