Kit"},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can use the following "},{"type":"codeVoice","code":"UIViewController"},{"type":"text","text":" to get started with MapLibre Native iOS with UIKit."}]},{"syntax":"swift","type":"codeListing","code":["class SimpleMap: UIViewController, MLNMapViewDelegate {","    var mapView: MLNMapView!","","    override func viewDidLoad() {","        super.viewDidLoad()","","        mapView = MLNMapView(frame: view.bounds)","        mapView.autoresizingMask = [.flexibleWidth, .flexibleHeight]","        view.addSubview(mapView)","","        mapView.delegate = self","    }","","    \/\/ MLNMapViewDelegate method called when map has finished loading","    func mapView(_: MLNMapView, didFinishLoading _: MLNStyle) {","    }","}"]}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/GettingStarted","interfaceLanguage":"occ"},"kind":"article","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre"]]},"abstract":[{"type":"text","text":"Setting up an Xcode project that uses MapLibre Native for iOS."}],"references":{"https://github.com/maplibre/maplibre-native":{"type":"link","url":"https:\/\/github.com\/maplibre\/maplibre-native","title":"maplibre-native","titleInlineContent":[{"type":"text","text":"maplibre-native"}],"identifier":"https:\/\/github.com\/maplibre\/maplibre-native"},"https://demotiles.maplibre.org/":{"url":"https:\/\/demotiles.maplibre.org\/","type":"link","identifier":"https:\/\/demotiles.maplibre.org\/","titleInlineContent":[{"text":"Demotiles","type":"text"}],"title":"Demotiles"},"DemotilesScreenshot.png":{"type":"image","variants":[{"traits":["2x","light"],"url":"\/images\/org.swift.MyProject\/DemotilesScreenshot@2x.png"}],"alt":null,"identifier":"DemotilesScreenshot.png"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AddMarkerSymbolExample":{"url":"\/documentation\/maplibre-native-for-ios\/addmarkersymbole