\/"},{"type":"text","text":" style:"}],"type":"paragraph"},{"inlineContent":[{"type":"image","identifier":"DemotilesScreenshot.png"}],"type":"paragraph"},{"level":2,"anchor":"UIKit","text":"UIKit","type":"heading"},{"inlineContent":[{"text":"You can use the following ","type":"text"},{"code":"UIViewController","type":"codeVoice"},{"text":" to get started with MapLibre Native iOS with UIKit.","type":"text"}],"type":"paragraph"},{"code":["class SimpleMap: UIViewController, MLNMapViewDelegate {","    var mapView: MLNMapView!","","    override func viewDidLoad() {","        super.viewDidLoad()","","        mapView = MLNMapView(frame: view.bounds)","        mapView.autoresizingMask = [.flexibleWidth, .flexibleHeight]","        view.addSubview(mapView)","","        mapView.delegate = self","    }","","    \/\/ MLNMapViewDelegate method called when map has finished loading","    func mapView(_: MLNMapView, didFinishLoading _: MLNStyle) {","    }","}"],"type":"codeListing","syntax":"swift"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre-native-for-ios\/gettingstarted"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AddMarkerSymbolExample":{"abstract":[{"text":"Simply add a marker to a map!","type":"text"}],"type":"topic","kind":"article","title":"Add Marker","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AddMarkerSymbolExample","role":"article","url":"\/documentation\/maplibre-native-for-ios\/addmarkersymbolexample"},"https://github.com/maplibre/maplibre-native":{"identifier":"https:\/\/github.com\/maplibre\/maplibre-native","title":"maplibre-native","titleInlineContent":[{"type":"text","text":"maplibre-native"}],"url":"https:\/\/github.com\/maplibre\/maplibre-native","type":"link"},"https://demotiles.maplibre.org/":{"identifier":"https:\/\/demotiles.maplibre.org\/","title":"Demotiles","titleInlineContent":[{"type":"text","text":"Demotiles"}],"u