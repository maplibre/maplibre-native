Content":[{"type":"image","identifier":"DemotilesScreenshot.png"}]},{"anchor":"UIKit","type":"heading","text":"UIKit","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can use the following "},{"type":"codeVoice","code":"UIViewController"},{"type":"text","text":" to get started with MapLibre Native iOS with UIKit."}]},{"syntax":"swift","code":["class SimpleMap: UIViewController, MLNMapViewDelegate {","    var mapView: MLNMapView!","","    override func viewDidLoad() {","        super.viewDidLoad()","","        mapView = MLNMapView(frame: view.bounds)","        mapView.autoresizingMask = [.flexibleWidth, .flexibleHeight]","        view.addSubview(mapView)","","        mapView.delegate = self","    }","","    \/\/ MLNMapViewDelegate method called when map has finished loading","    func mapView(_: MLNMapView, didFinishLoading _: MLNStyle) {","    }","}"],"type":"codeListing"}],"kind":"content"}],"kind":"article","seeAlsoSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AddMarkerSymbolExample"],"anchor":"Essentials","generated":true,"title":"Essentials"}],"references":{"AddPackageDependencies.png":{"alt":null,"identifier":"AddPackageDependencies.png","type":"image","variants":[{"traits":["2x","light"],"url":"\/images\/org.swift.MyProject\/AddPackageDependencies@2x.png"}]},"https://swiftpackageindex.com/maplibre/maplibre-gl-native-distribution":{"title":"Swift Package Index","titleInlineContent":[{"type":"text","text":"Swift Package Index"}],"url":"https:\/\/swiftpackageindex.com\/maplibre\/maplibre-gl-native-distribution","identifier":"https:\/\/swiftpackageindex.com\/maplibre\/maplibre-gl-native-distribution","type":"link"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AddMarkerSymbolExample":{"abstract":[{"text":"Simply add a marker to a map!","type":"text"}],"role":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AddMarkerSymbolExamp