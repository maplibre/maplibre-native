inlineContent":[{"type":"text","text":"Verify you can import MapLibre in your app:"}],"type":"paragraph"},{"syntax":"swift","code":["import MapLibre"],"type":"codeListing"},{"type":"heading","text":"SwiftUI","anchor":"SwiftUI","level":2},{"inlineContent":[{"text":"To use MapLibre with SwiftUI we need to create a wrapper for the UIKit view that MapLibre provides (using UIViewRepresentable. The simplest way to implement this protocol is as follows:","type":"text"}],"type":"paragraph"},{"code":["struct SimpleMap: UIViewRepresentable {","    func makeUIView(context _: Context) -> MLNMapView {","        let mapView = MLNMapView()","        return mapView","    }","","    func updateUIView(_: MLNMapView, context _: Context) {}","}"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"You can use this view directly in a SwiftUI View hierarchy, for example:"}],"type":"paragraph"},{"syntax":"swift","code":["struct MyApp: App {","    var body: some Scene {","        WindowGroup {","            SimpleMap().edgesIgnoringSafeArea(.all)","        }","    }","}"],"type":"codeListing"},{"type":"paragraph","inlineContent":[{"type":"text","text":"When running your app in the simulator you should be greeted with the default "},{"identifier":"https:\/\/demotiles.maplibre.org\/","isActive":true,"type":"reference"},{"type":"text","text":" style:"}]},{"type":"paragraph","inlineContent":[{"identifier":"DemotilesScreenshot.png","type":"image"}]},{"type":"heading","level":2,"text":"UIKit","anchor":"UIKit"},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can use the following "},{"type":"codeVoice","code":"UIViewController"},{"text":" to get started with MapLibre Native iOS with UIKit.","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["class SimpleMap: UIViewController, MLNMapViewDelegate {","    var mapView: MLNMapView!","","    override func viewDidLoad() {","        super.viewDidLoad()","","        mapView = MLNMapView(frame: view.bounds)","        mapView.autoresizingMask = [.flexibleWidth, .flexibleHeight]","        view.addSubview(mapView)","","        mapView.delegate = self","    }","","    \/\/ MLNMapViewDelegate method called when map has finished loading","    func mapView(_: MLNMapView, didFinishLoading _: MLNStyle) {","    }","}"]}]}],"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/GettingStarted"},"schemaVersion":{"minor":3,"patch":0,"major":0},"seeAlsoSections":[{"identifiers":["doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AddMarkerSymbolExample"],"title":"Essentials","generated":true}],"abstract":[{"text":"Setting up an Xcode project that uses MapLibre Native for iOS.","type":"text"}],"metadata":{"role":"article","title":"Getting Started","modules":[{"name":"MapLibre"}],"roleHeading":"Article"},"variants":[{"paths":["\/documentation\/maplibre-native-for-ios\/gettingstarted"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"article","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre"]]},"references":{"https://cocoapods.org":{"url":"https:\/\/cocoapods.org","identifier":"https:\/\/cocoapods.org","type":"link","titleInlineContent":[{"type":"text","text":"Cocoapods"}],"title":"Cocoapods"},"AddPackageDependencies.png":{"identifier":"AddPackageDependencies.png","alt":null,"variants":[{"url":"\/images\/AddPackageDependencies@2x.png","traits":["2x","light"]}],"type":"image"},"DemotilesScreenshot.png":{"variants":[{"url":"\/images\/DemotilesScreenshot@2x.png","traits":["2x","light"]}],"alt":null,"identifier":"DemotilesScreenshot.png","type":"image"},"https://github.com/maplibre/maplibre-gl-native-distribution":{"identifier":"https:\/\/github.com\/maplibre\/maplibre-gl-native-distribution","titleInlineContent":[{"text":"maplibre-gl-native-distributon","type":"text"}],"url":"https:\/\/github.com\/maplibre\/maplibre-gl-native-distribution","type":"link","title":