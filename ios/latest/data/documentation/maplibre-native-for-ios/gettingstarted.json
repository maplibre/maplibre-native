ler","type":"codeVoice"},{"type":"text","text":" to get started with MapLibre Native iOS with UIKit."}]},{"syntax":"swift","type":"codeListing","code":["class SimpleMap: UIViewController, MLNMapViewDelegate {","    var mapView: MLNMapView!","","    override func viewDidLoad() {","        super.viewDidLoad()","","        mapView = MLNMapView(frame: view.bounds)","        mapView.autoresizingMask = [.flexibleWidth, .flexibleHeight]","        view.addSubview(mapView)","","        mapView.delegate = self","    }","","    \/\/ MLNMapViewDelegate method called when map has finished loading","    func mapView(_: MLNMapView, didFinishLoading _: MLNStyle) {","    }","}"]}],"kind":"content"}],"metadata":{"role":"article","modules":[{"name":"MapLibre"}],"title":"Getting Started","roleHeading":"Article"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre"]]},"variants":[{"paths":["\/documentation\/maplibre-native-for-ios\/gettingstarted"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/GettingStarted","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AddMarkerSymbolExample":{"title":"Add Marker","url":"\/documentation\/maplibre-native-for-ios\/addmarkersymbolexample","kind":"article","abstract":[{"type":"text","text":"Simply add a marker to a map!"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AddMarkerSymbolExample","role":"article"},"https://swiftpackageindex.com/maplibre/maplibre-gl-native-distribution":{"title":"Swift Package Index","type":"link","identifier":"https:\/\/swiftpackageindex.com\/maplibre\/maplibre-gl-native-distribution","titleInlineContent":[{"type":"text","text":"Swift Package Index"}],"url":"https:\/\/swiftpackageindex.com\/maplibre\/maplibre-gl-native-distribution"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/or