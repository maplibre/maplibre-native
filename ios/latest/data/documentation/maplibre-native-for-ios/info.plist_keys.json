e\/MLNSymbolStyleLayer\/textAllowsOverlap","abstract":[{"text":"If true, the text will be visible even if it collides with other previously","type":"text"},{"text":" ","type":"text"},{"text":"drawn symbols.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/iconAllowsOverlap":{"url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/iconallowsoverlap","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/iconAllowsOverlap","type":"topic","title":"iconAllowsOverlap","role":"symbol","kind":"symbol","abstract":[{"text":"If true, the icon will be visible even if it collides with other previously","type":"text"},{"text":" ","type":"text"},{"text":"drawn symbols.","type":"text"}],"fragments":[{"text":"iconAllowsOverlap","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/textIgnoresPlacement":{"abstract":[{"text":"If true, other symbols can be visible even if they collide with the text.","type":"text"}],"role":"symbol","fragments":[{"text":"textIgnoresPlacement","kind":"identifier"}],"title":"textIgnoresPlacement","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/textIgnoresPlacement","url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/textignoresplacement","kind":"symbol","type":"topic"},"customizing-fonts.html#server-side-fonts":{"type":"link","identifier":"customizing-fonts.html#server-side-fonts","url":"customizing-fonts.html#server-side-fonts","titleInlineContent":[{"text":"server-side rendering","type":"text"}],"title":"server-side rendering"},"https://developer.apple.com/fonts/system-fonts/":{"type":"link","identifier":"https:\/\/developer.apple.com\/fonts\/system-fonts\/","url":"https:\/\/developer.apple.com\/fonts\/system-fonts\/","titleInlineContent":[{"text":"system font","type":"text"}],"title":"system font"}}}