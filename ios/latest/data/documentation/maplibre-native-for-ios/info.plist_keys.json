bre\/MLNSymbolStyleLayer\/textIgnoresPlacement","fragments":[{"kind":"identifier","text":"textIgnoresPlacement"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/textignoresplacement","title":"textIgnoresPlacement","abstract":[{"type":"text","text":"If true, other symbols can be visible even if they collide with the text."}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/iconAllowsOverlap":{"kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"iconAllowsOverlap"}],"title":"iconAllowsOverlap","abstract":[{"text":"If true, the icon will be visible even if it collides with other previously","type":"text"},{"text":" ","type":"text"},{"text":"drawn symbols.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/iconAllowsOverlap","url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/iconallowsoverlap"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/GestureRecognizers":{"type":"topic","abstract":[{"text":"Learn how to work with gesture recognizers","type":"text"}],"role":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/GestureRecognizers","title":"User Interactions","url":"\/documentation\/maplibre-native-for-ios\/gesturerecognizers","kind":"article"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","type":"topic","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"kind":"symbol"}}}