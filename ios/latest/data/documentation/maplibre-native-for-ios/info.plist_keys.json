ect/documentation/MapLibre-Native-for-iOS/Predicates_and_Expressions":{"kind":"article","url":"\/documentation\/maplibre-native-for-ios\/predicates_and_expressions","abstract":[{"text":"Using ","type":"text"},{"type":"codeVoice","code":"NSPredicate"},{"text":" with MapLibre iOS","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/Predicates_and_Expressions","type":"topic","role":"article","title":"Predicates and expressions"},"doc://org.swift.MyProject/documentation/MapLibre/MLNSymbolStyleLayer/textAllowsOverlap":{"title":"textAllowsOverlap","url":"\/documentation\/maplibre\/mlnsymbolstylelayer\/textallowsoverlap","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNSymbolStyleLayer\/textAllowsOverlap","role":"symbol","fragments":[{"text":"textAllowsOverlap","kind":"identifier"}],"abstract":[{"text":"If true, the text will be visible even if it collides with other previously","type":"text"},{"type":"text","text":" "},{"type":"text","text":"drawn symbols."}]},"https://developer.apple.com/documentation/foundation/nsurl/1413819-setresourcevalue":{"title":"NSURL\/setResourceValue:forKey:error:","titleInlineContent":[{"code":"NSURL\/setResourceValue:forKey:error:","type":"codeVoice"}],"identifier":"https:\/\/developer.apple.com\/documentation\/foundation\/nsurl\/1413819-setresourcevalue","url":"https:\/\/developer.apple.com\/documentation\/foundation\/nsurl\/1413819-setresourcevalue","type":"link"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre","role":"collection","kind":"symbol"}}}