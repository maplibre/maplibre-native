er = \"poi\"","        layer.predicate = NSPredicate(format: \"class == %@\", \"shop\")","","        \/\/ Style the circle layer color based on the rank","        layer.circleColor = NSExpression(mglJSONObject: [\"step\", [\"get\", \"rank\"], 0, \"red\", 10, \"green\", 20, \"blue\", 30, \"purple\", 40, \"yellow\"] as [Any])","        layer.circleRadius = NSExpression(forConstantValue: 3)","","        style.addLayer(layer)","    }","}"]},{"inlineContent":[{"type":"image","identifier":"DDSCircleLayerExample.png"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Map data from OpenStreetMap. Style uses OpenMapTiles."}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnimatedLineExample":{"role":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnimatedLineExample","abstract":[{"type":"text","text":"Add an animated line to a map"}],"kind":"article","url":"\/documentation\/maplibre-native-for-ios\/animatedlineexample","title":"Animated Line","type":"topic"},"https://openmaptiles.org/schema/#poi":{"title":"POIs","titleInlineContent":[{"type":"text","text":"POIs"}],"url":"https:\/\/openmaptiles.org\/schema\/#poi","type":"link","identifier":"https:\/\/openmaptiles.org\/schema\/#poi"},"https://github.com/maplibre/demotiles/tree/gh-pages/tiles-omt":{"title":"tiles around Innsbruck, Austria","titleInlineContent":[{"type":"text","text":"tiles around Innsbruck, Austria"}],"url":"https:\/\/github.com\/maplibre\/demotiles\/tree\/gh-pages\/tiles-omt","type":"link","identifier":"https:\/\/github.com\/maplibre\/demotiles\/tree\/gh-pages\/tiles-omt"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/PMTiles":{"type":"topic","role":"article","abstract":[{"type":"text","text":"Working with PMTiles"}],"title":"PMTiles","url":"\/documentation\/maplibre-native-for-ios\/pmtiles","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/PMTiles"