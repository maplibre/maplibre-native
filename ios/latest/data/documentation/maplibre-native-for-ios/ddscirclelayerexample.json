yer","fragments":[{"kind":"identifier","text":"MLNLineStyleLayer"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnimatedLineExample":{"url":"\/documentation\/maplibre-native-for-ios\/animatedlineexample","title":"Animated Line","role":"article","abstract":[{"text":"Add an animated line to a map","type":"text"}],"kind":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnimatedLineExample","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/PMTiles":{"title":"PMTiles","kind":"article","type":"topic","url":"\/documentation\/maplibre-native-for-ios\/pmtiles","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/PMTiles","abstract":[{"type":"text","text":"Working with PMTiles"}],"role":"article"},"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","role":"collection","kind":"symbol"},"DDSCircleLayerExample.png":{"type":"image","identifier":"DDSCircleLayerExample.png","variants":[{"url":"\/images\/org.swift.MyProject\/DDSCircleLayerExample@2x.png","traits":["2x","light"]}],"alt":null}}}