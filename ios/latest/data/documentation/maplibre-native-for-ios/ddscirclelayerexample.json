ssion(mglJSONObject: [\"step\", [\"get\", \"rank\"], 0, \"red\", 10, \"green\", 20, \"blue\", 30, \"purple\", 40, \"yellow\"] as [Any])","        layer.circleRadius = NSExpression(forConstantValue: 3)","","        style.addLayer(layer)","    }","}"],"type":"codeListing","syntax":"swift"},{"type":"paragraph","inlineContent":[{"type":"image","identifier":"DDSCircleLayerExample.png"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Map data from OpenStreetMap. Style uses OpenMapTiles."}]}]}],"metadata":{"role":"article","modules":[{"name":"MapLibre"}],"title":"Vector Tile Sources","roleHeading":"Article"},"references":{"https://openmaptiles.org/schema/#poi":{"title":"POIs","type":"link","titleInlineContent":[{"type":"text","text":"POIs"}],"identifier":"https:\/\/openmaptiles.org\/schema\/#poi","url":"https:\/\/openmaptiles.org\/schema\/#poi"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/LineStyleLayerExample":{"type":"topic","role":"article","kind":"article","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/LineStyleLayerExample","title":"Using GeoJSON with a line style layer","url":"\/documentation\/maplibre-native-for-ios\/linestylelayerexample","abstract":[{"text":"Adding an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","type":"reference","isActive":true},{"type":"text","text":" to the map using a GeoJSON file."}]},"https://github.com/maplibre/demotiles/tree/gh-pages/tiles-omt":{"type":"link","identifier":"https:\/\/github.com\/maplibre\/demotiles\/tree\/gh-pages\/tiles-omt","title":"tiles around Innsbruck, Austria","titleInlineContent":[{"type":"text","text":"tiles around Innsbruck, Austria"}],"url":"https:\/\/github.com\/maplibre\/demotiles\/tree\/gh-pages\/tiles-omt"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/POIAlongRouteExample":{"role":"article","url":"\/documentation\/maplibre-native-for-ios\/poialongrouteexample"