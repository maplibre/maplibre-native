ion/MapLibre-Native-for-iOS/LineStyleLayerExample":{"title":"Using GeoJSON with a line style layer","type":"topic","abstract":[{"type":"text","text":"Adding an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","isActive":true},{"type":"text","text":" to the map using a GeoJSON file."}],"role":"article","url":"\/documentation\/maplibre-native-for-ios\/linestylelayerexample","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/LineStyleLayerExample","kind":"article"},"DDSCircleLayerExample.png":{"alt":null,"identifier":"DDSCircleLayerExample.png","variants":[{"url":"\/images\/org.swift.MyProject\/DDSCircleLayerExample@2x.png","traits":["2x","light"]}],"type":"image"},"https://openmaptiles.org/schema/#poi":{"type":"link","url":"https:\/\/openmaptiles.org\/schema\/#poi","titleInlineContent":[{"type":"text","text":"POIs"}],"identifier":"https:\/\/openmaptiles.org\/schema\/#poi","title":"POIs"},"doc://org.swift.MyProject/documentation/MapLibre/MLNLineStyleLayer":{"kind":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNLineStyleLayer"}],"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","isActive":true,"type":"reference"},{"type":"text","text":" is a style layer that renders one or more stroked"},{"type":"text","text":" "},{"type":"text","text":"polylines on the map."}],"title":"MLNLineStyleLayer","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","url":"\/documentation\/maplibre\/mlnlinestylelayer"},"https://maplibre.org/maplibre-style-spec/expressions/#step":{"title":"step","titleInlineContent":[{"type":"text","text":"step"}],"identifier":"https:\/\/maplibre.org\/maplibre-style-spec\/expressions\/#step","url":"https:\/\/maplibre.org\/maplibre-style-spec\/expressions\/#step","type":"link"},"doc://org.swift.MyProject/documentation/MapLib