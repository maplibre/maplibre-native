te = NSPredicate(format: \"class == %@\", \"shop\")","","        \/\/ Style the circle layer color based on the rank","        layer.circleColor = NSExpression(mglJSONObject: [\"step\", [\"get\", \"rank\"], 0, \"red\", 10, \"green\", 20, \"blue\", 30, \"purple\", 40, \"yellow\"] as [Any])","        layer.circleRadius = NSExpression(forConstantValue: 3)","","        style.addLayer(layer)","    }","}"]},{"type":"paragraph","inlineContent":[{"identifier":"DDSCircleLayerExample.png","type":"image"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Map data from OpenStreetMap. Style uses OpenMapTiles."}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnimatedLineExample":{"type":"topic","title":"Animated Line","abstract":[{"text":"Add an animated line to a map","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnimatedLineExample","kind":"article","role":"article","url":"\/documentation\/maplibre-native-for-ios\/animatedlineexample"},"doc://org.swift.MyProject/documentation/MapLibre/MLNLineStyleLayer":{"type":"topic","title":"MLNLineStyleLayer","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNLineStyleLayer","type":"reference"},{"text":" is a style layer that renders one or more stroked","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"polylines on the map."}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnlinestylelayer","fragments":[{"text":"MLNLineStyleLayer","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/PMTiles":{"abstract":[{"text":"Working with PMTiles","type":"text"}],"title":"PMTiles","type":"topic","url":"\/documentation\/maplibre-native-for-ios\/pmtiles","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Nat