te = NSPredicate(format: \"class == %@\", \"shop\")","","        \/\/ Style the circle layer color based on the rank","        layer.circleColor = NSExpression(mglJSONObject: [\"step\", [\"get\", \"rank\"], 0, \"red\", 10, \"green\", 20, \"blue\", 30, \"purple\", 40, \"yellow\"] as [Any])","        layer.circleRadius = NSExpression(forConstantValue: 3)","","        style.addLayer(layer)","    }","}"]},{"inlineContent":[{"type":"image","identifier":"DDSCircleLayerExample.png"}],"type":"paragraph"},{"inlineContent":[{"text":"Map data from OpenStreetMap. Style uses OpenMapTiles.","type":"text"}],"type":"paragraph"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/PMTiles":{"url":"\/documentation\/maplibre-native-for-ios\/pmtiles","type":"topic","role":"article","title":"PMTiles","abstract":[{"text":"Working with PMTiles","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/PMTiles","kind":"article"},"DDSCircleLayerExample.png":{"identifier":"DDSCircleLayerExample.png","type":"image","variants":[{"url":"\/images\/DDSCircleLayerExample@2x.png","traits":["2x","light"]}],"alt":null},"https://maplibre.org/maplibre-style-spec/expressions/#step":{"title":"step","titleInlineContent":[{"type":"text","text":"step"}],"url":"https:\/\/maplibre.org\/maplibre-style-spec\/expressions\/#step","type":"link","identifier":"https:\/\/maplibre.org\/maplibre-style-spec\/expressions\/#step"},"https://openmaptiles.org/schema/#poi":{"identifier":"https:\/\/openmaptiles.org\/schema\/#poi","type":"link","title":"POIs","titleInlineContent":[{"type":"text","text":"POIs"}],"url":"https:\/\/openmaptiles.org\/schema\/#poi"},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/LineStyleLayerExample":{"url":"\/documentation\/maplibre-native-for-ios\/linestylelayerexample","type":"topic","role":"article","title":"Using GeoJSON with a line style layer","abstract":[{"type":"text","text":"Adding an "},{"type":"refere